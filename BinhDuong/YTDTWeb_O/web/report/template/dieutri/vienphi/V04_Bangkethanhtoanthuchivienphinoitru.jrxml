	<queryString><![CDATA[SELECT substr(HSBA1.HSBA_SOVAOVIEN, -5) SOVAOVIEN, BN.BENHNHAN_HOTEN HOTEN, BN.BENHNHAN_TUOI TUOI,
     decode(BN.BENHNHAN_DONVITUOI , 2,'T',3, 'N','') DONVITUOI,
     GIOI.DMGT_TEN GIOITINH, DMKHOA.DMKHOA_MA KHOA, HSBA1.HSBA_NGAYGIOVAOV NGAYGIOVAOV, HSBA1.HSBA_NGAYGIORAV NGAYGIORAV, DTDMKHOIBHYT.DTDMKHOIBHYT_MA KHOIBHYT,
     (nvl(HS.HSTHTOAN_BNTRA ,0) + nvl(HS.HSTHTOAN_BHYT,0)) AS TONGCHI,    
     HS.HSTHTOAN_MIENDT AS GIAM_MIEN_PHI, HS.HSTHTOAN_THTOAN AS THTOAN, HS.HSTHTOAN_BHYT BHYT, tmpTamUng.TAMUNG_SOTIEN AS TAMUNG, 
     tmpHoanUng.HOANUNG_SOTIEN AS HOANUNG, HS.HSTHTOAN_BOVIEN AS BOVIEN, TO_DATE(HSTHTOAN_NGAYTT) NGAYTT,
     HS.HSTHTOAN_KYHIEU KYHIEU, HS.HSTHTOAN_QUYEN QUYEN, HS.HSTHTOAN_BIENLAI BIENLAI
    
FROM  HSBA HSBA1 
    LEFT JOIN HS_THTOAN HS ON HS.HSBA_SOVAOVIEN = HSBA1.HSBA_SOVAOVIEN
    LEFT JOIN HSBA_BHYT HSBABHYT ON HSBABHYT.HSBA_SOVAOVIEN = HSBA1.HSBA_SOVAOVIEN
    LEFT JOIN DM_KHOA DMKHOA ON HSBA1.HSBA_KHOARAV = DMKHOA.DMKHOA_MASO
    LEFT JOIN DT_DM_KHOI_BHYT DTDMKHOIBHYT ON HSBABHYT.HSBABHYT_KHOIBH = DTDMKHOIBHYT.DTDMKHOIBHYT_MASO
    LEFT JOIN BENH_NHAN BN ON BN.BENHNHAN_MA = HSBA1.BENHNHAN_MA
    LEFT JOIN DM_GIOI_TINH GIOI ON BN.DMGT_MASO = GIOI.DMGT_MASO
    LEFT JOIN DT_DM_NHAN_VIEN NHANVIEN ON NHANVIEN.DTDMNHANVIEN_MASO = HS.HSTHTOAN_THUNGAN
    LEFT JOIN (SELECT tu.HSBA_SOVAOVIEN AS SOVAOVIEN, SUM(TU.TAMUNG_SOTIEN) AS TAMUNG_SOTIEN
               FROM tam_ung tu LEFT JOIN hs_thtoan hstt ON tu.HSBA_SOVAOVIEN = hstt.HSBA_SOVAOVIEN
                   WHERE to_date(hstt.HSTHTOAN_NGAYTT) >= to_date($P{TuNgay}) and to_date(hstt.HSTHTOAN_NGAYTT) <= to_date($P{DenNgay})
                   GROUP BY tu.HSBA_SOVAOVIEN) tmpTamUng ON HSBA1.HSBA_SOVAOVIEN = tmpTamUng.SOVAOVIEN
    LEFT JOIN (SELECT hu.HSBA_SOVAOVIEN AS SOVAOVIEN, SUM(hu.HOANUNG_SOTIEN) AS HOANUNG_SOTIEN
               FROM hoan_ung hu 
               LEFT JOIN hs_thtoan hstt ON hstt.HSBA_SOVAOVIEN = hu.HSBA_SOVAOVIEN
                   WHERE to_date(hstt.HSTHTOAN_NGAYTT) >= to_date($P{TuNgay}) and to_date(hstt.HSTHTOAN_NGAYTT) <= to_date($P{DenNgay})
                   GROUP BY hu.HSBA_SOVAOVIEN) tmpHoanUng ON HSBA1.HSBA_SOVAOVIEN = tmpHoanUng.SOVAOVIEN
WHERE to_date(HS.HSTHTOAN_NGAYTT) >= to_date($P{TuNgay}) and to_date(HS.HSTHTOAN_NGAYTT) <= to_date($P{DenNgay})
      AND ($P{masokhoa} is null OR DMKHOA.DMKHOA_MASO = $P{masokhoa})]]></queryString>

