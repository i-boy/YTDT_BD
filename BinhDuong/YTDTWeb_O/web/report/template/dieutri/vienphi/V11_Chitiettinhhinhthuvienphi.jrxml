	<queryString><![CDATA[Select v_khoa.DMKHOA_MASO,
       v_khoa.DMKHOA_TEN,

       v_dg_cls_ngoaitru.XETNGHIEM AS NGOAITRU_XETNGHIEM,
       v_dg_cls_ngoaitru.CDHA AS NGOAITRU_CDHA,
       v_dg_cls_ngoaitru.MAU AS NGOAITRU_MAU,
       v_dg_cls_ngoaitru.TTPT AS NGOAITRU_TTPT,
       v_dg_cls_ngoaitru.KTC AS NGOAITRU_KTC,
       v_dg_cls_ngoaitru.CONGKHAM AS NGOAITRU_CONGKHAM,
       v_dg_cls_ngoaitru.GIUONG AS NGOAITRU_GIUONG,
       v_dg_cls_ngoaitru.VANCHUYEN AS NGOAITRU_VANCHUYEN,

       v_bntra_cls_ngoaitru.SUM_GIABNTRA AS NGOAITRU_CLS_BNTRA,

       v_dg_tpk.THUOC AS NGOAITRU_THUOC,
       v_dg_tpk.VTYT AS NGOAITRU_VTYT,

       v_thuoc_bntra_ngoaitru.THUOC_BNTRA AS NGOAITRU_THUOC_BNTRA,

       v_dg_cls_noitru.XETNGHIEM AS NOITRU_XETNGHIEM,
       v_dg_cls_noitru.CDHA AS NOITRU_CDHA,
       v_dg_cls_noitru.MAU AS NOITRU_MAU,
       v_dg_cls_noitru.TTPT AS NOITRU_TTPT,
       v_dg_cls_noitru.KTC AS NOITRU_KTC,
       v_dg_cls_noitru.CONGKHAM AS NOITRU_CONGKHAM,
       v_dg_cls_noitru.GIUONG AS NOITRU_GIUONG,
       v_dg_cls_noitru.VANCHUYEN AS NOITRU_VANCHUYEN,

       v_bntra_cls_noitru.SUM_GIABNTRA AS NOITRU_CLS_BNTRA,

       v_dg_tnt.THUOC AS NOITRU_THUOC,
       v_dg_tnt.VTYT AS NOITRU_VTYT,

       v_thuoc_bntra_noitru.THUOC_BNTRA AS NOITRU_THUOC_BNTRA

FROM
(Select dmkhoa.DMKHOA_MASO,
       dmkhoa.DMKHOA_TEN
FROM dm_khoa dmkhoa
WHERE dmkhoa.DMKHOA_MASO IN
(
SELECT
       dmkhoa.DMKHOA_MASO
FROM tham_kham tk
     LEFT JOIN dt_dm_ban_kham dmbk ON tk.THAMKHAM_BANKHAM = dmbk.DTDMBANKHAM_MASO
     LEFT JOIN dm_khoa dmkhoa ON dmbk.DMKHOA_MASO = dmkhoa.DMKHOA_MASO
WHERE tk.TIEPDON_MA IN(
        SELECT hsttk.TIEPDON_MA
        FROM hs_thtoank hsttk
        WHERE hsttk.HSTHTOANK_NGAYGIOTT Is Not Null
              AND TO_DATE(hsttk.HSTHTOANK_NGAYGIOTT) >= TO_DATE($P{TuNgay})
              AND TO_DATE(hsttk.HSTHTOANK_NGAYGIOTT) <= TO_DATE($P{DenNgay})
				)
) OR dmkhoa.DMKHOA_MASO IN
(SELECT hsbakhoa.KHOA_MA
FROM hsba_khoa hsbakhoa
WHERE hsbakhoa.HSBA_SOVAOVIEN IN
(
 SELECT hstt.HSBA_SOVAOVIEN
FROM hs_thtoan hstt
WHERE hstt.HSTHTOAN_NGAYTT Is Not Null
      AND TO_DATE(hstt.HSTHTOAN_NGAYTT) >= TO_DATE($P{TuNgay})
      AND TO_DATE(hstt.HSTHTOAN_NGAYTT) <= TO_DATE($P{DenNgay})
      AND (hstt.HSTHTOAN_BOVIEN Is Null OR hstt.HSTHTOAN_BOVIEN = 0))
)) v_khoa
LEFT JOIN
-- View SUM don gia cls theo loai va khoa (BN ngoai tru);
(Select SUM_DONGIA_CLS_NGOAITRU.DMKHOA_MASO,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NGOAITRU.CLSKHAM_LOAI LIKE 'XN') THEN SUM_DONGIA_CLS_NGOAITRU.SUM_DONGIA ELSE 0 END) AS XETNGHIEM,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NGOAITRU.CLSKHAM_LOAI LIKE 'CD') THEN SUM_DONGIA_CLS_NGOAITRU.SUM_DONGIA ELSE 0 END) AS CDHA,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NGOAITRU.CLSKHAM_LOAI LIKE 'MA') THEN SUM_DONGIA_CLS_NGOAITRU.SUM_DONGIA ELSE 0 END) AS MAU,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NGOAITRU.CLSKHAM_LOAI LIKE 'TT' OR SUM_DONGIA_CLS_NGOAITRU.CLSKHAM_LOAI LIKE 'PT') THEN SUM_DONGIA_CLS_NGOAITRU.SUM_DONGIA ELSE 0 END) AS TTPT,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NGOAITRU.CLSKHAM_LOAI LIKE 'KT') THEN SUM_DONGIA_CLS_NGOAITRU.SUM_DONGIA ELSE  0 END) AS KTC,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NGOAITRU.CLSKHAM_LOAI LIKE 'KH') THEN SUM_DONGIA_CLS_NGOAITRU.SUM_DONGIA ELSE  0 END) AS CONGKHAM,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NGOAITRU.CLSKHAM_LOAI LIKE 'GI' ) THEN SUM_DONGIA_CLS_NGOAITRU.SUM_DONGIA ELSE 0 END) AS GIUONG,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NGOAITRU.CLSKHAM_LOAI LIKE 'VC') THEN SUM_DONGIA_CLS_NGOAITRU.SUM_DONGIA ELSE 0 END) AS VANCHUYEN
FROM
(
SELECT SUM(clskham.CLSKHAM_DONGIA * (clskham.CLSKHAM_LAN - NVL(clskham.CLSKHAM_TRA,0))) AS SUM_DONGIA,
       clskham.CLSKHAM_LOAI,
       dmkhoa.DMKHOA_MASO
FROM cls_kham clskham
     LEFT JOIN tham_kham tk ON clskham.CLSKHAM_THAMKHAM = tk.THAMKHAM_MA
     LEFT JOIN dt_dm_ban_kham dmbk ON tk.THAMKHAM_BANKHAM = dmbk.DTDMBANKHAM_MASO
     LEFT JOIN dm_khoa dmkhoa ON dmbk.DMKHOA_MASO = dmkhoa.DMKHOA_MASO
WHERE tk.TIEPDON_MA IN(
        SELECT hsttk.TIEPDON_MA
        FROM hs_thtoank hsttk
        WHERE hsttk.HSTHTOANK_NGAYGIOTT Is Not Null
              AND TO_DATE(hsttk.HSTHTOANK_NGAYGIOTT) >= TO_DATE($P{TuNgay})
              AND TO_DATE(hsttk.HSTHTOANK_NGAYGIOTT) <= TO_DATE($P{DenNgay})
				)
GROUP BY dmkhoa.DMKHOA_MASO, clskham.CLSKHAM_LOAI
)  SUM_DONGIA_CLS_NGOAITRU
GROUP BY SUM_DONGIA_CLS_NGOAITRU.DMKHOA_MASO

)v_dg_cls_ngoaitru ON v_khoa.DMKHOA_MASO = v_dg_cls_ngoaitru.DMKHOA_MASO
LEFT JOIN
(
-- View SUM don gia benh nhan tra theo khoa (tat ca cac cls ngoai tru)
SELECT
       SUM(clskham.CLSKHAM_DONGIABNTRA) AS SUM_GIABNTRA,
       dmkhoa.DMKHOA_MASO
FROM cls_kham clskham
     LEFT JOIN tham_kham tk ON clskham.CLSKHAM_THAMKHAM = tk.THAMKHAM_MA
     LEFT JOIN dt_dm_ban_kham dmbk ON tk.THAMKHAM_BANKHAM = dmbk.DTDMBANKHAM_MASO
     LEFT JOIN dm_khoa dmkhoa ON dmbk.DMKHOA_MASO = dmkhoa.DMKHOA_MASO
WHERE tk.TIEPDON_MA IN(
        SELECT hsttk.TIEPDON_MA
        FROM hs_thtoank hsttk
        WHERE hsttk.HSTHTOANK_NGAYGIOTT Is Not Null
              AND TO_DATE(hsttk.HSTHTOANK_NGAYGIOTT) >= TO_DATE($P{TuNgay})
              AND TO_DATE(hsttk.HSTHTOANK_NGAYGIOTT) <= TO_DATE($P{DenNgay})
				)
 GROUP BY dmkhoa.DMKHOA_MASO

)v_bntra_cls_ngoaitru ON v_khoa.DMKHOA_MASO = v_bntra_cls_ngoaitru.DMKHOA_MASO
LEFT JOIN
(-- View SUM gia thuoc phong kham theo khoa
SELECT SUM(CASE WHEN (SUM_DONGIA_THUOC_PK.DMLOAITHUOC_MA NOT LIKE 'YC') THEN SUM_DONGIA_THUOC_PK.SUM_DONGIA_THUOC ELSE 0 END) AS THUOC,
       SUM(CASE WHEN(SUM_DONGIA_THUOC_PK.DMLOAITHUOC_MA LIKE 'YC') THEN SUM_DONGIA_THUOC_PK.SUM_DONGIA_THUOC ELSE 0 END) AS VTYT,
       SUM_DONGIA_THUOC_PK.DMKHOA_MASO
FROM
(
SELECT -- SUM(tpk.THUOCPHONGKHAM_DONGIA * (tpk.THUOCPHONGKHAM_SOLUONG - NVL(tpk.THUOCPHONGKHAM_TRA,0))) AS SUM_DONGIA_THUOC,
       SUM(tpk.THUOCPHONGKHAM_THANHTIEN) AS SUM_DONGIA_THUOC,
       lt.DMLOAITHUOC_MA,
       dmkhoa.DMKHOA_MASO
FROM thuoc_phong_kham tpk
     LEFT JOIN dm_thuoc dmthuoc ON tpk.THUOCPHONGKHAM_MATHUOC = dmthuoc.DMTHUOC_MASO
     LEFT JOIN dm_phan_loai_thuoc plt ON dmthuoc.DMPHANLOAITHUOC_MASO = plt.DMPHANLOAITHUOC_MASO
     LEFT JOIN dm_loai_thuoc lt ON plt.DMLOAITHUOC_MASO = lt.DMLOAITHUOC_MASO
     LEFT JOIN tham_kham tk ON tpk.THUOCPHONGKHAM_THAMKHAM = tk.THAMKHAM_MA
     LEFT JOIN dt_dm_ban_kham dmbk ON tk.THAMKHAM_BANKHAM = dmbk.DTDMBANKHAM_MASO
     LEFT JOIN dm_khoa dmkhoa ON dmbk.DMKHOA_MASO = dmkhoa.DMKHOA_MASO
WHERE tk.TIEPDON_MA IN(
        SELECT hsttk.TIEPDON_MA
        FROM hs_thtoank hsttk
        WHERE hsttk.HSTHTOANK_NGAYGIOTT Is Not Null
             AND TO_DATE(hsttk.HSTHTOANK_NGAYGIOTT) >= TO_DATE($P{TuNgay})
             AND TO_DATE(hsttk.HSTHTOANK_NGAYGIOTT) <= TO_DATE($P{DenNgay})
)
GROUP BY dmkhoa.DMKHOA_MASO, lt.DMLOAITHUOC_MA
)  SUM_DONGIA_THUOC_PK
GROUP BY SUM_DONGIA_THUOC_PK.DMKHOA_MASO
)v_dg_tpk ON v_khoa.DMKHOA_MASO = v_dg_tpk.DMKHOA_MASO
LEFT JOIN
( -- View SUM thuoc phong kham benh nhan tra
SELECT SUM(tpk.THUOCPHONGKHAM_TIENBNTRA) AS THUOC_BNTRA,
       dmkhoa.DMKHOA_MASO
FROM thuoc_phong_kham tpk
     LEFT JOIN dm_thuoc dmthuoc ON tpk.THUOCPHONGKHAM_MATHUOC = dmthuoc.DMTHUOC_MASO
     LEFT JOIN tham_kham tk ON tpk.THUOCPHONGKHAM_THAMKHAM = tk.THAMKHAM_MA
     LEFT JOIN dt_dm_ban_kham dmbk ON tk.THAMKHAM_BANKHAM = dmbk.DTDMBANKHAM_MASO
     LEFT JOIN dm_khoa dmkhoa ON dmbk.DMKHOA_MASO = dmkhoa.DMKHOA_MASO
WHERE tk.TIEPDON_MA IN(
        SELECT hsttk.TIEPDON_MA
        FROM hs_thtoank hsttk
        WHERE hsttk.HSTHTOANK_NGAYGIOTT Is Not Null
              AND TO_DATE(hsttk.HSTHTOANK_NGAYGIOTT) >= TO_DATE($P{TuNgay})
              AND TO_DATE(hsttk.HSTHTOANK_NGAYGIOTT) <= TO_DATE($P{DenNgay})
)
GROUP BY dmkhoa.DMKHOA_MASO

)v_thuoc_bntra_ngoaitru ON v_khoa.DMKHOA_MASO = v_thuoc_bntra_ngoaitru.DMKHOA_MASO
LEFT JOIN
( -- View SUM don gia cls mo (noi tru) theo khoa
Select SUM_DONGIA_CLS_NOITRU.DMKHOA_MASO,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NOITRU.CLSMO_LOAI LIKE 'XN') THEN SUM_DONGIA_CLS_NOITRU.SUM_DONGIA ELSE 0 END) AS XETNGHIEM,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NOITRU.CLSMO_LOAI LIKE 'CD') THEN SUM_DONGIA_CLS_NOITRU.SUM_DONGIA ELSE 0 END) AS CDHA,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NOITRU.CLSMO_LOAI LIKE 'MA') THEN SUM_DONGIA_CLS_NOITRU.SUM_DONGIA ELSE 0 END) AS MAU,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NOITRU.CLSMO_LOAI LIKE 'TT' OR SUM_DONGIA_CLS_NOITRU.CLSMO_LOAI LIKE 'PT') THEN SUM_DONGIA_CLS_NOITRU.SUM_DONGIA ELSE 0 END) AS TTPT,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NOITRU.CLSMO_LOAI LIKE 'KT') THEN SUM_DONGIA_CLS_NOITRU.SUM_DONGIA ELSE 0 END) AS KTC,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NOITRU.CLSMO_LOAI LIKE 'KH') THEN SUM_DONGIA_CLS_NOITRU.SUM_DONGIA ELSE 0 END) AS CONGKHAM,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NOITRU.CLSMO_LOAI LIKE 'GI') THEN SUM_DONGIA_CLS_NOITRU.SUM_DONGIA ELSE 0 END) AS GIUONG,
       SUM(CASE WHEN(SUM_DONGIA_CLS_NOITRU.CLSMO_LOAI LIKE 'VC') THEN SUM_DONGIA_CLS_NOITRU.SUM_DONGIA ELSE 0 END) AS VANCHUYEN
FROM
(
SELECT SUM(clsmo.CLSMO_DONGIA * (clsmo.CLSMO_LAN - NVL(clsmo.CLSMO_TRA,0))) AS SUM_DONGIA,
       clsmo.CLSMO_LOAI,
       dmkhoa.DMKHOA_MASO
FROM cls_mo clsmo
     LEFT JOIN hsba_khoa hsbakhoa ON clsmo.HSBAKHOA_MASO = hsbakhoa.HSBAKHOA_MASO
     LEFT JOIN dm_khoa dmkhoa ON hsbakhoa.KHOA_MA = dmkhoa.DMKHOA_MASO
WHERE hsbakhoa.HSBA_SOVAOVIEN IN (
        SELECT hstt.HSBA_SOVAOVIEN
        FROM hs_thtoan hstt
        WHERE hstt.HSTHTOAN_NGAYTT Is Not Null
              AND TO_DATE(hstt.HSTHTOAN_NGAYTT) >= TO_DATE($P{TuNgay})
              AND TO_DATE(hstt.HSTHTOAN_NGAYTT) <= TO_DATE($P{DenNgay})
              AND (hstt.HSTHTOAN_BOVIEN Is Null OR hstt.HSTHTOAN_BOVIEN = 0)
        )
GROUP BY dmkhoa.DMKHOA_MASO, clsmo.CLSMO_LOAI
)  SUM_DONGIA_CLS_NOITRU
GROUP BY SUM_DONGIA_CLS_NOITRU.DMKHOA_MASO
)v_dg_cls_noitru ON v_khoa.DMKHOA_MASO = v_dg_cls_noitru.DMKHOA_MASO
LEFT JOIN
(-- View SUM don gia benh nhan tra theo khoa (tat ca cac cls noi tru)
SELECT SUM(clsmo.CLSMO_DONGIABNTRA) AS SUM_GIABNTRA,
       dmkhoa.DMKHOA_MASO
FROM cls_mo clsmo
     LEFT JOIN hsba_khoa hsbakhoa ON clsmo.HSBAKHOA_MASO = hsbakhoa.HSBAKHOA_MASO
     LEFT JOIN dm_khoa dmkhoa ON hsbakhoa.KHOA_MA = dmkhoa.DMKHOA_MASO
WHERE hsbakhoa.HSBA_SOVAOVIEN IN (
        SELECT hstt.HSBA_SOVAOVIEN
        FROM hs_thtoan hstt
        WHERE hstt.HSTHTOAN_NGAYTT Is Not Null
              AND TO_DATE(hstt.HSTHTOAN_NGAYTT) >= TO_DATE($P{TuNgay})
              AND TO_DATE(hstt.HSTHTOAN_NGAYTT) <= TO_DATE($P{DenNgay})
              AND (hstt.HSTHTOAN_BOVIEN Is Null OR hstt.HSTHTOAN_BOVIEN = 0)
        )
GROUP BY dmkhoa.DMKHOA_MASO
) v_bntra_cls_noitru ON v_khoa.DMKHOA_MASO = v_bntra_cls_noitru.DMKHOA_MASO
LEFT JOIN
( -- View SUM gia thuoc noi thu theo khoa
SELECT SUM(CASE WHEN(SUM_DONGIA_THUOC_NT.DMLOAITHUOC_MA NOT LIKE 'YC') THEN SUM_DONGIA_THUOC_NT.SUM_DONGIA_THUOC ELSE 0 END) AS THUOC,
       SUM(CASE WHEN(SUM_DONGIA_THUOC_NT.DMLOAITHUOC_MA LIKE 'YC') THEN SUM_DONGIA_THUOC_NT.SUM_DONGIA_THUOC ELSE 0 END) AS VTYT,
       SUM_DONGIA_THUOC_NT.DMKHOA_MASO
FROM
(SELECT -- SUM(tnt.THUOCNOITRU_DONGIA * (tnt.THUOCNOITRU_SOLUONG - NVL(tnt.THUOCNOITRU_TRA,0))) AS SUM_DONGIA_THUOC,
       SUM(tnt.THUOCNOITRU_THANHTIEN) AS SUM_DONGIA_THUOC,
       lt.DMLOAITHUOC_MA,
       dmkhoa.DMKHOA_MASO
FROM thuoc_noi_tru tnt
     LEFT JOIN dm_thuoc dmthuoc ON tnt.THUOCNOITRU_MATHUOC = dmthuoc.DMTHUOC_MASO
     LEFT JOIN dm_phan_loai_thuoc plt ON dmthuoc.DMPHANLOAITHUOC_MASO = plt.DMPHANLOAITHUOC_MASO
     LEFT JOIN dm_loai_thuoc lt ON plt.DMLOAITHUOC_MASO = lt.DMLOAITHUOC_MASO
     LEFT JOIN hsba_khoa hsbakhoa ON tnt.HSBA_KHOA = hsbakhoa.HSBAKHOA_MASO
     LEFT JOIN dm_khoa dmkhoa ON hsbakhoa.KHOA_MA = dmkhoa.DMKHOA_MASO
WHERE hsbakhoa.HSBA_SOVAOVIEN IN (
        SELECT hstt.HSBA_SOVAOVIEN
        FROM hs_thtoan hstt
        WHERE hstt.HSTHTOAN_NGAYTT Is Not Null
			  AND TO_DATE(hstt.HSTHTOAN_NGAYTT) >= TO_DATE($P{TuNgay})
              AND TO_DATE(hstt.HSTHTOAN_NGAYTT) <= TO_DATE($P{DenNgay})
              AND (hstt.HSTHTOAN_BOVIEN Is Null OR hstt.HSTHTOAN_BOVIEN = 0))
GROUP BY dmkhoa.DMKHOA_MASO, lt.DMLOAITHUOC_MA
)  SUM_DONGIA_THUOC_NT
GROUP BY SUM_DONGIA_THUOC_NT.DMKHOA_MASO
)v_dg_tnt ON v_khoa.DMKHOA_MASO = v_dg_tnt.DMKHOA_MASO
LEFT JOIN
( -- View SUM thuoc noi tru benh nhan tra
SELECT SUM(tnt.THUOCNOITRU_TIENBNTRA) AS THUOC_BNTRA,
       dmkhoa.DMKHOA_MASO
FROM thuoc_noi_tru tnt
     LEFT JOIN hsba_khoa hsbakhoa ON tnt.HSBA_KHOA = hsbakhoa.HSBAKHOA_MASO
     LEFT JOIN dm_khoa dmkhoa ON hsbakhoa.KHOA_MA = dmkhoa.DMKHOA_MASO
WHERE hsbakhoa.HSBA_SOVAOVIEN IN (
        SELECT hstt.HSBA_SOVAOVIEN
        FROM hs_thtoan hstt
        WHERE hstt.HSTHTOAN_NGAYTT Is Not Null
              AND TO_DATE(hstt.HSTHTOAN_NGAYTT) >= TO_DATE($P{TuNgay})
              AND TO_DATE(hstt.HSTHTOAN_NGAYTT) <= TO_DATE($P{DenNgay})
              AND (hstt.HSTHTOAN_BOVIEN Is Null OR hstt.HSTHTOAN_BOVIEN = 0))
GROUP BY dmkhoa.DMKHOA_MASO
)v_thuoc_bntra_noitru ON v_khoa.DMKHOA_MASO = v_thuoc_bntra_noitru.DMKHOA_MASO]]></queryString>

	<field name="DMKHOA_MASO" class="java.lang.Long"/>
	<field name="DMKHOA_TEN" class="java.lang.String"/>
	<field name="NGOAITRU_XETNGHIEM" class="java.lang.Double"/>
	<field name="NGOAITRU_CDHA" class="java.lang.Double"/>
	<field name="NGOAITRU_MAU" class="java.lang.Double"/>
	<field name="NGOAITRU_TTPT" class="java.lang.Double"/>
	<field name="NGOAITRU_KTC" class="java.lang.Double"/>
	<field name="NGOAITRU_CONGKHAM" class="java.lang.Double"/>
	<field name="NGOAITRU_GIUONG" class="java.lang.Double"/>
	<field name="NGOAITRU_VANCHUYEN" class="java.lang.Double"/>
	<field name="NGOAITRU_CLS_BNTRA" class="java.lang.Double"/>
	<field name="NGOAITRU_THUOC" class="java.lang.Double"/>
	<field name="NGOAITRU_VTYT" class="java.lang.Double"/>
	<field name="NGOAITRU_THUOC_BNTRA" class="java.lang.Double"/>
	<field name="NOITRU_XETNGHIEM" class="java.lang.Double"/>
	<field name="NOITRU_CDHA" class="java.lang.Double"/>
	<field name="NOITRU_MAU" class="java.lang.Double"/>
	<field name="NOITRU_TTPT" class="java.lang.Double"/>
	<field name="NOITRU_KTC" class="java.lang.Double"/>
	<field name="NOITRU_CONGKHAM" class="java.lang.Double"/>
	<field name="NOITRU_GIUONG" class="java.lang.Double"/>
	<field name="NOITRU_VANCHUYEN" class="java.lang.Double"/>
	<field name="NOITRU_CLS_BNTRA" class="java.lang.Double"/>
	<field name="NOITRU_THUOC" class="java.lang.Double"/>
	<field name="NOITRU_VTYT" class="java.lang.Double"/>
	<field name="NOITRU_THUOC_BNTRA" class="java.lang.Double"/>

	<variable name="SUM_TIENGIUONG" class="java.lang.Double" resetType="Report" calculation="Sum">
		<variableExpression><![CDATA[new Double(   
   ( $F{NOITRU_GIUONG} == null ? 0 : $F{NOITRU_GIUONG}.doubleValue() )
 )]]></variableExpression>
	</variable>
