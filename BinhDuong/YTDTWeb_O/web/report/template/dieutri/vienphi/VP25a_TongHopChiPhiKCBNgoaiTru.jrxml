 AND ( ( ( $P{makhoa} is not null) AND  (KHOA.DMKHOA_MA LIKE $P{makhoa}) )
	OR ( ( $P{makhoa} is null) AND (HS.HSTHTOANK_NGAYGIOTT is not null) )
)
