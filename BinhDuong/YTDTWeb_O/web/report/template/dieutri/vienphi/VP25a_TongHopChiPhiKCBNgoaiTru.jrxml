        COUNT(HS.HSTHTOANK_MA) AS LUOTNGUOI,
        SUM(HS.HSTHTOANK_CONGKHAM) AS TIENKHAM,
        (SUM(HS.HSTHTOANK_THUOC) + SUM(HS.HSTHTOANK_THUOCNDM)) AS THUOC,
        SUM(HS.HSTHTOANK_MAU) AS MAU,
        SUM(HSTHTOANK_XNTDCN) AS XETNGHIEM,
        SUM(HS.HSTHTOANK_CDHA) AS CDHA,
        SUM(HS.HSTHTOANK_DVKT_TT + HS.HSTHTOANK_PHAUTHUAT) AS PTTT,
        SUM(HS.HSTHTOANK_DV_KTC ) AS KTC,
        (SUM(HS.HSTHTOANK_VTTH) + SUM(HSTHTOANK_VTTHNDM)) AS VTTH,
        SUM(HS.HSTHTOANK_CP_VC) AS VANCHUYEN,
        SUM(HS.HSTHTOANK_BNTRA) BNTRA,
        SUM(HS.HSTHTOANK_BHYT) BHYTCHI
  FROM HS_THTOANK HS LEFT JOIN TIEP_DON TD ON HS.TIEPDON_MA = TD.TIEPDON_MA

     LEFT JOIN DT_DM_KHOI_BHYT KHOIBHYT ON TD.KHOIBHYT_MA = KHOIBHYT.DTDMKHOIBHYT_MA
     LEFT JOIN DT_DM_NHOM_BHYT NHOMBHYT ON KHOIBHYT.DTDMNHOMBHYT_MASO = NHOMBHYT.DTDMNHOMBHYT_MASO
     LEFT JOIN DM_DOI_TUONG DOITUONG ON TD.DOITUONG_MA = DOITUONG.DMDOITUONG_MASO
     LEFT JOIN DM_BENH_VIEN BENHVIEN  ON TD.KCBBHYT_MA = BENHVIEN.DMBENHVIEN_MASO
	 LEFT JOIN DT_DM_BAN_KHAM BANKHAM ON TD.TIEPDON_BANKHAM = BANKHAM.DTDMBANKHAM_MASO
     LEFT JOIN DM_KHOA KHOA ON BANKHAM.DMKHOA_MASO = KHOA.DMKHOA_MASO
WHERE HS.HSTHTOANK_NGAYGIOTT is not null
  AND TO_DATE(HS.HSTHTOANK_NGAYGIOTT) >= TO_DATE($P{TuNgay})
  AND TO_DATE(HS.HSTHTOANK_NGAYGIOTT) <= TO_DATE($P{DenNgay})
  AND DOITUONG.DMDOITUONG_MA LIKE 'BH'
  AND SUBSTR(TD.TIEPDON_SOTHEBH,4,2) LIKE $P{ma_noi_tinh}
  AND BENHVIEN.DMBENHVIEN_MA LIKE $P{maso_cskcb}
  AND TD.TIEPDON_TUYEN = 1
  
  AND TD.TIEPDON_BANKHAM is not null
  AND NHOMBHYT.DTDMPHLOAIBHYT_MASO = 6
  AND HS.HSTHTOANK_BHYT Is Not Null AND HS.HSTHTOANK_BHYT > 0
AND ( ( (NOT ( $P{chon} = 'IN')) AND  (KHOIBHYT.DTDMKHOIBHYT_MASO NOT IN ($P!{khoi})) )
	OR ( ( $P{chon} = 'IN') AND (KHOIBHYT.DTDMKHOIBHYT_MASO IN ($P!{khoi})) )
