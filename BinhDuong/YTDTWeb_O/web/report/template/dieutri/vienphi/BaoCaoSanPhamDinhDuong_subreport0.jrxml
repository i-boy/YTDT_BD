	<queryString><![CDATA[Select
       unionNhaSX.`DTDMNSX_TEN` AS NHASX_TEN,
       tonnhap.`Title` AS Title,
       tonnhap.`SOLUONG` AS SOLUONG
FROM
(Select dmnsx.`DTDMNSX_MASO` AS DTDMNSX_MASO,
       dmnsx.`DTDMNSX_TEN` AS DTDMNSX_TEN
FROM nhap_kho_dinh_duong nk LEFT JOIN dt_dm_nha_sx_spdd dmnsx ON nk.`DTDMNSX_MASO` = dmnsx.`DTDMNSX_MASO`
WHERE nk.`DTDMLA2_MASO` = 5 AND nk.`NKDD_NGAYNHAP` BETWEEN $P{tungay} AND $P{denngay}
GROUP BY nk.`DTDMNSX_MASO`
Union
Select dmnsx.`DTDMNSX_MASO` AS DTDMNSX_MASO,
       dmnsx.`DTDMNSX_TEN` AS DTDMNSX_TEN
FROM xuat_kho_dinh_duong xk LEFT JOIN dt_dm_nha_sx_spdd dmnsx ON xk.`DTDMNSX_MASO` = dmnsx.`DTDMNSX_MASO`
WHERE xk.`DTDMLA2_MASO` = 5 AND xk.`XKDD_NGAYXUAT` BETWEEN $P{tungay} AND $P{denngay}
GROUP BY xk.`DTDMNSX_MASO`
) unionNhaSX LEFT JOIN

(Select ' TỒN CŨ' as Title,
       dmnsx.`DTDMNSX_MASO` AS NHASX_MASO,
       tk.`TKDD_SOLUONG` AS SOLUONG
FROM ton_kho_dinh_duong tk LEFT JOIN dt_dm_nha_sx_spdd dmnsx ON tk.`DTDMNSX_MASO` = dmnsx.`DTDMNSX_MASO`
WHERE tk.`DTDMLA_MASO` = 2 AND tk.`DTDMLA2_MASO` = 5 AND tk.`TKDD_MASO` IN (Select MAX(tk1.`TKDD_MASO`) From
    ton_kho_dinh_duong tk1 where tk1.`TKDD_NGAYTON` < $P{tungay} AND tk1.`DTDMLA2_MASO` = 5 GROUP BY tk1.`DTDMNSX_MASO`)
UNION ALL
Select 'NHẬP' as Title,
        dmnsx.`DTDMNSX_MASO` AS NHASX_MASO,
       SUM(nk.`NKDD_SOLUONG`) AS SOLUONG
FROM nhap_kho_dinh_duong nk LEFT JOIN dt_dm_nha_sx_spdd dmnsx ON nk.`DTDMNSX_MASO` = dmnsx.`DTDMNSX_MASO`
WHERE nk.`DTDMLA_MASO` = 2 AND nk.`DTDMLA2_MASO` = 5 AND nk.`NKDD_NGAYNHAP` BETWEEN $P{tungay} AND $P{denngay}
GROUP BY nk.`DTDMNSX_MASO`) tonnhap ON unionNhaSX.`DTDMNSX_MASO` = tonnhap.`NHASX_MASO`
ORDER BY unionNhaSX.`DTDMNSX_TEN`]]></queryString>

