GROUP BY pba.KHOA_MASO) hasSondeCm ON pba.KHOA_MASO=hasSondeCm.KHOA_MASO
LEFT JOIN 
-- Count so benh nhan an qua sonde(id=1) va co che do an la Lat(Id=1) 
(Select pba.KHOA_MASO AS KHOA_MASO, Count(bncda.DTDMCDA_MASO) AS COUNT_SONDE_LAT
FROM benh_nhan_che_do_an bncda
