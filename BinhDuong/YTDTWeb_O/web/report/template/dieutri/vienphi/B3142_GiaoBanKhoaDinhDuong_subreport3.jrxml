	<queryString><![CDATA[Select
	pba.KHOA_MASO AS KHOA_MASO,
	pba.KHOA_TEN AS KHOA_TEN,
	countSondeDd.COUNT_SONDE_DD AS COUNT_SONDE_DD,
	hasSondeDd.HAS_SONDE_DD AS HAS_SONDE_DD,
	countSondeStm.COUNT_SONDE_STM AS COUNT_SONDE_STM,
	hasSondeStm.HAS_SONDE_STM AS HAS_SONDE_STM,
	countSondeCm.COUNT_SONDE_CM AS COUNT_SONDE_CM,
	hasSondeCm.HAS_SONDE_CM AS HAS_SONDE_CM,
	countSondeLat.COUNT_SONDE_LAT AS COUNT_SONDE_LAT,
	hasSondeLat.HAS_SONDE_LAT AS HAS_SONDE_LAT,
	countSondeThg.COUNT_SONDE_THG AS COUNT_SONDE_THG,	

	countChaoDd.COUNT_CHAO_DD AS COUNT_CHAO_DD,
	hasChaoDd.HAS_CHAO_DD AS HAS_CHAO_DD,
	countChaoStm.COUNT_CHAO_STM AS COUNT_CHAO_STM,
	hasChaoStm.HAS_CHAO_STM AS HAS_CHAO_STM,
	countChaoCm.COUNT_CHAO_CM AS COUNT_CHAO_CM,
	hasChaoCm.HAS_CHAO_CM AS HAS_CHAO_CM,
	countChaoLat.COUNT_CHAO_LAT AS COUNT_CHAO_LAT,
	hasChaoLat.HAS_CHAO_LAT AS HAS_CHAO_LAT,
	countChaoThg.COUNT_CHAO_THG AS COUNT_CHAO_THG,
	
	countComDd.COUNT_COM_DD AS COUNT_COM_DD,
	hasComDd.HAS_COM_DD AS HAS_COM_DD,
	countComStm.COUNT_COM_STM AS COUNT_COM_STM,
	hasComStm.HAS_COM_STM AS HAS_COM_STM,
	countComCm.COUNT_COM_CM AS COUNT_COM_CM,
	hasComCm.HAS_COM_CM AS HAS_COM_CM,
	countComLat.COUNT_COM_LAT AS COUNT_COM_LAT,
	hasComLat.HAS_COM_LAT AS HAS_COM_LAT,
	countComThg.COUNT_COM_THG AS COUNT_COM_THG,
	hasComThg.HAS_COM_THG AS HAS_COM_THG,
	countComBduong.COUNT_COM_BDUONG AS COUNT_COM_BDUONG
	
	
FROM
(Select pba1.KHOA_MASO AS KHOA_MASO,
	 dmkhoa.DMKHOA_TEN AS KHOA_TEN
FROM phieu_bao_an pba1 
	LEFT JOIN dm_khoa dmkhoa ON pba1.KHOA_MASO=dmkhoa.DMKHOA_MASO
	LEFT JOIN benh_nhan_phieu_bao_an bnpba1 ON pba1.PHIEUBAOAN_MASO=bnpba1.PHIEUBAOAN_MASO 
WHERE pba1.PHIEUBAOAN_NGAYAN=$P{ngayan} AND bnpba1.DTDMLA_MASO=$P{loaian_kp_maso} -- Khau phan
GROUP BY pba1.KHOA_MASO,dmkhoa.DMKHOA_TEN) pba
LEFT JOIN 
-- Count so benh nhan an qua sonde(id=1) va co che do an la DD(Id=2) 
(Select pba.KHOA_MASO AS KHOA_MASO, Count(bncda.DTDMCDA_MASO) AS COUNT_SONDE_DD
FROM benh_nhan_che_do_an bncda
	LEFT JOIN benh_nhan_phieu_bao_an bnpba ON bnpba.BNPBA_MASO=bncda.BNPBA_MASO
	LEFT JOIN phieu_bao_an pba ON bnpba.PHIEUBAOAN_MASO=pba.PHIEUBAOAN_MASO
WHERE pba.PHIEUBAOAN_NGAYAN=$P{ngayan}
 AND bncda.DTDMCDA_MASO=$P{cdoan_dd_maso} -- Che do an DD
 AND bncda.BNPBA_MASO IN(
 Select vBnSonde.BNPBA_MASO
 FROM v_benh_nhan_sonde vBnSonde
 WHERE vBnSonde.BNPBA_PHUTROI=1 AND vBnSonde.NGAYAN=$P{ngayan})
 GROUP BY pba.KHOA_MASO) countSondeDd ON pba.KHOA_MASO=countSondeDd.KHOA_MASO
LEFT JOIN
-- Count so benh nhan an qua sonde(id=1) va co che do an la DD(Id=2) va co them 1 trong cac che do an khac
-- Neu so luong nay lon hon so luong count o cau query tren thi hien thi dau * tren report 
(Select pba.KHOA_MASO AS KHOA_MASO, Count(bncda.BNPBA_MASO) AS HAS_SONDE_DD
FROM benh_nhan_che_do_an bncda
	 LEFT JOIN benh_nhan_phieu_bao_an bnpba ON bnpba.BNPBA_MASO=bncda.BNPBA_MASO
	 LEFT JOIN phieu_bao_an pba ON bnpba.PHIEUBAOAN_MASO=pba.PHIEUBAOAN_MASO
 WHERE pba.PHIEUBAOAN_NGAYAN=$P{ngayan}
 AND bncda.BNPBA_MASO IN(Select bncda1.BNPBA_MASO
 FROM benh_nhan_che_do_an bncda1
 WHERE bncda1.DTDMCDA_MASO=$P{cdoan_dd_maso})
 AND bncda.BNPBA_MASO IN(
	 Select vBnSonde.BNPBA_MASO
	 FROM v_benh_nhan_sonde vBnSonde
	 WHERE vBnSonde.BNPBA_PHUTROI=1 AND vBnSonde.NGAYAN=$P{ngayan})
 GROUP BY pba.KHOA_MASO) hasSondeDd ON pba.KHOA_MASO=hasSondeDd.KHOA_MASO
LEFT JOIN 
-- Count so benh nhan an qua sonde(id=1) va co che do an la STM(Id=3) 
(Select pba.KHOA_MASO AS KHOA_MASO, Count(bncda.DTDMCDA_MASO) AS COUNT_SONDE_STM
FROM benh_nhan_che_do_an bncda
	LEFT JOIN benh_nhan_phieu_bao_an bnpba ON bnpba.BNPBA_MASO=bncda.BNPBA_MASO
	LEFT JOIN phieu_bao_an pba ON bnpba.PHIEUBAOAN_MASO=pba.PHIEUBAOAN_MASO
WHERE pba.PHIEUBAOAN_NGAYAN=$P{ngayan}
	AND bncda.DTDMCDA_MASO=$P{cdoan_stm_maso} -- Che do an STM
	AND bncda.BNPBA_MASO NOT IN(
	 Select bncda1.BNPBA_MASO
	 FROM benh_nhan_che_do_an bncda1
	 WHERE bncda1.DTDMCDA_MASO=$P{cdoan_dd_maso}) -- Khong co che do an DD
	AND bncda.BNPBA_MASO IN(
	 Select vBnSonde.BNPBA_MASO
	 FROM v_benh_nhan_sonde vBnSonde
	 WHERE vBnSonde.BNPBA_PHUTROI=1 AND vBnSonde.NGAYAN=$P{ngayan})
GROUP BY pba.KHOA_MASO) countSondeStm ON pba.KHOA_MASO=countSondeStm.KHOA_MASO
LEFT JOIN
-- Count so benh nhan an qua sonde(id=1) va co che do an la STM(Id=3) va co them cac che do an khac, nhung khong co che do DD
-- Neu so luong nay lon hon so luong count o cau query tren thi hien thi dau * tren report 
(Select pba.KHOA_MASO AS KHOA_MASO, Count(bncda.BNPBA_MASO) AS HAS_SONDE_STM
FROM benh_nhan_che_do_an bncda
	LEFT JOIN benh_nhan_phieu_bao_an bnpba ON bnpba.BNPBA_MASO=bncda.BNPBA_MASO
	LEFT JOIN phieu_bao_an pba ON bnpba.PHIEUBAOAN_MASO=pba.PHIEUBAOAN_MASO
WHERE pba.PHIEUBAOAN_NGAYAN=$P{ngayan}
	AND bncda.BNPBA_MASO IN(Select bncda1.BNPBA_MASO
	 FROM benh_nhan_che_do_an bncda1
	 WHERE bncda1.DTDMCDA_MASO=$P{cdoan_stm_maso}) -- Che do an STM
	AND bncda.BNPBA_MASO NOT IN(Select bncda1.BNPBA_MASO
	 FROM benh_nhan_che_do_an bncda1
	 WHERE bncda1.DTDMCDA_MASO=$P{cdoan_dd_maso}) -- Khong co che do an DD
	AND bncda.BNPBA_MASO IN(
	 Select vBnSonde.BNPBA_MASO
	 FROM v_benh_nhan_sonde vBnSonde
	 WHERE vBnSonde.BNPBA_PHUTROI=1 AND vBnSonde.NGAYAN=$P{ngayan})
GROUP BY pba.KHOA_MASO) hasSondeStm ON pba.KHOA_MASO=hasSondeStm.KHOA_MASO
LEFT JOIN 
-- Count so benh nhan an qua sonde(id=1) va co che do an la CM(Id=5) 
(Select pba.KHOA_MASO AS KHOA_MASO, Count(bncda.DTDMCDA_MASO) AS COUNT_SONDE_CM
FROM benh_nhan_che_do_an bncda
	LEFT JOIN benh_nhan_phieu_bao_an bnpba ON bnpba.BNPBA_MASO=bncda.BNPBA_MASO
	LEFT JOIN phieu_bao_an pba ON bnpba.PHIEUBAOAN_MASO=pba.PHIEUBAOAN_MASO
WHERE pba.PHIEUBAOAN_NGAYAN=$P{ngayan}
	AND bncda.DTDMCDA_MASO=$P{cdoan_cm_maso} -- Che do an CM
	AND bncda.BNPBA_MASO NOT IN(
	 Select bncda1.BNPBA_MASO
	 FROM benh_nhan_che_do_an bncda1
	 WHERE bncda1.DTDMCDA_MASO IN($P{cdoan_dd_maso},$P{cdoan_stm_maso})) -- Khong co che do an DD, STM
	AND bncda.BNPBA_MASO IN(
	 Select vBnSonde.BNPBA_MASO
	 FROM v_benh_nhan_sonde vBnSonde
	 WHERE vBnSonde.BNPBA_PHUTROI=1 AND vBnSonde.NGAYAN=$P{ngayan})
GROUP BY pba.KHOA_MASO) countSondeCm ON pba.KHOA_MASO=countSondeCm.KHOA_MASO
