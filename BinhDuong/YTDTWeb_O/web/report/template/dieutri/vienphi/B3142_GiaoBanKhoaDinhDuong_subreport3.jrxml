	 WHERE bncda1.DTDMCDA_MASO IN($P{cdoan_dd_maso},$P{cdoan_stm_maso},$P{cdoan_cm_maso},$P{cdoan_lat_maso})) -- Khong co che do an DD, STM, CM, Lat
	AND bncda.BNPBA_MASO IN(
