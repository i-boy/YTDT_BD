	<parameter name="SOBENHAN" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["BD1100000075"]]></defaultValueExpression>
	</parameter>
	<parameter name="TYLEBH" isForPrompting="false" class="java.lang.Short"/>
	<parameter name="LANIN" isForPrompting="false" class="java.lang.String"/>
	<parameter name="THUNGAN" isForPrompting="false" class="java.lang.String"/>
	<parameter name="BIENLAISO" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[Select NOIDUNG_HEADER, NOIDUNG_DETAIL, DONVI, 
       SUM(SOLUONG) AS SOLUONG, DONGIA,
       SUM(BNTRA) AS BNTRA,
       SUM(BHYT_TRA) AS BHYT_TRA , LOAI
              

FROM
(SELECT v_clsmo.NOIDUNG_HEADER AS NOIDUNG_HEADER,
       v_clsmo.NOIDUNG_DETAIL AS NOIDUNG_DETAIL,
       v_clsmo.DONVI AS DONVI,
       (v_clsmo.LAN - v_clsmo.TRA) AS SOLUONG,
       v_clsmo.DONGIA AS DONGIA,
       v_clsmo.BNTRA AS BNTRA,
       (case when(v_clsmo.NGOAIDM = 1) then 0 else v_clsmo.DONGIA * (v_clsmo.LAN - v_clsmo.TRA) - v_clsmo.BNTRA end) AS BHYT_TRA,

       v_clsmo.LOAI AS LOAI  -- Thong tin nay dung de sort chinh xac theo mau neu co yeu cau
FROM
(SELECT decode(clsmo.CLSMO_LOAI , 'XN', '  Xét nghiệm và thăm dò chức năng'
        ,'CD', '   Chẩn đoán hình ảnh' 
        ,'TT' ,'    Thủ thuật, phẫu thuật'
        ,'PT','    Thủ thuật, phẫu thuật'
        ,'KT' ,'     Dịch vụ kỹ thuật cao'
        ,'MA' ,'      Máu và chế phảm máu'
        ,'VC' ,'        Vận chuyển',
        ' Ngày giường chuyên khoa'
       ) AS NOIDUNG_HEADER,
       clsbg.DTDMCLSBG_DIENGIAI AS NOIDUNG_DETAIL,
       'lần' AS DONVI,
       clsmo.CLSMO_LAN AS LAN,
       NVL(clsmo.CLSMO_TRA,0) AS TRA,
       clsmo.CLSMO_DONGIA AS DONGIA,
       clsmo.CLSMO_DONGIABH AS DONGIABH,
       clsmo.CLSMO_PHANDV AS PHANDV,
       clsmo.CLSMO_DONGIABNTRA AS BNTRA,
       clsmo.CLSMO_YEUCAU AS YEUCAU,
       clsmo.CLSMO_NDM AS NGOAIDM,
       NVL(clsmo.CLSMO_TYLEBH,0) AS TYLEBH,
       dmdt.DMDOITUONG_MA AS DTMA ,
       clsmo.CLSMO_LOAI AS LOAI

FROM cls_mo clsmo

     LEFT JOIN hsba_khoa hsbakhoa ON clsmo.HSBAKHOA_MASO = hsbakhoa.HSBAKHOA_MASO
     LEFT JOIN hsba ON hsbakhoa.HSBA_SOVAOVIEN = hsba.HSBA_SOVAOVIEN
     LEFT JOIN dm_doi_tuong dmdt ON hsba.DOITUONG_MA = dmdt.DMDOITUONG_MASO
     LEFT JOIN dt_dm_cls dmcls ON clsmo.CLSMO_LOAICLS = dmcls.DTDMCLS_MASO
     LEFT JOIN dt_dm_cls_bang_gia clsbg ON clsmo.CLSMO_MAHANG = clsbg.DTDMCLSBG_MASO

WHERE hsba.HSBA_SOVAOVIEN LIKE $P{SOBENHAN}) v_clsmo

UNION ALL
SELECT *
FROM
(SELECT DECODE(dmthuoc.DMTHUOC_PLBHYT,10,'        Vật tư y tế', '       Thuốc, dịch truyền') AS NOIDUNG_HEARDER,
       dmthuoc.DMTHUOC_TEN AS NOIDUNG_DETAIL,
       dvt.DMDONVITINH_TEN AS DONVI,
       SUM((tnt.THUOCNOITRU_SOLUONG - COALESCE(tnt.THUOCNOITRU_TRA,0))) AS SOLUONG,
       max(DECODE(tnt.THUOCNOITRU_KHONGTHU , 1, 0,tnt.THUOCNOITRU_DONGIATT)) AS DONGIA,
       SUM(DECODE(tnt.THUOCNOITRU_KHONGTHU , 1, 0,COALESCE(tnt.THUOCNOITRU_TIENBNTRA,0))) AS BNTRA,
       SUM(DECODE(tnt.THUOCNOITRU_KHONGTHU , 1, 0,(tnt.THUOCNOITRU_THANHTIEN - DECODE(tnt.THUOCNOITRU_KHONGTHU , 1, 0,COALESCE(tnt.THUOCNOITRU_TIENBNTRA,0))))) AS BHYT_TRA,
       lt.DMLOAITHUOC_MA AS LOAI   -- Thong tin nay dung de sort chinh xac theo mau neu co yeu cau
FROM thuoc_noi_tru tnt
     LEFT JOIN dm_thuoc dmthuoc ON tnt.THUOCNOITRU_MATHUOC = dmthuoc.DMTHUOC_MASO
     LEFT JOIN hsba_khoa hsbakhoa ON tnt.HSBA_KHOA = hsbakhoa.HSBAKHOA_MASO
     LEFT JOIN hsba ON hsbakhoa.HSBA_SOVAOVIEN = hsba.HSBA_SOVAOVIEN
     LEFT JOIN dm_phan_loai_thuoc plt ON dmthuoc.DMPHANLOAITHUOC_MASO = plt.DMPHANLOAITHUOC_MASO
     LEFT JOIN dm_loai_thuoc lt ON plt.DMLOAITHUOC_MASO = lt.DMLOAITHUOC_MASO    
     LEFT JOIN dm_don_vi_tinh dvt ON dmthuoc.DMDONVITINH_MASO = dvt.DMDONVITINH_MASO
WHERE hsba.HSBA_SOVAOVIEN LIKE $P{SOBENHAN}
      AND ((tnt.THUOCNOITRU_YEUCAU Is Null OR tnt.THUOCNOITRU_YEUCAU = 0)
      OR (tnt.THUOCNOITRU_NDM Is Null OR tnt.THUOCNOITRU_NDM = 0))
      AND (COALESCE(tnt.THUOCNOITRU_SOLUONG,0) - COALESCE(tnt.THUOCNOITRU_TRA,0) > 0)
GROUP BY dmthuoc.DMTHUOC_MASO,dmthuoc.DMTHUOC_PLBHYT,lt.DMLOAITHUOC_MA,dmthuoc.DMTHUOC_TEN,dvt.DMDONVITINH_TEN) v_tnt)  tmp
GROUP BY tmp.NOIDUNG_HEADER, tmp.NOIDUNG_DETAIL, tmp.DONGIA,TMP.DONVI, tmp.LOAI
ORDER BY tmp.NOIDUNG_HEADER DESC, tmp.NOIDUNG_DETAIL ASC]]></queryString>

