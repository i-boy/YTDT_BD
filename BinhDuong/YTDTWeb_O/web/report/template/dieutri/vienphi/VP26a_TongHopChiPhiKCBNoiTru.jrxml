  AND SUBSTR(HSBABHYT.HSBABHYT_SOTHEBH,4,2) LIKE $P{ma_noi_tinh}
	AND BENHVIEN.DMBENHVIEN_MA LIKE $P{maso_cskcb}
  AND HSBABHYT.HSBABHYT_TUYEN <> 1
  AND NHOMBHYT.DTDMPHLOAIBHYT_MASO = 6
 AND ( ( (NOT ( $P{chon} = 'IN')) AND  (KHOIBHYT.DTDMKHOIBHYT_MASO NOT IN ($P!{khoi})) )
	OR ( ( $P{chon} = 'IN') AND (KHOIBHYT.DTDMKHOIBHYT_MASO IN ($P!{khoi})) )
)
AND ( ( ( $P{makhoa} is not null) AND  (KHOA.DMKHOA_MA LIKE $P{makhoa}) )
