	<queryString><![CDATA[SELECT union_data.*
FROM
-- Benh nhan noi tinh KCD ban dau : Dung tuyen
(SELECT  'A' AS TUYENKCB_KYHIEU,
'BỆNH NHÂN NỘI TỈNH KCB BAN ĐẦU' AS TUYEN_KCB,
        'I' AS STT_DTTT,
        'Ðúng tuyến' AS DUNGTUYEN_TRAITUYEN,
        COUNT(HS.HSTHTOAN_MA) AS LUOTNGUOI,

        SUM(trunc((nvl2(hsba.HSBA_NGAYGIORAV,TO_DATE(hsba.HSBA_NGAYGIORAV),TO_DATE(CURRENT_DATE))  
	- nvl2(hsba.HSBA_NGAYGIOVAOV, TO_DATE(hsba.HSBA_NGAYGIOVAOV), TO_DATE(CURRENT_DATE)))
	,2) + $P{cong_ngay_dt}) AS SONGAYDIEUTRI,
         SUM(HSTHTOAN_XNTDCN) AS XETNGHIEM,

         SUM(HS.HSTHTOAN_CDHA) AS CHANDOAN_HINH_ANH,
       (SUM(HS.HSTHTOAN_THUOC) + SUM(HS.HSTHTOAN_THUOCNDM ) ) AS THUOC,
       SUM(HS.HSTHTOAN_MAU) AS MAU,
       (SUM(HS.HSTHTOAN_DVKT_TT) + SUM(HS.HSTHTOAN_PHAUTHUAT ) ) AS PTTT,
       (SUM(HS.HSTHTOAN_VTTH) + SUM(HS.HSTHTOAN_VTTHNDM ) ) AS VTTH,
        SUM(HS.HSTHTOAN_DV_KTC) AS KTC,
       (SUM(HS.HSTHTOAN_PHONG) + SUM(HS.HSTHTOAN_PHONGNDM ) ) AS GIUONG,
       SUM(HS.HSTHTOAN_CP_VC) AS VANCHUYEN,

         SUM(HS.HSTHTOAN_BNTRA)AS BNTRA,
         SUM(HS.HSTHTOAN_BHYT)AS BHYTCHI

   FROM HS_THTOAN HS LEFT JOIN HSBA ON HS.HSBA_SOVAOVIEN = HSBA.HSBA_SOVAOVIEN
        LEFT JOIN HSBA_BHYT HSBABHYT ON HSBABHYT.HSBA_SOVAOVIEN = HSBA.HSBA_SOVAOVIEN
        LEFT JOIN DT_DM_KHOI_BHYT KHOIBHYT ON HSBABHYT.HSBABHYT_KHOIBH = KHOIBHYT.DTDMKHOIBHYT_MASO
        LEFT JOIN DT_DM_NHOM_BHYT NHOMBHYT ON KHOIBHYT.DTDMNHOMBHYT_MASO = NHOMBHYT.DTDMNHOMBHYT_MASO
        LEFT JOIN DM_DOI_TUONG DOITUONG ON HSBA.DOITUONG_MA = DOITUONG.DMDOITUONG_MASO
        LEFT JOIN DM_BENH_VIEN BENHVIEN ON HSBABHYT.HSBABHYT_MAKCB = BENHVIEN.DMBENHVIEN_MASO
	LEFT JOIN DM_KHOA KHOA ON HSBA.HSBA_KHOADANGDT = KHOA.DMKHOA_MASO

   WHERE HSBA.HSBA_NGAYGIORAV is not null
        AND TO_DATE(HSBA.HSBA_NGAYGIORAV) >= TO_DATE($P{TuNgay})
  AND TO_DATE(HSBA.HSBA_NGAYGIORAV) <= TO_DATE($P{DenNgay})
	AND DOITUONG.DMDOITUONG_MA LIKE 'BH'
  AND SUBSTR(HSBABHYT.HSBABHYT_SOTHEBH,4,2) LIKE $P{ma_noi_tinh}
	AND BENHVIEN.DMBENHVIEN_MA LIKE $P{maso_cskcb}
  AND HSBABHYT.HSBABHYT_TUYEN = 1
	AND NHOMBHYT.DTDMPHLOAIBHYT_MASO = 6
  AND ( ( (NOT ( $P{chon} = 'IN')) AND  (KHOIBHYT.DTDMKHOIBHYT_MASO NOT IN ($P!{khoi})) )
	OR ( ( $P{chon} = 'IN') AND (KHOIBHYT.DTDMKHOIBHYT_MASO IN ($P!{khoi})) )
)
AND ( ( ( $P{makhoa} is not null) AND  (KHOA.DMKHOA_MA LIKE $P{makhoa}) )
	OR ( ( $P{makhoa} is null) AND (HSBA.HSBA_NGAYGIORAV is not null) )
)
UNION ALL
