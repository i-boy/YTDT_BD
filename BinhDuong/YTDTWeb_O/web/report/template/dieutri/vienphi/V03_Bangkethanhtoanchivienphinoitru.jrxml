	<queryString><![CDATA[SELECT hs.HSBA_SOVAOVIEN AS SOVAOVIEN,
 bn.BENHNHAN_HOTEN AS HOTEN,
 bn.BENHNHAN_TUOI AS TUOI,
 bn.BENHNHAN_DONVITUOI AS DONVITUOI ,
 dmgt.DMGT_TEN AS GIOITINH,
 dmkhoa.DMKHOA_MA AS KHOA,
 hsba.HSBA_NGAYGIOVAOV AS NGAYGIOVAOV,
 hsba.HSBA_NGAYGIORAV AS NGAYGIORAV,
(trunc(	to_date(case when  (hsba.HSBA_NGAYGIORAV IS NULL) then 
(case when (to_date(current_date)>= TO_DATE($P{DenNgay}))then  TO_DATE($P{DenNgay}) else TO_DATE(CURRENT_DATE) end )
ELSE
(case when (TO_DATE(CURRENT_DATE)>=TO_DATE(hsba.HSBA_NGAYGIORAV)) then  (case when (TO_DATE(hsba.HSBA_NGAYGIORAV)>=TO_DATE($P{DenNgay})) then TO_DATE($P{DenNgay}) else TO_DATE(hsba.HSBA_NGAYGIORAV)  end) else TO_DATE(CURRENT_DATE) end )
end) 
- to_date(case when (TO_DATE($P{TuNgay})>=TO_DATE(hsba.HSBA_NGAYGIOVAOV)) then TO_DATE($P{TuNgay}) else TO_DATE(hsba.HSBA_NGAYGIOVAOV) end )
						,2) + 1) as SONGAYDIEUTRI,
 khoibhyt.DTDMKHOIBHYT_MA AS KHOIBHYT,
( case when(HS.HSTHTOAN_BNTRA Is Null) then 0 else HS.HSTHTOAN_BNTRA end + case when(HS.HSTHTOAN_BHYT Is Null) then 0 else HS.HSTHTOAN_BHYT end ) AS TONGCHI,
 HS.HSTHTOAN_MIENDT AS GIAM_MIEN_PHI,
 HS.HSTHTOAN_THTOAN AS THTOAN, 
 hs.HSTHTOAN_KYHIEU AS KYHIEU,
 hs.HSTHTOAN_QUYEN AS QUYEN,
 hs.HSTHTOAN_BIENLAI AS BIENLAI,
 hs.HSTHTOAN_NGAYTT AS NGAYTT,
(SELECT SUM(TU.TAMUNG_SOTIEN)
 FROM hs_thtoan hs2 LEFT JOIN hsba ON hs2.HSBA_SOVAOVIEN=hsba.HSBA_SOVAOVIEN
 LEFT JOIN tam_ung tu ON tu.HSBA_SOVAOVIEN=hsba.HSBA_SOVAOVIEN
 WHERE hs2.HSTHTOAN_MA=hs.HSTHTOAN_MA AND(to_date(tu.TAMUNG_NGAYGIO) >=TO_DATE($P{TuNgay}) AND to_date(tu.TAMUNG_NGAYGIO) <=TO_DATE($P{DenNgay})))  TAMUNGTRONGKY,
(SELECT SUM(TU.TAMUNG_SOTIEN)
 FROM hs_thtoan hs2 LEFT JOIN hsba ON hs2.HSBA_SOVAOVIEN=hsba.HSBA_SOVAOVIEN
 LEFT JOIN tam_ung tu ON tu.HSBA_SOVAOVIEN=hsba.HSBA_SOVAOVIEN
 WHERE hs2.HSTHTOAN_MA=hs.HSTHTOAN_MA AND(to_date(tu.TAMUNG_NGAYGIO) < TO_DATE($P{TuNgay}) AND to_date(tu.TAMUNG_NGAYGIO) > TO_DATE($P{DenNgay})))  TAMUNGNGOAIKY,
(SELECT SUM(hu.HOANUNG_SOTIEN)
 FROM hs_thtoan hs2 LEFT JOIN hsba ON hs2.HSBA_SOVAOVIEN=hsba.HSBA_SOVAOVIEN
 LEFT JOIN hoan_ung hu ON hu.HSBA_SOVAOVIEN=hsba.HSBA_SOVAOVIEN
 WHERE hs2.HSTHTOAN_MA=hs.HSTHTOAN_MA AND(to_date(hu.HOANUNG_NGAYGIO) >=TO_DATE($P{TuNgay}) AND to_date(hu.HOANUNG_NGAYGIO) <=TO_DATE($P{DenNgay})))  HOANUNGTRONGKY,
(SELECT SUM(hu.HOANUNG_SOTIEN)
 FROM hs_thtoan hs2 LEFT JOIN hsba ON hs2.HSBA_SOVAOVIEN=hsba.HSBA_SOVAOVIEN
 LEFT JOIN hoan_ung hu ON hu.HSBA_SOVAOVIEN=hsba.HSBA_SOVAOVIEN
 WHERE hs2.HSTHTOAN_MA=hs.HSTHTOAN_MA AND(to_date(hu.HOANUNG_NGAYGIO) < TO_DATE($P{TuNgay}) AND to_date(hu.HOANUNG_NGAYGIO) > TO_DATE($P{DenNgay})))  HOANUNGNGOAIKY,
 
 tmpTamUng.TAMUNG_SOTIEN AS TAMUNG,
 tmpHoanUng.HOANUNG_SOTIEN AS HOANUNG, 
 HS.HSTHTOAN_BOVIEN AS BOVIEN
FROM hs_thtoan hs LEFT JOIN hsba ON hs.HSBA_SOVAOVIEN=hsba.HSBA_SOVAOVIEN
 LEFT JOIN benh_nhan bn ON hsba.BENHNHAN_MA=bn.BENHNHAN_MA
 LEFT JOIN dm_gioi_tinh dmgt ON bn.DMGT_MASO=dmgt.DMGT_MASO
 LEFT JOIN dm_khoa dmkhoa ON hsba.HSBA_KHOADANGDT=dmkhoa.DMKHOA_MASO
 LEFT JOIN tiep_don td ON hsba.TIEPDON_MA=td.TIEPDON_MA
 LEFT JOIN dt_dm_khoi_bhyt khoibhyt ON td.KHOIBHYT_MA=khoibhyt.DTDMKHOIBHYT_MA
 LEFT JOIN(SELECT tu.HSBA_SOVAOVIEN AS SOVAOVIEN,
 SUM(TU.TAMUNG_SOTIEN) AS TAMUNG_SOTIEN
	 FROM tam_ung tu
 LEFT JOIN hs_thtoan hstt ON tu.HSBA_SOVAOVIEN=hstt.HSBA_SOVAOVIEN
 WHERE
 hstt.HSTHTOAN_NGAYTT >=TO_DATE($P{TuNgay})
 	 and
 hstt.HSTHTOAN_NGAYTT <= TO_DATE($P{DenNgay})
 GROUP BY tu.HSBA_SOVAOVIEN) tmpTamUng ON hsba.HSBA_SOVAOVIEN=tmpTamUng.SOVAOVIEN

 LEFT JOIN(SELECT hu.HSBA_SOVAOVIEN AS SOVAOVIEN,
 SUM(hu.HOANUNG_SOTIEN) AS HOANUNG_SOTIEN
	 FROM hoan_ung hu
 LEFT JOIN hs_thtoan hstt ON hstt.HSBA_SOVAOVIEN=hu.HSBA_SOVAOVIEN
 WHERE
 hstt.HSTHTOAN_NGAYTT >=TO_DATE($P{TuNgay})
 	 and
 hstt.HSTHTOAN_NGAYTT <=TO_DATE($P{DenNgay})
 GROUP BY hu.HSBA_SOVAOVIEN) tmpHoanUng ON hsba.HSBA_SOVAOVIEN=tmpHoanUng.SOVAOVIEN
Where hs.hsthtoan_ngaytt >=TO_DATE($P{TuNgay}) AND hs.hsthtoan_ngaytt <=TO_DATE($P{DenNgay})
 AND hs.HSTHTOAN_THTOAN < 0]]></queryString>

