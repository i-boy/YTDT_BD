	<parameter name="SOYTE" isForPrompting="false" class="java.lang.String"/>
	<parameter name="BENHVIEN" isForPrompting="false" class="java.lang.String"/>
	<parameter name="DIENTHOAI" isForPrompting="false" class="java.lang.String"/>
	<parameter name="TINH" isForPrompting="false" class="java.lang.String"/>
	<parameter name="TUNGAYDATE" isForPrompting="false" class="java.util.Date"/>
	<parameter name="DENNGAYDATE" isForPrompting="false" class="java.util.Date"/>
	<parameter name="KHOAMASO" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="BNTRA" isForPrompting="false" class="java.lang.Double"/>
	<parameter name="KHOARAVIEN" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[select hsBH.HSBABHYT_MA,bn.BENHNHAN_HOTEN,bn.BENHNHAN_TUOI TUOI,
(CASE(bn.BENHNHAN_DONVITUOI)  WHEN 2 THEN 'T' WHEN 3 THEN  'N' ELSE '' END)DONVI ,
 gt.DMGT_TEN ,
 (NVL2(bn.BENHNHAN_DIACHI,(bn.BENHNHAN_DIACHI||' '),'')||
 NVL2(bn.XA_MA ,(xa.DMXA_TEN||' '),'')||
 NVL2(bn.HUYEN_MA ,(huyen.DMHUYEN_TEN||' '),'')||
 NVL2(bn.TINH_MA,tinh.DMTINH_TEN,'')) DIACHI , 
hs.HSBA_SOVAOVIEN,khoa.DMKHOA_MA ,khoi.DTDMKHOIBHYT_MA ,khoi.DTDMKHOIBHYT_TYLEBHYT1 
,(hs.HSBA_NGAYGIOVAOV) AS  HSBA_NGAYGIOVAOV,(hs.HSBA_NGAYGIORAV) AS HSBA_NGAYGIORAV,to_date(hstt.HSTHTOAN_NGAYTT) AS HSTHTOAN_NGAYTT,
NVL2(hstt.HSTHTOAN_BHYT   ,hstt.HSTHTOAN_BHYT, 0) HSTHTOAN_BHYT 
,NVL2(hstt.HSTHTOAN_BNTRA  , hstt.HSTHTOAN_BNTRA, 0  ) HSTHTOAN_BNTRA
,(CASE WHEN (hstt.HSTHTOAN_BNTRA IS NULL OR hstt.HSTHTOAN_BNTRA =0)  THEN  0  ELSE 1  END) COUNT_BNTRA
,(CASE WHEN ( hs.HSBA_NGAYGIORAV IS NOT NULL AND hs.HSBA_NGAYGIOVAOV IS NOT NULL) THEN TO_DATE(hs.HSBA_NGAYGIORAV) - TO_DATE(hs.HSBA_NGAYGIOVAOV) + 1  ELSE  0  END) SONGAYDIEUTRI    
from (((((((((HSBA_BHYT hsBH 
join HS_THTOAN hstt on(hstt.HSBA_SOVAOVIEN = hsBH.HSBA_SOVAOVIEN)) 
left join DT_DM_KHOI_BHYT khoi on(hsBH.HSBABHYT_KHOIBH = khoi.DTDMKHOIBHYT_MASO)) 
left join HSBA hs on(hsBH.HSBA_SOVAOVIEN = hs.HSBA_SOVAOVIEN)) 
left join DM_KHOA khoa on(hs.HSBA_KHOADANGDT = khoa.DMKHOA_MASO)) 
left join BENH_NHAN bn on(hs.BENHNHAN_MA = bn.BENHNHAN_MA)) 
INNER JOIN DM_GIOI_TINH gt on(bn.DMGT_MASO = gt.DMGT_MASO)) 
INNER JOIN DM_TINH tinh on(bn.TINH_MA = tinh.DMTINH_MASO)) 
INNER JOIN DM_HUYEN huyen on(bn.HUYEN_MA = huyen.DMHUYEN_MASO)) 
INNER JOIN DM_XA xa on(bn.XA_MA = xa.DMXA_MASO)) 
 
where 
(
($P{TUNGAYDATE} is NULL) or ($P{DENNGAYDATE} is NULL) or 
(
((TO_DATE(hstt.HSTHTOAN_NGAYTT)>=TO_DATE($P{TUNGAYDATE}))  ) and 
( (TO_DATE(hstt.HSTHTOAN_NGAYTT)<=TO_DATE($P{DENNGAYDATE})))
)
) 
AND ($P{KHOAMASO} is null or hstt.HSTHTOAN_KHOA = $P{KHOAMASO}) 
AND ($P{BNTRA} is null or hstt.HSTHTOAN_BNTRA > $P{BNTRA})]]></queryString>

