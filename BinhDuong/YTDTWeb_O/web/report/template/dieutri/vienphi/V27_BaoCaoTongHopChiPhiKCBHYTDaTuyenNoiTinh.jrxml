	<queryString><![CDATA[SELECT BN.BENHNHAN_HOTEN HOTEN, BENH.DMBENHICD_MA MABENH,KCBBHYT.DTDMKCBBHYT_TEN TEN_KCB_BHYT, 
		HSBA1.HSBA_NGAYGIOVAOV NGAYVAOV , HSBA1.HSBA_NGAYGIORAV NGAYRAV,
	0 NGOAITRU, HS.HSTHTOAN_TONGCHI NOITRU
					
			
				FROM ((((( (HSBA HSBA1 JOIN HS_THTOAN HS ON HS.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN)
				
				JOIN HSBA_BHYT HSBABHYT ON HSBABHYT.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN)	
				LEFT JOIN DT_DM_KCB_BHYT KCBBHYT ON HSBABHYT.HSBABHYT_MAKCB LIKE KCBBHYT.DTDMKCBBHYT_MASO)
				LEFT JOIN DM_BENH_ICD BENH ON BENH.DMBENHICD_MASO LIKE HSBA1.HSBA_MACHDOANTUYENT)					
				LEFT JOIN BENH_NHAN BN ON BN.BENHNHAN_MA LIKE HSBA1.BENHNHAN_MA)
				LEFT JOIN DT_DM_TINH_BHYT TINHBHYT ON TINHBHYT.DTDMTINHBHYT_MASO = HSBABHYT.HSBABHYT_TINHBH)
		WHERE 	($P{NoiTinh} IS NULL OR TINHBHYT.DTDMTINHBHYT_MA LIKE $P{NoiTinh})
			and ($P{MaKCBBHYT} IS NULL OR KCBBHYT.DTDMKCBBHYT_MA LIKE $P{MaKCBBHYT})

	UNION
		SELECT BN.BENHNHAN_HOTEN HOTEN, BENH.DMBENHICD_MA MABENH,KCBBHYT.DTDMKCBBHYT_TEN TEN_KCB_BHYT, 
		TD1.TIEPDON_NGAYGIO NGAYVAOV, TD1.TIEPDON_NGAYGIO NGAYRAV,
		HS.HSTHTOANK_TONGCHI NGOAITRU, 0  NOITRU
		
		FROM 
				(((((TIEP_DON TD1 JOIN HS_THTOANK HS ON HS.TIEPDON_MA = TD1.TIEPDON_MA)
				JOIN DT_DM_KCB_BHYT KCBBHYT ON 	KCBBHYT.DTDMKCBBHYT_MA LIKE TD1.KCBBHYT_MA)
				LEFT JOIN BENH_NHAN BN ON BN.BENHNHAN_MA LIKE TD1.BENHNHAN_MA)
				LEFT JOIN DM_BENH_ICD BENH ON BENH.DMBENHICD_MASO = TD1.TIEPDON_MACHDOANBD)
				LEFT JOIN DT_DM_TINH_BHYT TINHBHYT ON TINHBHYT.DTDMTINHBHYT_MASO = TD1.TINHBHYT_MA)

	    WHERE 	($P{NoiTinh} IS NULL OR TINHBHYT.DTDMTINHBHYT_MA LIKE $P{NoiTinh})		
			and ($P{MaKCBBHYT} IS NULL OR KCBBHYT.DTDMKCBBHYT_MA LIKE $P{MaKCBBHYT})]]></queryString>

