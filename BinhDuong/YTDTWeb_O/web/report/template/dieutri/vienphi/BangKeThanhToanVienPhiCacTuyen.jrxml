	<parameter name="soyte" isForPrompting="false" class="java.lang.String"/>
	<parameter name="benhvien" isForPrompting="false" class="java.lang.String"/>
	<parameter name="sodt" isForPrompting="false" class="java.lang.String"/>
	<parameter name="MaSoBenhVien" isForPrompting="false" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[null]]></defaultValueExpression>
	</parameter>
	<parameter name="TenBenhVien" isForPrompting="false" class="java.lang.String"/>
	<parameter name="TINH" isForPrompting="false" class="java.lang.String"/>
	<parameter name="TuNgay" isForPrompting="true" class="java.util.Date"/>
	<parameter name="DenNgay" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT bn.BENHNHAN_HOTEN HOTEN,
       bn.BENHNHAN_TUOI TUOI,
       decode( bn.BENHNHAN_DONVITUOI , 2 , 'T'
	    , 3 ,  'N'
		,'') DONVITUOI,
       hsbabhyt.HSBABHYT_MAKCB MAKCB,
       khoa.DMKHOA_TEN KHOA,
       to_date(hsba.HSBA_NGAYGIOVAOV) NGAYGIOVAOV,
       to_date(hsba.HSBA_NGAYGIORAV) NGAYGIORAV,
       (trunc(to_date(nvl2(hsba.HSBA_NGAYGIORAV,
				 decode(greatest(to_date(current_date),to_date(hsba.HSBA_NGAYGIORAV)),to_date(current_date), decode(greatest(to_date(hsba.HSBA_NGAYGIORAV),to_DATE($P{DenNgay})), to_date(hsba.HSBA_NGAYGIORAV),to_DATE($P{DenNgay}), to_DATE(hsba.HSBA_NGAYGIORAV)) , to_DATE(current_date) ),
				 decode( greatest(to_date(current_date),to_DATE($P{DenNgay})),to_date(current_date), to_DATE($P{DenNgay}) , to_date(current_date))
				   
				))
				-
				to_date(decode(greatest(to_DATE($P{TuNgay}), to_DATE(hsba.HSBA_NGAYGIOVAOV)),to_DATE($P{TuNgay}), to_DATE($P{TuNgay}) , to_DATE(hsba.HSBA_NGAYGIOVAOV) ))
			) + 1) SONGAYDIEUTRI,
    ( nvl(hstt.HSTHTOAN_THUOC,0) +  nvl(hstt.HSTHTOAN_THUOCNDM, 0)) AS THUOC,
    hstt.HSTHTOAN_MAU AS MAU,
    hstt.HSTHTOAN_XNTDCN AS XETNGHIEM,
    hstt.HSTHTOAN_CDHA AS CHANDOAN_HINH_ANH,
    ( nvl(hstt.HSTHTOAN_PHAUTHUAT,0) +  nvl(hstt.HSTHTOAN_PHAUTHUATNDM, 0)) AS PT,
    hstt.HSTHTOAN_CONGKHAM AS CONGKHAM,
    hstt.HSTHTOAN_DVKT_TT AS THUTHUAT,
    hstt.HSTHTOAN_DV_KTC AS KTC,
    ( nvl(hstt.HSTHTOAN_PHONG,0) +  nvl(hstt.HSTHTOAN_PHONGNDM, 0)) AS GIUONG,
    hstt.HSTHTOAN_CP_VC AS VANCHUYEN,
    hstt.HSTHTOAN_TONGCHI AS TONGCONG

FROM hs_thtoan hstt
     LEFT JOIN hsba ON hstt.HSBA_SOVAOVIEN = hsba.HSBA_SOVAOVIEN
     LEFT JOIN benh_nhan bn ON hsba.BENHNHAN_MA = bn.BENHNHAN_MA
     LEFT JOIN dm_khoa khoa ON hstt.HSTHTOAN_KHOA = khoa.DMKHOA_MASO
     LEFT JOIN hsba_bhyt hsbabhyt ON hstt.HSBA_SOVAOVIEN = hsbabhyt.HSBA_SOVAOVIEN
     LEFT JOIN dm_benh_vien benhvien ON hsbabhyt.HSBABHYT_MAKCB = benhvien.DMBENHVIEN_MASO
WHERE hstt.HSTHTOAN_NGAYTT Is Not Null
  AND to_DATE(hstt.HSTHTOAN_NGAYTT) >= to_date($P{TuNgay})
  AND to_DATE(hstt.HSTHTOAN_NGAYTT) <= to_date($P{DenNgay})
  AND (($P{MaSoBenhVien} is null) or hsbabhyt.HSBABHYT_MAKCB = $P{MaSoBenhVien})
  and (($P{MaSoBenhVien} is not null) or  hstt.HSTHTOAN_NGAYTT Is Not Null)]]></queryString>

