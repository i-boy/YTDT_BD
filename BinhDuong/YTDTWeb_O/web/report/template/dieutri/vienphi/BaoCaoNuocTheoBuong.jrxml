	<queryString><![CDATA[SELECT nn.DMKHOA_MASO,
       dmkhoa.DMKHOA_TEN,
       nn.DMTANG_MASO,
       dmtang.DMTANG_TEN,
       SUM(nn.NHAPNUOC_SOLUONG) AS SOLUONG
FROM nhap_nuoc nn
     LEFT JOIN dm_khoa dmkhoa ON nn.DMKHOA_MASO = dmkhoa.DMKHOA_MASO
     LEFT JOIN dm_tang dmtang ON nn.DMTANG_MASO = dmtang.DMTANG_MASO
WHERE to_date(nn.NHAPNUOC_NGAY) >= to_date($P{tungay})
      AND to_date(nn.NHAPNUOC_NGAY) <= to_date($P{denngay})
GROUP BY nn.DMKHOA_MASO, nn.DMTANG_MASO,dmkhoa.DMKHOA_TEN,dmtang.DMTANG_TEN
