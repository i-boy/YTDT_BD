	<queryString><![CDATA[SELECT td.TIEPDON_MA, 
	wm_concat(thuoc.DMTHUOC_TEN) as LIST_THUOC, 
	cls_temp.CLS as LIST_CLS
       
FROM (((THUOC_PHONG_KHAM tpk
	left join THAM_KHAM tk on (tpk.THUOCPHONGKHAM_THAMKHAM = tk.THAMKHAM_MA))
	left join TIEP_DON td on (tk.TIEPDON_MA = td.TIEPDON_MA))
	left join DM_THUOC thuoc on (tpk.THUOCPHONGKHAM_MATHUOC = thuoc.DMTHUOC_MASO)),
	
	( SELECT wm_concat(bg.DTDMCLSBG_DIENGIAI) as CLS

	FROM (((CLS_KHAM clsk
		left join THAM_KHAM tk1 on (clsk.CLSKHAM_THAMKHAM = tk1.THAMKHAM_MA))
		left join TIEP_DON td1 on (tk1.TIEPDON_MA = td1.TIEPDON_MA))
		left join DT_DM_CLS_BANG_GIA bg on (clsk.CLSKHAM_MAHANG = bg.DTDMCLSBG_MASO))

	WHERE ($P{MATIEPDON} is null or td1.TIEPDON_MA = $P{MATIEPDON})
	) cls_temp

WHERE ($P{MATIEPDON} is null or td.TIEPDON_MA = $P{MATIEPDON})
GROUP BY td.TIEPDON_MA,cls_temp.CLS]]></queryString>

