	<queryString><![CDATA[SELECT T.DMTHUOC_TEN TENHANG, DV.DMDONVITINH_TEN DONVI, NSX.DMNHASANXUAT_TEN SX,
	SUM(CT.THUOCPHONGKHAM_SOLUONG) SOLUONG, CT.THUOCPHONGKHAM_DONGIA DONGIA,
	SUM(CT.THUOCPHONGKHAM_SOLUONG) * CT.THUOCPHONGKHAM_DONGIA THANHTIEN, PL.DMPHANLOAITHUOC_MA MAPL

FROM THUOC_PHONG_KHAM CT 
	LEFT JOIN DM_THUOC T ON CT.THUOCPHONGKHAM_MATHUOC = T.DMTHUOC_MASO
	LEFT JOIN DM_PHAN_LOAI_THUOC PL ON T.DMPHANLOAITHUOC_MASO = PL.DMPHANLOAITHUOC_MASO
	LEFT JOIN DM_DON_VI_TINH DV ON T.DMDONVITINH_MASO = DV.DMDONVITINH_MASO
	LEFT JOIN DM_NHA_SAN_XUAT NSX ON CT.THUOCPHONGKHAM_HANGSX = NSX.DMNHASANXUAT_MASO
	LEFT JOIN THAM_KHAM TK ON CT.THUOCPHONGKHAM_THAMKHAM = TK.THAMKHAM_MA

WHERE (CT.THUOCPHONGKHAM_LOAI LIKE '1') 
	AND ($P{BANKHAMMASO} IS NULL OR TK.THAMKHAM_BANKHAM = $P{BANKHAMMASO}) 
	AND ($P{LOAITHUOCMASO} IS NULL or PL.DMLOAITHUOC_MASO = $P{LOAITHUOCMASO}) 	
	AND ($P{TUNGAYDATE} IS NULL OR (TO_DATE(CT.THUOCPHONGKHAM_NGAYGIO)>=TO_DATE($P{TUNGAYDATE}))  ) 
	AND ($P{DENNGAYDATE} IS NULL OR (TO_DATE(CT.THUOCPHONGKHAM_NGAYGIO)<=TO_DATE($P{DENNGAYDATE}))  ) 

GROUP BY T.DMTHUOC_TEN, DV.DMDONVITINH_TEN, NSX.DMNHASANXUAT_TEN, CT.THUOCPHONGKHAM_DONGIA, PL.DMPHANLOAITHUOC_MA]]></queryString>

