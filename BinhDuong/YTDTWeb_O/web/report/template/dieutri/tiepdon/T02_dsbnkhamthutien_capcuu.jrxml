	<parameter name="DTBHYT" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="NgayVaoVien" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT D.DMDOITUONG_MA DOITUONG
	, B.BENHNHAN_HOTEN HOTEN
	, B.BENHNHAN_TUOI TUOI
	, decode(B.BENHNHAN_DONVITUOI,2,'T',decode(B.BENHNHAN_DONVITUOI,3, 'Ng','')) DONVI
	, bk.DTDMBANKHAM_MA KHAM
	, (decode(T.TIEPDON_SOTHEBH , NULL,'',T.TIEPDON_SOTHEBH)||decode(T.TIEPDON_SOTHETE , NULL,'',T.TIEPDON_SOTHETE)|| decode(T.TIEPDON_THENGHEO , NULL,'',T.TIEPDON_THENGHEO)) SOTHE
	, T.TIEPDON_MA MATD
	, T.TIEPDON_NGAYGIO NGAYGIO
	, HS.HSTHTOANK_BNTRA AS BNTRA
	, kq.DMDIEUTRI_MA GIAIQUYET
	
FROM TIEP_DON T 
	LEFT JOIN DM_DOI_TUONG D ON T.DOITUONG_MA = D.DMDOITUONG_MASO
	LEFT JOIN BENH_NHAN B ON T.BENHNHAN_MA = B.BENHNHAN_MA
	LEFT JOIN HS_THTOANK HS ON HS.TIEPDON_MA = T.TIEPDON_MA
	LEFT JOIN DM_DIEU_TRI kq ON T.DIEUTRI_MA = kq.DMDIEUTRI_MASO
	LEFT JOIN DT_DM_BAN_KHAM bk ON T.TIEPDON_BANKHAM = bk.DTDMBANKHAM_MASO

WHERE (bk.DTDMBANKHAM_MA LIKE 'CC_')
	AND ($P{TuNgay} IS NULL OR (TO_DATE(T.TIEPDON_NGAYGIO)>=TO_DATE($P{TuNgay}))) 
	AND ($P{DenNgay} IS NULL OR (TO_DATE(T.TIEPDON_NGAYGIO)<=TO_DATE($P{DenNgay}))) 
	AND ($P{DoiTuong} IS NULL OR T.DOITUONG_MA = $P{DoiTuong}) 
	AND ($P{NgayVaoVien} IS NULL OR (TO_DATE(T.TIEPDON_NGAYGIO)<=TO_DATE($P{NgayVaoVien}) ))
	
GROUP BY T.TIEPDON_MA,D.DMDOITUONG_MA, B.BENHNHAN_HOTEN, B.BENHNHAN_TUOI, bk.DTDMBANKHAM_MA, T.TIEPDON_NGAYGIO, HS.HSTHTOANK_BNTRA, kq.DMDIEUTRI_MA,B.BENHNHAN_DONVITUOI,T.TIEPDON_SOTHEBH,T.TIEPDON_SOTHETE,T.TIEPDON_THENGHEO
ORDER BY T.TIEPDON_NGAYGIO]]></queryString>

