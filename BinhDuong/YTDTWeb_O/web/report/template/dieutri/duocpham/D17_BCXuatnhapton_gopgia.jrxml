	<parameter name="DMNCT_MASO" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="DMKHOA_MASO" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="DMNGUONKINHPHI_MASO" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="DMLOAITHUOC_MASO" isForPrompting="false" class="java.lang.Integer"/>
