	<parameter name="DENNGAY" isForPrompting="true" class="java.util.Date"/>
	<parameter name="SOYTE" isForPrompting="false" class="java.lang.String"/>
	<parameter name="BENHVIEN" isForPrompting="false" class="java.lang.String"/>
	<parameter name="DIENTHOAI" isForPrompting="false" class="java.lang.String"/>
	<parameter name="TINH" isForPrompting="false" class="java.lang.String"/>
	<parameter name="DMTHUOC_TEN" isForPrompting="false" class="java.lang.String"/>
	<parameter name="NHOMTHUOCMASO" isForPrompting="false" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT GOPGIA.DMTHUOC_MASO, GOPGIA.TEN_HANG, GOPGIA.DMDONVITINH_TEN, GOPGIA.DMPHANLOAITHUOC_MA, GOPGIA.TENPHANNHOM, 0 DONGIA,
    SUM(GOPGIA.SL_TONDAU) SL_TONDAU, SUM(GOPGIA.TT_TONDAU) TT_TONDAU,
    SUM(GOPGIA.SL_NHAP) SL_NHAP, SUM(GOPGIA.TT_NHAP) TT_NHAP,
    SUM(GOPGIA.SL_TRA) SL_TRA, SUM(GOPGIA.TT_TRA) TT_TRA,
    SUM(GOPGIA.SL_XUAT) SL_XUAT, SUM(GOPGIA.TT_XUAT) TT_XUAT,
    0 SL_THANHLY, 0 TT_THANHLY,
    SUM(GOPGIA.SL_TONCUOI) SL_TONCUOI, SUM(GOPGIA.TT_TONCUOI) TT_TONCUOI
FROM 
(
SELECT DT.DMTHUOC_MASO, DT.DMTHUOC_TEN || ' ' || DECODE(DT.DMNCT_MA,NULL, '',DT.DMNCT_MA) || ' ' || DECODE(DT.DMQUOCGIA_MA,NULL,'',DT.DMQUOCGIA_MA)  TEN_HANG, DT.MALIENKET,
      DT.DMPHANNHOMTHUOC_TEN TENPHANNHOM,
      DT.DMDONVITINH_TEN, DT.DMPHANLOAITHUOC_MA DMPHANLOAITHUOC_MA, DT.DONGIA DONGIA,
      SUM(DT.TONDAU) AS SL_TONDAU, SUM(DT.TONDAU) * DT.DONGIA AS TT_TONDAU,
      SUM(DT.NHAP) AS SL_NHAP, SUM(DT.NHAP) * DT.DONGIA AS TT_NHAP,
      SUM(DT.TRA) AS SL_TRA, SUM(DT.TRA) * DT.DONGIA AS TT_TRA,
      SUM(DT.XUAT)  AS SL_XUAT, SUM(DT.XUAT) * DT.DONGIA AS TT_XUAT,
      0 AS SL_THANHLY, 0 AS TT_THANHLY ,
      (SUM(DT.TONDAU) + SUM(DT.NHAP) + SUM(DT.TRA) - SUM(DT.XUAT)) AS SL_TONCUOI,
      (SUM(DT.TONDAU) + SUM(DT.NHAP) + SUM(DT.TRA) - SUM(DT.XUAT)) * DT.DONGIA AS TT_TONCUOI
FROM
(      
    SELECT TK.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, TK.TONKHO_MALIENKET MALIENKET, PNTHUOC.DMPHANNHOMTHUOC_TEN,
              DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA,
              SUM(TK.TONKHO_TON) as TONDAU, 0 NHAP, 0 TRA, 0 XUAT, TK.TONKHO_DONGIA DONGIA,
              PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, TK.DMNCT_MASO, TK.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
      FROM TON_KHO TK INNER JOIN (
                                  SELECT MAX(TK.TONKHO_MA) AS TONKHO_MA FROM TON_KHO TK INNER JOIN DM_KHOA KHOA ON TK.DMKHOA_MASO = KHOA.DMKHOA_MASO
                                    WHERE TK.DMKHOA_MASO = $P{DMKHOA_MASO}
                                  AND ($P{TUNGAY} IS NULL or TO_DATE(TONKHO_NGAYGIOCN) < TO_DATE($P{TUNGAY}))
                                    GROUP BY TK.TONKHO_MALIENKET, TK.DMKHOA_MASO
                                    ORDER BY TK.TONKHO_MA
                                 ) T1 ON TK.TONKHO_MA = T1.TONKHO_MA
      INNER JOIN DM_THUOC THUOC ON TK.DMTHUOC_MASO = THUOC.DMTHUOC_MASO
      INNER JOIN DM_DON_VI_TINH DVT ON THUOC.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
      LEFT JOIN DM_NGUON_KINH_PHI KP ON TK.DMNGUONKINHPHI_MASO = KP.DMNGUONKINHPHI_MASO
      LEFT JOIN DM_NGUON_CHUONG_TRINH NCT ON TK.DMNCT_MASO = NCT.DMNCT_MASO
      INNER JOIN DM_KHOA KHOA ON KHOA.DMKHOA_MASO = TK.DMKHOA_MASO
      INNER JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON THUOC.DMPHANLOAITHUOC_MASO = PLTHUOC.DMPHANLOAITHUOC_MASO
      INNER JOIN DM_LOAI_THUOC LTHUOC ON PLTHUOC.DMLOAITHUOC_MASO = LTHUOC.DMLOAITHUOC_MASO
      INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = THUOC.DMPHANNHOMTHUOC_MASO
      LEFT JOIN DM_QUOC_GIA QG ON TK.DMQUOCGIA_MASO = QG.DMQUOCGIA_MASO
      WHERE TK.DMKHOA_MASO = $P{DMKHOA_MASO}
      AND (TK.TONKHO_TON > 0)
      AND ($P{TUNGAY} IS NULL or TO_DATE(TK.TONKHO_NGAYGIOCN) < TO_DATE($P{TUNGAY}))
      GROUP BY TK.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, TK.TONKHO_MALIENKET, PNTHUOC.DMPHANNHOMTHUOC_TEN, DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA, TK.TONKHO_DONGIA, PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, TK.DMNCT_MASO, TK.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA      
UNION ALL
    SELECT CT.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CT.CTNHAPKHO_MALK MALIENKET, PNTHUOC.DMPHANNHOMTHUOC_TEN,
              DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA,
              0 TONDAU, SUM(CT.CTNHAPKHO_SOLUONG) NHAP, 0 TRA, 0 XUAT, CT.CTNHAPKHO_DONGIA DONGIA,
              PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, PNK.DMNCT_MASO, PNK.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
      FROM CT_NHAP_KHO CT
      INNER JOIN PHIEU_NHAP_KHO PNK ON PNK.PHIEUNHAPKHO_MA = CT.PHIEUNHAPKHO_MA
      INNER JOIN DM_THUOC THUOC ON CT.DMTHUOC_MASO = THUOC.DMTHUOC_MASO
      INNER JOIN DM_DON_VI_TINH DVT ON THUOC.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
      LEFT JOIN DM_NGUON_KINH_PHI KP ON PNK.DMNGUONKINHPHI_MASO = KP.DMNGUONKINHPHI_MASO
      LEFT JOIN DM_NGUON_CHUONG_TRINH NCT ON PNK.DMNCT_MASO = NCT.DMNCT_MASO
      INNER JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON THUOC.DMPHANLOAITHUOC_MASO = PLTHUOC.DMPHANLOAITHUOC_MASO
      INNER JOIN DM_LOAI_THUOC LTHUOC ON PLTHUOC.DMLOAITHUOC_MASO = LTHUOC.DMLOAITHUOC_MASO
      INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = THUOC.DMPHANNHOMTHUOC_MASO
    LEFT JOIN DM_QUOC_GIA QG ON CT.DMQUOCGIA_MASO = QG.DMQUOCGIA_MASO
      WHERE ($P{TUNGAY} IS NULL or TO_DATE(PNK.PHIEUNHAPKHO_NGAYGIOCN) >= TO_DATE($P{TUNGAY}))
    AND ($P{DENNGAY} IS NULL or TO_DATE(PNK.PHIEUNHAPKHO_NGAYGIOCN) <= TO_DATE($P{DENNGAY}))
    GROUP BY CT.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CT.CTNHAPKHO_MALK, PNTHUOC.DMPHANNHOMTHUOC_TEN, DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA,
    CT.CTNHAPKHO_DONGIA, PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, PNK.DMNCT_MASO, PNK.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA   
UNION ALL
    SELECT CT.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CT.CTXUATKHO_MALK MALIENKET, PNTHUOC.DMPHANNHOMTHUOC_TEN,
        DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA,
          0 TONDAU, 0 NHAP, 0 TRA, SUM(CT.CTXUATKHO_SOLUONG) XUAT, CT.CTXUATKHO_DONGIA DONGIA,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CT.DMNCT_MASO, CT.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
    FROM  CT_XUAT_KHO CT
    INNER JOIN PHIEU_XUAT_KHO XK ON CT.PHIEUXUATKHO_MA = XK.PHIEUXUATKHO_MA
    INNER JOIN DM_THUOC THUOC ON CT.DMTHUOC_MASO = THUOC.DMTHUOC_MASO
    INNER JOIN DM_DON_VI_TINH DVT ON THUOC.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
    LEFT JOIN DM_NGUON_KINH_PHI KP ON CT.DMNGUONKINHPHI_MASO = KP.DMNGUONKINHPHI_MASO
    LEFT JOIN DM_NGUON_CHUONG_TRINH NCT ON CT.DMNCT_MASO = NCT.DMNCT_MASO
    INNER JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON THUOC.DMPHANLOAITHUOC_MASO = PLTHUOC.DMPHANLOAITHUOC_MASO
    INNER JOIN DM_LOAI_THUOC LTHUOC ON PLTHUOC.DMLOAITHUOC_MASO = LTHUOC.DMLOAITHUOC_MASO
    INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = THUOC.DMPHANNHOMTHUOC_MASO
  LEFT JOIN DM_QUOC_GIA QG ON CT.DMQUOCGIA_MASO = QG.DMQUOCGIA_MASO
  WHERE ($P{TUNGAY} IS NULL or TO_DATE(XK.PHIEUXUATKHO_NGAYGIOCN) >= TO_DATE($P{TUNGAY}))
  AND ($P{DENNGAY} IS NULL or TO_DATE(XK.PHIEUXUATKHO_NGAYGIOCN) <= TO_DATE($P{DENNGAY}))
  AND XK.DMKHOA_XUAT = $P{DMKHOA_MASO}
  GROUP BY CT.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CT.CTXUATKHO_MALK, PNTHUOC.DMPHANNHOMTHUOC_TEN, DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA, CT.CTXUATKHO_DONGIA,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CT.DMNCT_MASO, CT.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA         
UNION ALL
  SELECT CT.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CT.CTXUATKHO_MALK MALIENKET, PNTHUOC.DMPHANNHOMTHUOC_TEN,
        DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA,
          0 TONDAU, SUM(CT.CTXUATKHO_SOLUONG) NHAP, 0 TRA, 0 XUAT, CT.CTXUATKHO_DONGIA DONGIA,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CT.DMNCT_MASO, CT.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
    FROM  CT_XUAT_KHO CT 
    INNER JOIN PHIEU_XUAT_KHO XK ON CT.PHIEUXUATKHO_MA = XK.PHIEUXUATKHO_MA
    INNER JOIN DM_THUOC THUOC ON CT.DMTHUOC_MASO = THUOC.DMTHUOC_MASO
    INNER JOIN DM_DON_VI_TINH DVT ON THUOC.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
    LEFT JOIN DM_NGUON_KINH_PHI KP ON CT.DMNGUONKINHPHI_MASO = KP.DMNGUONKINHPHI_MASO
    LEFT JOIN DM_NGUON_CHUONG_TRINH NCT ON CT.DMNCT_MASO = NCT.DMNCT_MASO
    INNER JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON THUOC.DMPHANLOAITHUOC_MASO = PLTHUOC.DMPHANLOAITHUOC_MASO
    INNER JOIN DM_LOAI_THUOC LTHUOC ON PLTHUOC.DMLOAITHUOC_MASO = LTHUOC.DMLOAITHUOC_MASO
    INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = THUOC.DMPHANNHOMTHUOC_MASO
  LEFT JOIN DM_QUOC_GIA QG ON CT.DMQUOCGIA_MASO = QG.DMQUOCGIA_MASO
  WHERE ($P{TUNGAY} IS NULL or TO_DATE(XK.PHIEUXUATKHO_NGAYGIOCN) >= TO_DATE($P{TUNGAY}))
  AND ($P{DENNGAY} IS NULL or TO_DATE(XK.PHIEUXUATKHO_NGAYGIOCN) <= TO_DATE($P{DENNGAY}))
  AND XK.DMKHOA_NHAN = $P{DMKHOA_MASO}
  GROUP BY CT.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CT.CTXUATKHO_MALK, PNTHUOC.DMPHANNHOMTHUOC_TEN, DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA, CT.CTXUATKHO_DONGIA,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CT.DMNCT_MASO, CT.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
UNION ALL
SELECT CTXBH.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CTXBH.CTXUATBH_MALK MALIENKET, PNTHUOC.DMPHANNHOMTHUOC_TEN,
        DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA,
          0 TONDAU, 0 NHAP, 0 TRA, SUM(CTXBH.CTXUATBH_SOLUONG) XUAT, CTXBH.CTXUATBH_DONGIA DONGIA,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CTXBH.DMNCT_MASO, CTXBH.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
    FROM CT_XUAT_BH CTXBH
    INNER JOIN PHIEU_XUAT_BH PXBH ON PXBH.PHIEUXUATBH_MA = CTXBH.PHIEUXUATBH_MA
    INNER JOIN DM_THUOC THUOC ON CTXBH.DMTHUOC_MASO = THUOC.DMTHUOC_MASO
    INNER JOIN DM_DON_VI_TINH DVT ON THUOC.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
    LEFT JOIN DM_NGUON_KINH_PHI KP ON KP.DMNGUONKINHPHI_MASO = CTXBH.DMNGUONKINHPHI_MASO
    LEFT JOIN DM_NGUON_CHUONG_TRINH NCT ON NCT.DMNCT_MASO = CTXBH.DMNCT_MASO
    INNER JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON THUOC.DMPHANLOAITHUOC_MASO = PLTHUOC.DMPHANLOAITHUOC_MASO
    INNER JOIN DM_LOAI_THUOC LTHUOC ON PLTHUOC.DMLOAITHUOC_MASO = LTHUOC.DMLOAITHUOC_MASO
    INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = THUOC.DMPHANNHOMTHUOC_MASO
    LEFT JOIN DM_QUOC_GIA QG ON QG.DMQUOCGIA_MASO = CTXBH.DMQUOCGIA_MASO
    WHERE ($P{TUNGAY} IS NULL or TO_DATE(PXBH.PHIEUXUATBH_NGAYGIOCN) >= TO_DATE($P{TUNGAY}))
    AND ($P{DENNGAY} IS NULL or TO_DATE(PXBH.PHIEUXUATBH_NGAYGIOCN) <= TO_DATE($P{DENNGAY}))
    AND DMKHOA_MASO = $P{DMKHOA_MASO}
    GROUP BY CTXBH.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CTXBH.CTXUATBH_MALK, PNTHUOC.DMPHANNHOMTHUOC_TEN,
        DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA,CTXBH.CTXUATBH_DONGIA,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CTXBH.DMNCT_MASO, CTXBH.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
UNION ALL
        SELECT CTXBH.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CTXBH.CTXUATBHXV_MALK MALIENKET, PNTHUOC.DMPHANNHOMTHUOC_TEN,
        DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA,
          0 TONDAU, 0 NHAP, 0 TRA, SUM(CTXBH.CTXUATBHXV_SOLUONG) XUAT, CTXBH.CTXUATBHXV_DONGIA DONGIA,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CTXBH.DMNCT_MASO, CTXBH.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
    FROM CT_XUAT_BH_XUAT_VIEN CTXBH
    INNER JOIN PHIEU_XUAT_BH_XUAT_VIEN PXV ON PXV.PHIEUXUATBHXV_MA = CTXBH.PHIEUXUATBHXV_MA
    INNER JOIN DM_THUOC THUOC ON CTXBH.DMTHUOC_MASO = THUOC.DMTHUOC_MASO
    INNER JOIN DM_DON_VI_TINH DVT ON THUOC.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
    LEFT JOIN DM_NGUON_KINH_PHI KP ON CTXBH.DMNGUONKINHPHI_MASO = KP.DMNGUONKINHPHI_MASO
    LEFT JOIN DM_NGUON_CHUONG_TRINH NCT ON CTXBH.DMNCT_MASO = NCT.DMNCT_MASO
    INNER JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON THUOC.DMPHANLOAITHUOC_MASO = PLTHUOC.DMPHANLOAITHUOC_MASO
    INNER JOIN DM_LOAI_THUOC LTHUOC ON PLTHUOC.DMLOAITHUOC_MASO = LTHUOC.DMLOAITHUOC_MASO
    INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = THUOC.DMPHANNHOMTHUOC_MASO
    LEFT JOIN DM_QUOC_GIA QG ON CTXBH.DMQUOCGIA_MASO = QG.DMQUOCGIA_MASO
    WHERE ($P{TUNGAY} IS NULL or TO_DATE(PXV.PHIEUXUATBHXV_NGAYGIOCN) >= TO_DATE($P{TUNGAY}))
    AND ($P{DENNGAY} IS NULL or TO_DATE(PXV.PHIEUXUATBHXV_NGAYGIOCN) <= TO_DATE($P{DENNGAY}))
    AND DMKHOA_MASO = $P{DMKHOA_MASO}
    GROUP BY CTXBH.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CTXBH.CTXUATBHXV_MALK, PNTHUOC.DMPHANNHOMTHUOC_TEN,
        DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA, CTXBH.CTXUATBHXV_DONGIA ,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CTXBH.DMNCT_MASO, CTXBH.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA   
UNION ALL 
        SELECT CTTNCC.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CTTNCC.CTTRANHACUNGCAP_MALK MALIENKET, PNTHUOC.DMPHANNHOMTHUOC_TEN,
        DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA,
          0 TONDAU, 0 NHAP, 0 TRA, SUM(CTTNCC.CTTRANHACUNGCAP_SOLUONG) XUAT, CTTNCC.CTTRANHACUNGCAP_DONGIA DONGIA,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CTTNCC.DMNCT_MASO, CTTNCC.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
    FROM CT_TRA_NHA_CUNG_CAP CTTNCC 
    INNER JOIN PHIEU_TRA_NHA_CUNG_CAP TNCC ON TNCC.PHIEUTRANHACUNGCAP_MA = CTTNCC.PHIEUTRANHACUNGCAP_MA
    INNER JOIN DM_THUOC THUOC ON CTTNCC.DMTHUOC_MASO = THUOC.DMTHUOC_MASO
    INNER JOIN DM_DON_VI_TINH DVT ON THUOC.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
    LEFT JOIN DM_NGUON_KINH_PHI KP ON CTTNCC.DMNGUONKINHPHI_MASO = KP.DMNGUONKINHPHI_MASO
    LEFT JOIN DM_NGUON_CHUONG_TRINH NCT ON CTTNCC.DMNCT_MASO = NCT.DMNCT_MASO
    INNER JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON THUOC.DMPHANLOAITHUOC_MASO = PLTHUOC.DMPHANLOAITHUOC_MASO
    INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = THUOC.DMPHANNHOMTHUOC_MASO
    INNER JOIN DM_LOAI_THUOC LTHUOC ON PLTHUOC.DMLOAITHUOC_MASO = LTHUOC.DMLOAITHUOC_MASO
  LEFT JOIN DM_QUOC_GIA QG ON CTTNCC.DMQUOCGIA_MASO = QG.DMQUOCGIA_MASO
    WHERE ($P{TUNGAY} IS NULL or TO_DATE(TNCC.PHIEUTRANHACUNGCAP_NGAYGIOCN) >= TO_DATE($P{TUNGAY}))
    AND ($P{DENNGAY} IS NULL or TO_DATE(TNCC.PHIEUTRANHACUNGCAP_NGAYGIOCN) <= TO_DATE($P{DENNGAY}))
  AND DMKHOA_XUAT = $P{DMKHOA_MASO}
    GROUP BY CTTNCC.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CTTNCC.CTTRANHACUNGCAP_MALK, PNTHUOC.DMPHANNHOMTHUOC_TEN,
        DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA, CTTNCC.CTTRANHACUNGCAP_DONGIA,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CTTNCC.DMNCT_MASO, CTTNCC.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA   
UNION ALL
    SELECT CT.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CT.CTTRAKHO_MALK MALIENKET, PNTHUOC.DMPHANNHOMTHUOC_TEN,
        DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA,
          0 TONDAU, 0 NHAP, 0 TRA, SUM(CT.CTTRAKHO_SOLUONG) XUAT, CT.CTTRAKHO_DONGIA DONGIA,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CT.DMNCT_MASO, CT.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
    FROM CT_TRA_KHO CT
    INNER JOIN PHIEU_TRA_KHO TK ON CT.PHIEUTRAKHO_MA = TK.PHIEUTRAKHO_MA
    INNER JOIN DM_THUOC THUOC ON CT.DMTHUOC_MASO = THUOC.DMTHUOC_MASO
    INNER JOIN DM_DON_VI_TINH DVT ON THUOC.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
    LEFT JOIN DM_NGUON_KINH_PHI KP ON CT.DMNGUONKINHPHI_MASO = KP.DMNGUONKINHPHI_MASO
    LEFT JOIN DM_NGUON_CHUONG_TRINH NCT ON CT.DMNCT_MASO = NCT.DMNCT_MASO
    INNER JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON THUOC.DMPHANLOAITHUOC_MASO = PLTHUOC.DMPHANLOAITHUOC_MASO
    INNER JOIN DM_LOAI_THUOC LTHUOC ON PLTHUOC.DMLOAITHUOC_MASO = LTHUOC.DMLOAITHUOC_MASO
    INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = THUOC.DMPHANNHOMTHUOC_MASO
    LEFT JOIN DM_QUOC_GIA QG ON CT.DMQUOCGIA_MASO = QG.DMQUOCGIA_MASO
    WHERE ($P{TUNGAY} IS NULL or TO_DATE(TK.PHIEUTRAKHO_NGAYGIOCN) >= TO_DATE($P{TUNGAY}))
    AND ($P{DENNGAY} IS NULL or TO_DATE(TK.PHIEUTRAKHO_NGAYGIOCN) <= TO_DATE($P{DENNGAY}))
    AND TK.DMKHOA_TRA = $P{DMKHOA_MASO}
    GROUP BY CT.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CT.CTTRAKHO_MALK, PNTHUOC.DMPHANNHOMTHUOC_TEN,
        DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA, CT.CTTRAKHO_DONGIA,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CT.DMNCT_MASO, CT.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
UNION ALL
    SELECT CT.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CT.CTTRAKHO_MALK MALIENKET, PNTHUOC.DMPHANNHOMTHUOC_TEN,
        DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA,
          0 TONDAU, 0 NHAP, SUM(CT.CTTRAKHO_SOLUONG) TRA, 0 XUAT, CT.CTTRAKHO_DONGIA DONGIA,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CT.DMNCT_MASO, CT.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
    FROM CT_TRA_KHO CT
    INNER JOIN PHIEU_TRA_KHO TK ON CT.PHIEUTRAKHO_MA = TK.PHIEUTRAKHO_MA
    INNER JOIN DM_THUOC THUOC ON CT.DMTHUOC_MASO = THUOC.DMTHUOC_MASO
    INNER JOIN DM_DON_VI_TINH DVT ON THUOC.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
    LEFT JOIN DM_NGUON_KINH_PHI KP ON CT.DMNGUONKINHPHI_MASO = KP.DMNGUONKINHPHI_MASO
    LEFT JOIN DM_NGUON_CHUONG_TRINH NCT ON CT.DMNCT_MASO = NCT.DMNCT_MASO
    INNER JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON THUOC.DMPHANLOAITHUOC_MASO = PLTHUOC.DMPHANLOAITHUOC_MASO
    INNER JOIN DM_LOAI_THUOC LTHUOC ON PLTHUOC.DMLOAITHUOC_MASO = LTHUOC.DMLOAITHUOC_MASO
    INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = THUOC.DMPHANNHOMTHUOC_MASO
  LEFT JOIN DM_QUOC_GIA QG ON CT.DMQUOCGIA_MASO = QG.DMQUOCGIA_MASO
    WHERE ($P{TUNGAY} IS NULL or TO_DATE(TK.PHIEUTRAKHO_NGAYGIOCN) >= TO_DATE($P{TUNGAY}))
    AND ($P{DENNGAY} IS NULL or TO_DATE(TK.PHIEUTRAKHO_NGAYGIOCN) <= TO_DATE($P{DENNGAY}))
      AND TK.DMKHOA_NHAN = $P{DMKHOA_MASO}
    GROUP BY CT.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, CT.CTTRAKHO_MALK, PNTHUOC.DMPHANNHOMTHUOC_TEN,
        DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA, CT.CTTRAKHO_DONGIA,
          PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, CT.DMNCT_MASO, CT.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
) DT
WHERE ($P{DMLOAITHUOC_MASO} IS NULL OR DT.DMLOAITHUOC_MASO = $P{DMLOAITHUOC_MASO})
AND ($P{DMPHANLOAI_MA} IS NULL OR DT.DMPHANLOAITHUOC_MA IN ($P!{DMPHANLOAI_MA}))
AND ($P{DMNCT_MASO} IS NULL OR DT.DMNCT_MASO = $P{DMNCT_MASO})
AND ($P{DMNGUONKINHPHI_MASO} IS NULL OR DT.DMNGUONKINHPHI_MASO = $P{DMNGUONKINHPHI_MASO})
AND ($P{NHOMTHUOCMASO} IS NULL or DT.DMPHANNHOMTHUOC_MASO = $P{NHOMTHUOCMASO})
AND ($P{DMTHUOC_TEN} IS NULL OR UPPER(DT.DMTHUOC_TEN) LIKE (('%'||$P{DMTHUOC_TEN})||'%'))
GROUP BY DT.DMTHUOC_MASO, DT.DMTHUOC_TEN,  DT.MALIENKET, DT.DMNCT_MA, DT.DMQUOCGIA_MA, DT.DMPHANNHOMTHUOC_TEN, DT.DMDONVITINH_TEN, DT.DMPHANLOAITHUOC_MA, DT.DONGIA
ORDER BY DT.DMPHANNHOMTHUOC_TEN, DT.DMTHUOC_TEN
) GOPGIA
GROUP BY GOPGIA.DMTHUOC_MASO, GOPGIA.TEN_HANG, GOPGIA.DMDONVITINH_TEN, GOPGIA.DMPHANLOAITHUOC_MA, GOPGIA.TENPHANNHOM
ORDER BY GOPGIA.TENPHANNHOM, GOPGIA.TEN_HANG]]></queryString>

