	<queryString><![CDATA[SELECT A.DMTHUOC_MA, A.TENHANG, A.DMDONVITINH_TEN, A.DMQUOCGIA_MA, A.DMLOAITHUOC_MA, A.DMPHANNHOMTHUOC_TEN DMPHANLOAITHUOC_TEN,
SUM(A.SOLUONG) AS SOLUONG, A.DONGIA AS DONGIA, SUM(A.SOLUONG) *A.DONGIA AS THANHTIEN
FROM
(
    SELECT TNT.THUOCNOITRU_NGAYGIO NGAY, hsba.DOITUONG_MA DOITUONG,
        TNT.THUOCNOITRU_KHO KHOAXUAT, TNT.THUOCNOITRU_KHOA KHOANHAN, T.DMTHUOC_MA, T.DMTHUOC_TEN,
        T.DMTHUOC_TEN || ' ' || DECODE(T.DMTHUOC_HAMLUONG,NULL,'',T.DMTHUOC_HAMLUONG)  TENHANG, DVT.DMDONVITINH_TEN,QG.DMQUOCGIA_MA,
        LT.DMLOAITHUOC_MA, TNT.THUOCNOITRU_SOLUONG  SOLUONG, TNT.THUOCNOITRU_DONGIABH  DONGIA,
        LT.DMLOAITHUOC_MASO, PLT.DMPHANLOAITHUOC_MASO, PNTHUOC.DMPHANNHOMTHUOC_TEN
    FROM THUOC_NOI_TRU TNT
    INNER JOIN HSBA_KHOA hsbak ON TNT.HSBA_KHOA = hsbak.HSBAKHOA_MASO
    INNER JOIN HSBA hsba ON hsbak.HSBA_SOVAOVIEN = hsba.HSBA_SOVAOVIEN
    INNER JOIN PHIEU_DU_TRU PDT ON TNT.THUOCNOITRU_MAPHIEUDT = PDT.PHIEUDT_MA
    INNER JOIN DM_THUOC T on (TNT.THUOCNOITRU_MATHUOC = T.DMTHUOC_MASO)
    INNER JOIN DM_DON_VI_TINH DVT on (T.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO)
    INNER JOIN DM_PHAN_LOAI_THUOC PLT on (T.DMPHANLOAITHUOC_MASO = PLT.DMPHANLOAITHUOC_MASO)
    INNER JOIN DM_LOAI_THUOC LT on (PLT.DMLOAITHUOC_MASO = LT.DMLOAITHUOC_MASO)
    INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = T.DMPHANNHOMTHUOC_MASO
    LEFT JOIN DM_QUOC_GIA QG on (TNT.THUOCNOITRU_QUOCGIA = QG.DMQUOCGIA_MASO)
    WHERE THUOCNOITRU_MAPHIEUPDTTRA IS NULL
    AND PDT.PHIEUDT_DAXUAT = 1
    AND ($P{TUNGAYDATE} IS NULL OR TO_DATE(TNT.THUOCNOITRU_NGAYGIO) >= TO_DATE($P{TUNGAYDATE}))
    AND ($P{DENNGAYDATE} IS NULL OR TO_DATE(TNT.THUOCNOITRU_NGAYGIO) <= TO_DATE($P{DENNGAYDATE}))
UNION ALL
    SELECT TNT.THUOCNOITRU_NGAYGIO NGAY, hsba.DOITUONG_MA DOITUONG,
    TNT.THUOCNOITRU_KHO KHOAXUAT, TNT.THUOCNOITRU_KHOA KHOANHAN, T.DMTHUOC_MA, T.DMTHUOC_TEN,
    T.DMTHUOC_TEN || ' ' || DECODE(T.DMTHUOC_HAMLUONG,NULL,'',T.DMTHUOC_HAMLUONG) TENHANG, DVT.DMDONVITINH_TEN,QG.DMQUOCGIA_MA,
    LT.DMLOAITHUOC_MA, TNT.THUOCNOITRU_SOLUONG SOLUONG, TNT.THUOCNOITRU_DONGIABH DONGIA,
    LT.DMLOAITHUOC_MASO, PLT.DMPHANLOAITHUOC_MASO, PNTHUOC.DMPHANNHOMTHUOC_TEN
    FROM THUOC_NOI_TRU TNT
    INNER JOIN HSBA_KHOA hsbak ON TNT.HSBA_KHOA = hsbak.HSBAKHOA_MASO
    INNER JOIN HSBA hsba ON hsbak.HSBA_SOVAOVIEN = hsba.HSBA_SOVAOVIEN
    INNER JOIN PHIEU_DU_TRU PDT ON TNT.THUOCNOITRU_MAPHIEUPDTTRA = PDT.PHIEUDT_MA
    INNER JOIN DM_THUOC T on (TNT.THUOCNOITRU_MATHUOC = T.DMTHUOC_MASO)
    INNER JOIN DM_DON_VI_TINH DVT on (T.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO)
    INNER JOIN DM_PHAN_LOAI_THUOC PLT on (T.DMPHANLOAITHUOC_MASO = PLT.DMPHANLOAITHUOC_MASO)
    INNER JOIN DM_LOAI_THUOC LT on (PLT.DMLOAITHUOC_MASO = LT.DMLOAITHUOC_MASO)
    INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = T.DMPHANNHOMTHUOC_MASO
    LEFT JOIN DM_QUOC_GIA QG on (TNT.THUOCNOITRU_QUOCGIA = QG.DMQUOCGIA_MASO)
    WHERE TNT.THUOCNOITRU_MAPHIEUPDTTRA IS NOT NULL
    AND ($P{TUNGAYDATE} IS NULL OR TO_DATE(TNT.THUOCNOITRU_NGAYGIO) >= TO_DATE($P{TUNGAYDATE}))
    AND ($P{DENNGAYDATE} IS NULL OR TO_DATE(TNT.THUOCNOITRU_NGAYGIO) <= TO_DATE($P{DENNGAYDATE}))
UNION ALL
    SELECT TNT.THUOCNOITRU_NGAYGIO NGAY, hsba.DOITUONG_MA DOITUONG, THUOCNOITRU_KHOA KHOAXUAT, THUOCNOITRU_KHOA KHOANHAN, T.DMTHUOC_MA, T.DMTHUOC_TEN,
    T.DMTHUOC_TEN || ' ' || DECODE(T.DMTHUOC_HAMLUONG,NULL,'',T.DMTHUOC_HAMLUONG) TENHANG, DVT.DMDONVITINH_TEN,QG.DMQUOCGIA_MA,
    LT.DMLOAITHUOC_MA, TNT.THUOCNOITRU_SOLUONG SOLUONG, TNT.THUOCNOITRU_DONGIABH DONGIA,
    LT.DMLOAITHUOC_MASO, PLT.DMPHANLOAITHUOC_MASO, PNTHUOC.DMPHANNHOMTHUOC_TEN
    FROM THUOC_NOI_TRU TNT
    INNER JOIN HSBA_KHOA hsbak ON TNT.HSBA_KHOA = hsbak.HSBAKHOA_MASO
    INNER JOIN HSBA hsba ON hsbak.HSBA_SOVAOVIEN = hsba.HSBA_SOVAOVIEN
    INNER JOIN DM_THUOC T on (TNT.THUOCNOITRU_MATHUOC = T.DMTHUOC_MASO)
    INNER JOIN DM_DON_VI_TINH DVT on (T.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO)
    INNER JOIN DM_PHAN_LOAI_THUOC PLT on (T.DMPHANLOAITHUOC_MASO = PLT.DMPHANLOAITHUOC_MASO)
    INNER JOIN DM_LOAI_THUOC LT on (PLT.DMLOAITHUOC_MASO = LT.DMLOAITHUOC_MASO)
    INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = T.DMPHANNHOMTHUOC_MASO
    LEFT JOIN DM_QUOC_GIA QG on (TNT.THUOCNOITRU_QUOCGIA = QG.DMQUOCGIA_MASO)
    WHERE TNT.THUOCNOITRU_MAPHIEU IS NOT NULL
    AND TNT.THUOCNOITRU_TUTRUC_PDT = 2
    AND ($P{TUNGAYDATE} IS NULL OR TO_DATE(TNT.THUOCNOITRU_NGAYGIO) >= TO_DATE($P{TUNGAYDATE}))
    AND ($P{DENNGAYDATE} IS NULL OR TO_DATE(TNT.THUOCNOITRU_NGAYGIO) <= TO_DATE($P{DENNGAYDATE}))
UNION ALL
    SELECT TNT.THUOCNOITRU_NGAYGIO NGAY, hsba.DOITUONG_MA DOITUONG, THUOCNOITRU_KHOA KHOAXUAT, THUOCNOITRU_KHOA KHOANHAN, T.DMTHUOC_MA, T.DMTHUOC_TEN,
    T.DMTHUOC_TEN || ' ' || DECODE(T.DMTHUOC_HAMLUONG,NULL,'',T.DMTHUOC_HAMLUONG) TENHANG, DVT.DMDONVITINH_TEN,QG.DMQUOCGIA_MA,
    LT.DMLOAITHUOC_MA, TNT.THUOCNOITRU_SOLUONG SOLUONG, TNT.THUOCNOITRU_DONGIABH DONGIA,
    LT.DMLOAITHUOC_MASO, PLT.DMPHANLOAITHUOC_MASO, PNTHUOC.DMPHANNHOMTHUOC_TEN
    FROM THUOC_NOI_TRU TNT
    INNER JOIN HSBA_KHOA hsbak ON TNT.HSBA_KHOA = hsbak.HSBAKHOA_MASO
    INNER JOIN HSBA hsba ON hsbak.HSBA_SOVAOVIEN = hsba.HSBA_SOVAOVIEN
    INNER JOIN DM_THUOC T on (TNT.THUOCNOITRU_MATHUOC = T.DMTHUOC_MASO)
    INNER JOIN DM_DON_VI_TINH DVT on (T.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO)
    INNER JOIN DM_PHAN_LOAI_THUOC PLT on (T.DMPHANLOAITHUOC_MASO = PLT.DMPHANLOAITHUOC_MASO)
    INNER JOIN DM_LOAI_THUOC LT on (PLT.DMLOAITHUOC_MASO = LT.DMLOAITHUOC_MASO)
    INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = T.DMPHANNHOMTHUOC_MASO
    LEFT JOIN DM_QUOC_GIA QG on (TNT.THUOCNOITRU_QUOCGIA = QG.DMQUOCGIA_MASO)
    WHERE TNT.THUOCNOITRU_TUTRUC_PDT = 1
    AND ($P{TUNGAYDATE} IS NULL OR TO_DATE(TNT.THUOCNOITRU_NGAYGIO) >= TO_DATE($P{TUNGAYDATE}))
    AND ($P{DENNGAYDATE} IS NULL OR TO_DATE(TNT.THUOCNOITRU_NGAYGIO) <= TO_DATE($P{DENNGAYDATE}))
) A
WHERE A.SOLUONG > 0
AND ($P{LOAITHUOCMASO} is NULL or A.DMLOAITHUOC_MASO=$P{LOAITHUOCMASO})
AND ($P{KHOANHAN} is NULL or A.KHOANHAN=$P{KHOANHAN})
AND ($P{KHOAXUAT} is NULL or A.KHOAXUAT=$P{KHOAXUAT})
AND ($P{PLTHUOCMA} is NULL or A.DMPHANLOAITHUOC_MASO = ($P{PLTHUOCMA}))
AND ($P{DOITUONGMASO} is NULL or A.DOITUONG = ($P{DOITUONGMASO}))
GROUP BY A.DMTHUOC_MA, A.TENHANG, A.DMDONVITINH_TEN, A.DMQUOCGIA_MA, A.DMLOAITHUOC_MA, A.DMPHANNHOMTHUOC_TEN, A.DONGIA
ORDER BY A.DMPHANNHOMTHUOC_TEN, A.DMTHUOC_MA]]></queryString>

