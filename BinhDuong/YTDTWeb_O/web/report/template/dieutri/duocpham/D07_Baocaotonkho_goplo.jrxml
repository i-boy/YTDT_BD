	<parameter name="NHOMTHUOCMASO" isForPrompting="false" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT TK.DMTHUOC_TEN TENTHUOC, TK.HAMLUONG, TK.TONKHO_LO, TK.DMDONVITINH_TEN DONVI, TK.DMQUOCGIA_MA NSX, TK.DMNCT_MA CT, TK.DMPHANNHOMTHUOC_TEN DMPHANNHOMTHUOC_TEN,
          SUM(TK.TONKHO_TON) TON, 0 DONGIA, SUM(TK.THANHTIEN) THANHTIEN
FROM
    (SELECT tk.dmthuoc_maso, T.DMTHUOC_TEN, DECODE(T.DMTHUOC_HAMLUONG,NULL , '', T.DMTHUOC_HAMLUONG) HAMLUONG, DECODE(TK.TONKHO_LO,NULL, '',TK.TONKHO_LO) TONKHO_LO,
          DVT.DMDONVITINH_TEN, QG.DMQUOCGIA_MA, NCT.DMNCT_MA, PNTHUOC.DMPHANNHOMTHUOC_TEN,
          TK.TONKHO_TON, TK.TONKHO_DONGIA, TK.TONKHO_DONGIA * sum(TK.TONKHO_TON) AS THANHTIEN
    FROM TON_KHO TK INNER JOIN 
        (select max(tonkho_ma) as tonkho_ma2,lower(tonkho_malienket) from ton_kho
         where ($P{NGAY} is NULL OR to_Date(tonkho_ngaygiocn)<= to_Date($P{NGAY}))
         and ($P{MAKHO} IS NULL or DMKHOA_MASO = $P{MAKHO}) group by lower(tonkho_malienket) order by tonkho_ma
         ) tkmax ON TK.TONKHO_MA = tkmax.TONKHO_MA2
    INNER join DM_THUOC T on (TK.DMTHUOC_MASO=t.DMTHUOC_MASO)
    INNER join DM_DON_VI_TINH DVT on (T.DMDONVITINH_MASO=dvt.DMDONVITINH_MASO)
    INNER join DM_PHAN_LOAI_THUOC PLT on (T.DMPHANLOAITHUOC_MASO=PLT.DMPHANLOAITHUOC_MASO)
    INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = T.DMPHANNHOMTHUOC_MASO
    left join DM_QUOC_GIA qg on (TK.DMQUOCGIA_MASO=qg.DMQUOCGIA_MASO)
    LEFT JOIN DM_NGUON_CHUONG_TRINH NCT ON TK.DMNCT_MASO = NCT.DMNCT_MASO
    WHERE ($P{MANSX} IS NULL or TK.DMQUOCGIA_MASO = $P{MANSX})
    AND ($P{MALOAI} IS NULL or PLT.DMLOAITHUOC_MASO = $P{MALOAI})
    AND ($P{MACT} IS NULL or TK.DMNCT_MASO = $P{MACT})
    AND ($P{MAKP} IS NULL or TK.DMNGUONKINHPHI_MASO = $P{MAKP})
    AND ($P{PHANLOAI} IS NULL OR  PLT.DMPHANLOAITHUOC_MA IN ($P!{PHANLOAI}) )
    AND ($P{NHOMTHUOCMASO} IS NULL or T.DMPHANNHOMTHUOC_MASO = $P{NHOMTHUOCMASO})
    AND ($P{TON0} IS NULL or TK.TONKHO_TON > 0)
   GROUP BY TK.DMTHUOC_MASO, T.DMTHUOC_TEN,T.DMTHUOC_HAMLUONG,TK.TONKHO_LO, TK.TONKHO_DONGIA, DVT.DMDONVITINH_TEN, QG.DMQUOCGIA_MA, NCT.DMNCT_MA, PNTHUOC.DMPHANNHOMTHUOC_TEN,TK.TONKHO_TON
    ) TK
group by TK.DMTHUOC_TEN, TK.HAMLUONG, TK.TONKHO_LO, TK.DMDONVITINH_TEN, TK.DMQUOCGIA_MA, TK.DMNCT_MA, TK.DMPHANNHOMTHUOC_TEN
order by TK.DMPHANNHOMTHUOC_TEN, TK.DMTHUOC_TEN]]></queryString>

