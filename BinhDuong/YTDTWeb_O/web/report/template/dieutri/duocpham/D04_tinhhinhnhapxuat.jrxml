	<parameter name="BOQUA" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT TK.TONKHO_MALIENKET, TK.DMTHUOC_TEN, nvl(trim(TK.DMTHUOC_HAMLUONG),'') DMTHUOC_HAMLUONG,
      TK.DMDONVITINH_TEN, TK.DMPHANLOAITHUOC_MA, TK.DMPHANLOAITHUOC_TEN, TK.DMNCT_MA,
      SUM(TK.TONDAU) AS TONDAU, SUM(TK.NHAP) AS NHAP, SUM(TK.TRA) AS TRA, SUM(TK.XUAT)  AS XUAT,
      (SUM(TK.TONDAU) + SUM(TK.NHAP) + SUM(TK.TRA) - SUM(TK.XUAT)) AS TON,TK.DMPHANNHOMTHUOC_TEN
FROM
(
  SELECT TKDAU.TONKHO_MALIENKET, TKDAU.DMTHUOC_TEN, TKDAU.DMTHUOC_HAMLUONG,
      TKDAU.DMDONVITINH_TEN, TKDAU.DMPHANLOAITHUOC_MA, TKDAU.DMPHANLOAITHUOC_TEN, TKDAU.DMNCT_MA,
      TKDAU.TONDAU, TKDAU.NHAP, TKDAU.TRA, TKDAU.XUAT,
      TKDAU.DMLOAITHUOC_MASO, TKDAU.DMPHANLOAITHUOC_MASO, TKDAU.DMNCT_MASO, TKDAU.DMNGUONKINHPHI_MASO,TKDAU.DMPHANNHOMTHUOC_TEN
  FROM
  (
      SELECT TK.TONKHO_MALIENKET, THUOC.DMTHUOC_TEN, decode(THUOC.DMTHUOC_HAMLUONG,NULL , '', THUOC.DMTHUOC_HAMLUONG) AS DMTHUOC_HAMLUONG,
              DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, PLTHUOC.DMPHANLOAITHUOC_TEN, NCT.DMNCT_MA,
              TK.TONKHO_TON as TONDAU, 0 NHAP, 0 TRA, 0 XUAT,
              PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, TK.DMNCT_MASO, TK.DMNGUONKINHPHI_MASO,PNTHUOC.DMPHANNHOMTHUOC_TEN
      FROM TON_KHO TK INNER JOIN (SELECT MAX(tonkho_ma) AS tonkho_ma2,LOWER(tonkho_malienket) FROM ton_kho
                                      WHERE ($P{TUNGAYDATE} IS NULL or (TO_DATE(TONKHO_NGAYGIOCN) < TO_DATE($P{TUNGAYDATE})))
                                      AND ($P{KHOMASO} IS NULL or DMKHOA_MASO = $P{KHOMASO})
                                      GROUP BY LOWER(tonkho_malienket)
                                      ORDER BY tonkho_ma)  T1 ON TK.TONKHO_MA = T1.TONKHO_MA2
      INNER JOIN DM_THUOC THUOC ON TK.DMTHUOC_MASO = THUOC.DMTHUOC_MASO
      INNER JOIN DM_DON_VI_TINH DVT ON THUOC.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
      INNER JOIN DM_NGUON_KINH_PHI KP ON TK.DMNGUONKINHPHI_MASO = KP.DMNGUONKINHPHI_MASO
      INNER JOIN DM_KHOA KHOA ON TK.DMKHOA_MASO = KHOA.DMKHOA_MASO
      INNER JOIN DM_NGUON_CHUONG_TRINH NCT ON TK.DMNCT_MASO = NCT.DMNCT_MASO
      INNER JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON THUOC.DMPHANLOAITHUOC_MASO = PLTHUOC.DMPHANLOAITHUOC_MASO
      INNER JOIN DM_LOAI_THUOC LTHUOC ON PLTHUOC.DMLOAITHUOC_MASO = LTHUOC.DMLOAITHUOC_MASO
      INNER JOIN DM_PHAN_NHOM_THUOC PNTHUOC ON PNTHUOC.DMPHANNHOMTHUOC_MASO = THUOC.DMPHANNHOMTHUOC_MASO
      WHERE (TK.TONKHO_TON > 0)
    ORDER BY TK.TONKHO_MA
  )  TKDAU

  UNION ALL

  SELECT TK.TONKHO_MALIENKET, T.DMTHUOC_TEN, DECODE(T.DMTHUOC_HAMLUONG , NULL , '', T.DMTHUOC_HAMLUONG) DMTHUOC_HAMLUONG,
       DVT.DMDONVITINH_TEN, PLT.DMPHANLOAITHUOC_MA, PLT.DMPHANLOAITHUOC_TEN, NCT.DMNCT_MA,
       0 TONDAU, SUM(TK.TONKHO_NHAP) NHAP,
       SUM(TK.TONKHO_TRA) TRA, SUM(TK.TONKHO_XUAT) XUAT,
       PLT.DMLOAITHUOC_MASO, PLT.DMPHANLOAITHUOC_MASO, TK.DMNCT_MASO, TK.DMNGUONKINHPHI_MASO,PNTHUOC.DMPHANNHOMTHUOC_TEN
    FROM TON_KHO TK, DM_THUOC T, DM_PHAN_LOAI_THUOC PLT, DM_DON_VI_TINH DVT, DM_NGUON_CHUONG_TRINH NCT,DM_PHAN_NHOM_THUOC PNTHUOC
    WHERE ($P{TUNGAYDATE} IS NULL or (TO_DATE(TONKHO_NGAYGIOCN)>=TO_DATE($P{TUNGAYDATE})))
    AND ($P{DENNGAYDATE} IS NULL or (TO_DATE(TONKHO_NGAYGIOCN)<=TO_DATE($P{DENNGAYDATE})))
    AND ($P{KHOMASO} IS NULL or TK.DMKHOA_MASO = $P{KHOMASO})
  AND TK.DMTHUOC_MASO = T.DMTHUOC_MASO
  AND T.DMPHANLOAITHUOC_MASO = PLT.DMPHANLOAITHUOC_MASO
  AND T.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
  AND TK.DMNCT_MASO = NCT.DMNCT_MASO
  AND PNTHUOC.DMPHANNHOMTHUOC_MASO = T.DMPHANNHOMTHUOC_MASO
    GROUP BY TK.TONKHO_MALIENKET
    , T.DMTHUOC_TEN,DMTHUOC_HAMLUONG,DVT.DMDONVITINH_TEN, PLT.DMPHANLOAITHUOC_MA, PLT.DMPHANLOAITHUOC_TEN, NCT.DMNCT_MA,
    PLT.DMLOAITHUOC_MASO, PLT.DMPHANLOAITHUOC_MASO, TK.DMNCT_MASO, TK.DMNGUONKINHPHI_MASO, PNTHUOC.DMPHANNHOMTHUOC_TEN
)  TK
WHERE ($P{LOAITHUOCMASO} IS NULL OR TK.DMLOAITHUOC_MASO = $P{LOAITHUOCMASO})
AND ($P{PLMASO} IS NULL OR TK.DMPHANLOAITHUOC_MASO = $P{PLMASO})
AND ($P{NCTMASO} IS NULL OR TK.DMNCT_MASO = $P{NCTMASO})
AND ($P{NKPMASO} IS NULL OR TK.DMNGUONKINHPHI_MASO = $P{NKPMASO})
GROUP BY TK.TONKHO_MALIENKET
,TK.DMTHUOC_TEN, DMTHUOC_HAMLUONG,
 TK.DMDONVITINH_TEN, TK.DMPHANLOAITHUOC_MA, TK.DMPHANLOAITHUOC_TEN, TK.DMNCT_MA,TK.DMPHANNHOMTHUOC_TEN
HAVING ($P{BOQUA} IS NULL OR SUM(TK.XUAT) >0)
order by TK.DMPHANNHOMTHUOC_TEN, TK.DMTHUOC_TEN]]></queryString>

