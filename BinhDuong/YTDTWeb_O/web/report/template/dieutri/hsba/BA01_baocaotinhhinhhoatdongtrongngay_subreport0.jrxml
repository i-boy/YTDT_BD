	<parameter name="time2" isForPrompting="false" class="java.util.Date"/>
	<parameter name="time3" isForPrompting="false" class="java.util.Date"/>
	<queryString><![CDATA[select 
dm_khoa.DMKHOA_TEN
,dm_khoa.DMKHOA_GIUONGKH as giuongkh
,dm_khoa.DMKHOA_GIUONGTK as giuongtk
,sum( case when(hsba.HSBA_KHOAVAOV=hsba_chuyen_mon.KHOA_MA and( hsba_chuyen_mon.HSBACM_NGAYGIOVAOK >=$P{time1} and hsba_chuyen_mon.HSBACM_NGAYGIOVAOK < $P{time3})) then 1 else 0 end )
as VAOVIEN
,sum( case when(hsba.HSBA_KHOAVAOV !=hsba_chuyen_mon.KHOA_MA and( hsba_chuyen_mon.HSBACM_NGAYGIOVAOK >=$P{time1} and hsba_chuyen_mon.HSBACM_NGAYGIOVAOK < $P{time3})) then 1 else 0 end )
as CHUYENVAOKHOA
,sum( case when(hsba_chuyen_mon.HSBACM_CHUYENKHOA !=null and( hsba_chuyen_mon.HSBACM_NGAYGIORAK >=$P{time1} and hsba_chuyen_mon.HSBACM_NGAYGIORAK < $P{time3})) then 1 else 0 end )
as CHUYENRAKHOA
,sum( case when(hsba_chuyen_mon.HSBACM_HUONGDIEUTRI='2' and(hsba_chuyen_mon.HSBACM_NGAYGIORAK >=$P{time1} AND hsba_chuyen_mon.HSBACM_NGAYGIORAK < $P{time3})) then 1 else 0 end )
as CHUYENVIEN
,sum( case when(hsba_chuyen_mon.KETQUA_MA=6 and(hsba.HSBA_NGAYGIORAV >=$P{time1} AND hsba.HSBA_NGAYGIORAV < $P{time3})) then 1 else 0 end )
as XINVE
,sum( case when(hsba_chuyen_mon.KETQUA_MA=5 and(hsba.HSBA_NGAYGIOTV >=$P{time1} AND hsba.HSBA_NGAYGIORAV < $P{time3})) then 1 else 0 end )
as TUVONG
,sum( case when(hsba_chuyen_mon.KETQUA_MA=8 and(hsba.HSBA_NGAYGIORAV >=$P{time1} AND hsba.HSBA_NGAYGIORAV < $P{time3})) then 1 else 0 end )
as TRONVIEN
,sum( case when(hsba_chuyen_mon.KETQUA_MA not in(5, 6, 8) and(hsba.HSBA_NGAYGIORAV >=$P{time1} AND hsba.HSBA_NGAYGIORAV < $P{time3})) then 1 else 0 end )
as RAVIEN
,sum( case when((dm_nghe_nghiep.DMNGHENGHIEP_PHANLOAI=1 or dm_nghe_nghiep.DMNGHENGHIEP_PHANLOAI=2) and(hsba.HSBA_NGAYGIORAV is null and hsba.HSBA_NGAYGIOTV is null and hsba_chuyen_mon.HSBACM_NGAYGIORAK is null)) then 1 else 0 end )
as CBNV
,sum( case when((benh_nhan.DMGT_MASO=1) and(hsba.HSBA_NGAYGIORAV is null and hsba.HSBA_NGAYGIOTV is null and hsba_chuyen_mon.HSBACM_NGAYGIORAK is null)) then 1 else 0 end )
as NU
,sum( case when(((benh_nhan.BENHNHAN_TUOI<15 and benh_nhan.BENHNHAN_DONVITUOI=1) or(benh_nhan.BENHNHAN_DONVITUOI!=1)) and(hsba.HSBA_NGAYGIORAV is null and hsba.HSBA_NGAYGIOTV is null and hsba_chuyen_mon.HSBACM_NGAYGIORAK is null)) then 1 else 0 end )
as LESS15
,sum( case when((hsba.HSBA_NGAYGIORAV is null and hsba.HSBA_NGAYGIOTV is null and hsba_chuyen_mon.HSBACM_NGAYGIORAK is null)) then 1 else 0 end )
as CONLAI

from hsba_chuyen_mon
left join dm_khoa ON hsba_chuyen_mon.KHOA_MA=dm_khoa.DMKHOA_MASO
left join hsba ON hsba_chuyen_mon.HSBA_SOVAOVIEN=hsba.HSBA_SOVAOVIEN
left join benh_nhan ON hsba.BENHNHAN_MA=benh_nhan.BENHNHAN_MA
left join dm_nghe_nghiep ON benh_nhan.BENHNHAN_NGHE=dm_nghe_nghiep.DMNGHENGHIEP_MASO

group by dmkhoa_ten,dm_khoa.DMKHOA_GIUONGKH,dm_khoa.DMKHOA_GIUONGTK
order by dmkhoa_ten]]></queryString>

