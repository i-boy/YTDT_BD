 select 'Bỏng (W85-W99,X00-X19)' as Name, 5 as pos , 0 as pos1, '5Bỏng (W85-W99,X00-X19)' as poss  from dual union all
 select 'Đuối nước (W65-W84)' as Name, 4 as pos , 0 as pos1, '4Đuối nước (W65-W84)' as poss  from dual union all
