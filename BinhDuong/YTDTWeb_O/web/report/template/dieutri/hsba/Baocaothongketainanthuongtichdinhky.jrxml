left join dm_phuong_thuc_gay_tai_nan on hsba.DMPTGTN_MASO = dm_phuong_thuc_gay_tai_nan.DMPTGTN_MASO
where hsba.TAINAN_MA = 4  and hsba.HSBA_NGAYGIOVAOV BETWEEN TO_DATE($P{TuNgay})  AND TO_DATE($P{DenNgay}+1)

union all
select Decode(dm_tai_nan.DMTAINAN_MASO,4,'-  Ngộ độc',dm_tai_nan.DMTAINAN_TEN)  as TIEUDE
, '' as BENHNHANH_MA,0 as BENHNHAN_TUOI, 0 as DMGT_MASO, 0 as BENHNHANH_DONVITUOI, 0 as CHET, 0 as MAC
from dm_tai_nan

union all
select ('- '||dm_phuong_thuc_gay_tai_nan.DMPTGTN_TEN) as TIEUDE
, '' as BENHNHANH_MA,0 as BENHNHAN_TUOI, 0 as DMGT_MASO, 0 as BENHNHANH_DONVITUOI, 0 as CHET, 0 as MAC
from dm_phuong_thuc_gay_tai_nan where dm_phuong_thuc_gay_tai_nan.DMTAINAN_MASO = 4
) benh_nhana
group by benh_nhana.TIEUDE
)benhnhanfull
left join 
 (
 select '-  Ngộ độc' as Name, 6 as pos , 0 as pos1, '6Ngộ độc' as poss from dual union all
 select '- Động vật' as Name, 6 as pos , 3 as pos1, '-- Động vật' as poss  from dual union all
 select '- Hóa chất' as Name, 6 as pos ,1 as pos1, '-- Hóa chất' as poss  from dual union all
 select '- Khác' as Name, 6 as pos, 6 as pos1, '-- Khác' as poss  from dual union all
 select '- Rượu' as Name, 6 as pos , 5 as pos1, '-- Rượu' as poss  from dual union all
