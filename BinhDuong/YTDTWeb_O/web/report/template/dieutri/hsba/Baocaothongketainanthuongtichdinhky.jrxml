 select 'Ngã (W01-W19)' as Name, 1 as pos , 0 as pos1, '1Ngã (W01-W19)' as poss  from dual union all
 select 'Súc vật, động vật (W50-W64)' as Name,3 as pos , 0 as pos1, '3Súc vật, động vật (W50-W64)' as poss  from dual union all
 select 'Tai nạn giao thông (V01-V99)' as Name,0 as pos , 0 as pos1, '0Tai nạn giao thông (V01-V99)' as poss  from dual union all
 select 'Tai nạn lao động (W20-W49)' as Name, 2 as pos , 0 as pos1, '2Tai nạn lao động (W20-W49)' as poss  from dual union all
 select 'Tự tử (X60-X80)' as Name, 7 as pos  , 0 as pos1, '7Tự tử (X60-X80)' as poss from dual 
) dmpos
on benhnhanfull.MIEUTA = dmpos.Name
order by dmpos.pos,dmpos.pos1, dmpos.Name
) loaitainanfull

