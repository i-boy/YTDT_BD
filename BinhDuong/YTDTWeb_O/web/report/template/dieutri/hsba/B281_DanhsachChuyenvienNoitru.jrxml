	<queryString><![CDATA[SELECT
     hsba.hsba_sovaovien as sovaovien,
     bn.BENHNHAN_HOTEN AS hoten,
     bn.BENHNHAN_NAMSINH AS namsinh,
     gt.DMGT_TEN AS gioitinh,
     TO_DATE(hsba.HSBA_NGAYGIORAV)AS ngaygio,
     k.dmkhoa_ten AS khoa,
     bv.DMBENHVIEN_TEN AS benhvien
FROM
     benh_nhan bn INNER JOIN dm_gioi_tinh gt ON bn.DMGT_MASO = gt.DMGT_MASO
     INNER JOIN hsba hsba ON bn.BENHNHAN_MA = hsba.BENHNHAN_MA
     LEFT JOIN dm_khoa k ON hsba.hsba_khoarav = k.dmkhoa_maso	
     INNER JOIN hsba_chuyen_vien cv ON hsba.hsba_sovaovien = cv.hsba_sovaovien     
     INNER JOIN dm_benh_vien bv ON cv.hsbacv_chvienden = bv.dmbenhvien_maso
WHERE
     (TO_DATE(hsba.HSBA_NGAYGIORAV) >= $P{TUNGAY} AND TO_DATE(hsba.HSBA_NGAYGIORAV) <= $P{DENNGAY})
ORDER BY
     KHOA ASC, NGAYGIO ASC]]></queryString>

