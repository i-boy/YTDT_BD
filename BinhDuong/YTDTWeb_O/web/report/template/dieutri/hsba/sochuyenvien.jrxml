	<queryString><![CDATA[select BENHNHAN_HOTEN 
,decode(gt.DMGT_MASO , 1 , null , (bn.BENHNHAN_TUOI||decode(bn.BENHNHAN_DONVITUOI,2,'Th',decode(bn.BENHNHAN_DONVITUOI,3,'Ng','')))) as Nam
,decode(gt.DMGT_MASO , 2 , null , (bn.BENHNHAN_TUOI||decode(bn.BENHNHAN_DONVITUOI,2,'Th',decode(bn.BENHNHAN_DONVITUOI,3,'Ng','')))) as Nu 
,decode(dt.DMDOITUONG_MA , 'BH' , 'x' , null ) as BHYT 
,(case when bn.BENHNHAN_TUOI < 12 and BENHNHAN_DONVITUOI = 2 then 'x' else null end) as TREEM12T 
,(case when ((bn.BENHNHAN_TUOI <= 15 and bn.BENHNHAN_TUOI >= 1 and BENHNHAN_DONVITUOI = 1) or (bn.BENHNHAN_TUOI >= 12 and BENHNHAN_DONVITUOI = 2)) then 'x' else null end) as TREEM1_15T 
,nn.DMNGHENGHIEP_TEN
,bn.BENHNHAN_DIACHI DIACHI
,xa.DMXA_TEN TENXA
,huyen.DMHUYEN_TEN TENHUYEN
,tinh.DMTINH_TEN TENTINH
,bv.DMBENHVIEN_TEN
,decode(hs.HSBA_TUVVONG24G , 1 , 'x' , null ) as TV24G 
,decode(hs.HSBA_KETQUA , 1 , 'x' , null) as KQDT_KHOI
,decode(hs.HSBA_KETQUA , 2 , 'x' , null) as KQDT_DO
,decode(hs.HSBA_KETQUA , 4 , 'x' , null) as KQDT_NANGHON
,decode(hs.HSBA_KETQUA , 3 , 'x' , null) as KQDT_KHONGKHOI 
,decode(hs.HSBA_NGAYGIORAV,null,(TO_DATE(CURRENT_DATE)+1-hs.HSBA_NGAYGIOVAOV) ,(to_date(hs.HSBA_NGAYGIORAV)+1-hs.HSBA_NGAYGIOVAOV)) as TS ,
hs.HSBA_NGAYGIORAV,
hs.HSBA_NGAYGIOVAOV,
hs.HSBA_NGAYGIOTV,
hscv.HSBACV_NGAGIOCHVIEN,
(cdtt.DMBENHICD_MA|| '-' ||cdtt.DMBENHICD_TEN) as cdtd,
(cdbd.DMBENHICD_MA|| '-' ||cdbd.DMBENHICD_TEN) as cdbd,
(cddt.DMBENHICD_MA|| '-' ||cddt.DMBENHICD_TEN) as cddt
from ((((((((((HSBA_CHUYEN_VIEN hscv 
left join HSBA hs on (hscv.HSBA_SOVAOVIEN = hs.HSBA_SOVAOVIEN)
left join DM_BENH_ICD cdtt on hs.HSBA_MACHDOANTUYENT = cdtt.DMBENHICD_MASO
left join DM_BENH_ICD cdbd on hs.HSBA_MACHDOANBD = cdbd.DMBENHICD_MASO
left join HSBA_CHUYEN_MON cm on hs.HSBA_SOVAOVIEN = cm.HSBA_SOVAOVIEN 
left join DM_BENH_ICD cddt on cm.HSBACM_BENHCHINH = cddt.DMBENHICD_MASO
)  
left join BENH_NHAN bn on (hs.BENHNHAN_MA = bn.BENHNHAN_MA)) 
left join DM_GIOI_TINH gt on (bn.DMGT_MASO = gt.DMGT_MASO)) 
left join DM_DOI_TUONG dt on (hs.DOITUONG_MA = dt.DMDOITUONG_MASO)) 
left join DM_NGHE_NGHIEP nn on (bn.BENHNHAN_NGHE = nn.DMNGHENGHIEP_MASO)) 
left join DM_TINH tinh on (bn.TINH_MA = tinh.DMTINH_MASO)) 
left join DM_HUYEN huyen on (bn.HUYEN_MA = huyen.DMHUYEN_MASO)) 
left join DM_XA xa on (bn.XA_MA = xa.DMXA_MASO)) 
left join DM_BENH_VIEN bv on (hs.HSBA_DONVIGOI = bv.DMBENHVIEN_MASO)) 
left join DM_KET_QUA_DIEU_TRI kq on (hs.HSBA_KETQUA = kq.DMKQDT_MASO)) 

where ( $P{TUNGAYDATE} is null or (to_DATE(hscv.HSBACV_NGAGIOCHVIEN)-to_date($P{TUNGAYDATE}) ) >= 0 ) 
and  ( $P{DENNGAYDATE} is null or (to_date($P{DENNGAYDATE}) -to_DATE(hscv.HSBACV_NGAGIOCHVIEN)) >=0 )
and ($P{KHOAMASO} is null or hs.HSBA_KHOADANGDT =$P{KHOAMASO}) 
and ($P{BENHMASO} is null or hs.HSBA_MACHDOANBD = $P{BENHMASO}) 
and ($P{BVMASO} is null or hscv.HSBACV_CHVIENDEN = $P{BVMASO}) 
and ($P{TAINANMASO} is null or hs.TAINAN_MA = $P{TAINANMASO})]]></queryString>

