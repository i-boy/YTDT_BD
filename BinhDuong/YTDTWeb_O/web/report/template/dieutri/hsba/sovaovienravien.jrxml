	<queryString><![CDATA[select BENHNHAN_HOTEN,hs.HSBA_SOVAOVIEN
,decode(gt.DMGT_MASO , 1 , null , (bn.BENHNHAN_TUOI||decode(bn.BENHNHAN_DONVITUOI,2,'Th',decode(bn.BENHNHAN_DONVITUOI,3,'Ng','')))) as Nam
,decode(gt.DMGT_MASO , 2 , null , (bn.BENHNHAN_TUOI||decode(bn.BENHNHAN_DONVITUOI,2,'Th',decode(bn.BENHNHAN_DONVITUOI,3,'Ng','')))) as Nu
,decode(dt.DMDOITUONG_MA , 'BH' , 'x' , null ) as BHYT 
,(case when (bn.BENHNHAN_TUOI < 12 and BENHNHAN_DONVITUOI = 2) or (bn.BENHNHAN_TUOI < 365 and BENHNHAN_DONVITUOI = 3) then 'x' else null end) as TREEM12T 
,(case when ((bn.BENHNHAN_TUOI <= 15 and bn.BENHNHAN_TUOI >= 1 and BENHNHAN_DONVITUOI = 1) or (bn.BENHNHAN_TUOI >= 12 and BENHNHAN_DONVITUOI = 2)) then 'x' else null end) as TREEM1_15T 
,nn.DMNGHENGHIEP_TEN
,(decode(bn.BENHNHAN_DIACHI ,null , '' ,(bn.BENHNHAN_DIACHI||',') )||decode(bn.XA_MA , null ,'' , (xa.DMXA_TEN||','))||decode(bn.HUYEN_MA , null , '',(huyen.DMHUYEN_TEN||','))||decode(bn.TINH_MA , null,'',tinh.DMTINH_TEN)) as DIACHI 
,bv.DMBENHVIEN_TEN
,decode(hs.HSBA_TUVVONG24G , 1 , 'x' , null ) as TV24G 
,decode(hs.HSBA_KETQUA , 1 , 'x' , null) as KQDT_KHOI
,decode(hs.HSBA_KETQUA , 2 , 'x' , null) as KQDT_DO
,decode(hs.HSBA_KETQUA , 4 , 'x' , null) as KQDT_NANGHON
,decode(hs.HSBA_KETQUA , 3 , 'x' , null) as KQDT_KHONGKHOI 
,decode(trim(hs.HSBA_NGAYGIORAV) , null , to_date(CURRENT_DATE)+1 -to_date(hs.HSBA_NGAYGIOVAOV) ,to_date(hs.HSBA_NGAYGIORAV)+1-to_date(hs.HSBA_NGAYGIOVAOV)) as TS 
,decode(trim(hs.HSBA_MACHDOANTUYENT) , null, '', (icd1.DMBENHICD_MA||'-'||icd1.DMBENHICD_TEN) ) as ICD_TUYENTRUOC
,decode(trim(hs.HSBA_MACHDOANBD) , null, '', (icd2.DMBENHICD_MA||'-'||icd2.DMBENHICD_TEN) ) as ICD_BANDAU
,decode(trim(cm.HSBACM_BENHCHINH) , null, '', (icd3.DMBENHICD_MA||'-'||icd3.DMBENHICD_TEN) ) as ICD_DIEUTRI
,decode(hs.HSBA_DIEUTRI,4,HSBA_NGAYGIORAV,null) CHUYENVIEN
,HSBA_NGAYGIOVAOV, HSBA_NGAYGIORAV, HSBA_NGAYGIOTV

from HSBA hs
inner join BENH_NHAN bn on (hs.BENHNHAN_MA = bn.BENHNHAN_MA)
left join hsba_chuyen_mon cm on hs.HSBA_SOVAOVIEN = cm.HSBA_SOVAOVIEN
left join DM_GIOI_TINH gt on (bn.DMGT_MASO = gt.DMGT_MASO)
left join DM_DOI_TUONG dt on (hs.DOITUONG_MA = dt.DMDOITUONG_MASO)
left join DM_NGHE_NGHIEP nn on (bn.BENHNHAN_NGHE = nn.DMNGHENGHIEP_MASO)
left join DM_TINH tinh on (bn.TINH_MA = tinh.DMTINH_MASO)
left join DM_HUYEN huyen on (bn.HUYEN_MA = huyen.DMHUYEN_MASO)
left join DM_XA xa on (bn.XA_MA = xa.DMXA_MASO)
left join DM_BENH_VIEN bv on (hs.HSBA_DONVIGOI = bv.DMBENHVIEN_MASO)
left join DM_KET_QUA_DIEU_TRI kq on (hs.HSBA_KETQUA = kq.DMKQDT_MASO)
left join DM_BENH_ICD icd1 on (hs.HSBA_MACHDOANTUYENT = icd1.DMBENHICD_MASO)
left join DM_BENH_ICD icd2 on (hs.HSBA_MACHDOANBD = icd2.DMBENHICD_MASO)
left join DM_BENH_ICD icd3 on (cm.HSBACM_BENHCHINH = icd3.DMBENHICD_MASO)

where (hs.HSBA_KHOADANGDT_CM = cm.KHOA_MA or  hs.HSBA_KHOADANGDT = cm.KHOA_MA)
and ($P{SOBATU} is null or $P{SOBATU} ='' or hs.HSBA_SOVAOVIEN >= $P{SOBATU}) 
and ($P{SOBADEN} is null or $P{SOBADEN} ='' or hs.HSBA_SOVAOVIEN <= $P{SOBADEN})
order by hs.HSBA_SOVAOVIEN]]></queryString>

