	<queryString><![CDATA[SELECT sum(tmp.khangsinh) as khangsinh, sum(tmp.vitamin) as vitamin, sum(tmp.thuocme) thuocme, sum(tmp.yhct) as yhct, sum(tmp.cls) as cls,sum(tmp.corti) as corti ,
sum(tmp.dungtich)  as dungtich ,sum(tmp.thuocdam)  as thuocdam, sum(tmp.trongnuoc)  as trongnuoc,sum(tmp.ngoainuoc)  as ngoainuoc
FROM(
-- thuoc phong kham
SELECT sum(tmp_phanloai.khangsinh) as khangsinh, sum(tmp_phanloai.vitamin) as vitamin, sum(tmp_phanloai.thuocme) thuocme, 0 as yhct, 0 as cls,0 as corti ,
0  as dungtich ,0  as thuocdam, 0  as trongnuoc,0  as ngoainuoc
FROM(
SELECT case when phanloai.dmphanloaithuoc_maso = 6 then sum(tpk.thuocphongkham_thanhtien) else 0 end  as khangsinh ,
case when phanloai.dmphanloaithuoc_maso = 27 then sum(tpk.thuocphongkham_thanhtien) else 0 end  as vitamin, 
case when phanloai.dmphanloaithuoc_maso = 1 then sum(tpk.thuocphongkham_thanhtien) else 0 end  as thuocme
FROM thuoc_phong_kham tpk
inner join dm_thuoc thuoc on tpk.thuocphongkham_mathuoc = thuoc.dmthuoc_maso
inner join dm_phan_loai_thuoc phanloai on thuoc.dmphanloaithuoc_maso = phanloai.dmphanloaithuoc_maso
where (phanloai.dmphanloaithuoc_maso = 6 or phanloai.dmphanloaithuoc_maso = 27 or phanloai.dmphanloaithuoc_maso = 1)
and (to_date(tpk.thuocphongkham_ngaygio) between $P{TuNgay} and $P{DenNgay})
group by phanloai.dmphanloaithuoc_maso 
)tmp_phanloai

union all
SELECT 0 as khangsinh, 0 as vitamin, 0 as ,sum(tmp_loaithuoc.yhct) as yhct, sum(tmp_loaithuoc.cls) as cls,0 as corti,
0  as dungtich ,0  as thuocdam, 0  as trongnuoc,0  as ngoainuoc
FROM(
SELECT case when loaithuoc.dmloaithuoc_maso = 1 then sum(tpk.thuocphongkham_thanhtien) else 0 end  as yhct ,
case when loaithuoc.dmloaithuoc_maso = 2 then sum(tpk.thuocphongkham_thanhtien) else 0 end  as cls 
FROM thuoc_phong_kham tpk
inner join dm_thuoc thuoc on tpk.thuocphongkham_mathuoc = thuoc.dmthuoc_maso
inner join dm_phan_loai_thuoc phanloai on thuoc.dmphanloaithuoc_maso = phanloai.dmphanloaithuoc_maso
inner join dm_loai_thuoc loaithuoc on phanloai.dmloaithuoc_maso = loaithuoc.dmloaithuoc_maso
where (loaithuoc.dmloaithuoc_maso = 1 or loaithuoc.dmloaithuoc_maso = 2)
and (to_date(tpk.thuocphongkham_ngaygio) between $P{TuNgay} and $P{DenNgay})
group by loaithuoc.dmloaithuoc_maso
)tmp_loaithuoc

union all
SELECT 0 as khangsinh, 0 as vitamin, 0 as ,0 as yhct, 0 as cls,0 as corti, 
sum(tmp_dungtich.dungtich) as dungtich , sum(tmp_dungtich.thuocdam) as thuocdam, 
sum(tmp_dungtich.trongnuoc) as trongnuoc, sum(tmp_dungtich.ngoainuoc) as ngoainuoc       
FROM(
SELECT 
sum(tpk.thuocphongkham_thanhtien) as dungtich ,
case when thuoc.dmthuoc_dam = 1 then sum(tpk.thuocphongkham_thanhtien) else 0 end as thuocdam,
case when quocgia.dmquocgia_ma = 'VNA' then sum(tpk.thuocphongkham_thanhtien) else 0 end as trongnuoc,
case when quocgia.dmquocgia_ma <> 'VNA' then sum(tpk.thuocphongkham_thanhtien) else 0 end as ngoainuoc
FROM thuoc_phong_kham tpk
inner join dm_thuoc thuoc on tpk.thuocphongkham_mathuoc = thuoc.dmthuoc_maso
inner join dm_phan_loai_thuoc phanloai on thuoc.dmphanloaithuoc_maso = phanloai.dmphanloaithuoc_maso
inner join dm_loai_thuoc loaithuoc on phanloai.dmloaithuoc_maso = loaithuoc.dmloaithuoc_maso
inner join dm_quoc_gia quocgia on thuoc.dmthuoc_nuocdefa = quocgia.dmquocgia_maso 
where loaithuoc.dmloaithuoc_maso = 3  and thuoc.dmthuoc_dungtich is not null
and (to_date(tpk.thuocphongkham_ngaygio) between $P{TuNgay} and $P{DenNgay})
group by thuoc.dmthuoc_dam,quocgia.dmquocgia_ma
)tmp_dungtich

union all
SELECT 0 as khangsinh, 0 as vitamin , 0 as thuocme, 0 as yhct, 0 as cls, sum(tpk.thuocphongkham_thanhtien) as corti
,0  as dungtich ,0  as thuocdam, 0  as trongnuoc,0  as ngoainuoc
FROM thuoc_phong_kham tpk
inner join dm_thuoc thuoc on tpk.thuocphongkham_mathuoc = thuoc.dmthuoc_maso
where thuoc.dmthuoc_corti = 1 and (to_date(tpk.thuocphongkham_ngaygio) between $P{TuNgay} and $P{DenNgay})


-- thuoc noi tru
union all
SELECT sum(tmp_phanloai.khangsinh) as khangsinh, sum(tmp_phanloai.vitamin) as vitamin, sum(tmp_phanloai.thuocme) thuocme, 0 as yhct, 0 as cls,0 as corti ,
0  as dungtich ,0  as thuocdam, 0  as trongnuoc,0  as ngoainuoc
FROM(
SELECT case when phanloai.dmphanloaithuoc_maso = 6 then sum(tnt.thuocnoitru_thanhtien) else 0 end  as khangsinh ,
case when phanloai.dmphanloaithuoc_maso = 27 then sum(tnt.thuocnoitru_thanhtien) else 0 end  as vitamin, 
