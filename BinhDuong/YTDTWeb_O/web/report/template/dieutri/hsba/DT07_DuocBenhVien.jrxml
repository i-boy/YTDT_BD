union all
SELECT 0 as khangsinh, 0 as vitamin, 0 as ,sum(tmp_loaithuoc.yhct) as yhct, sum(tmp_loaithuoc.cls) as cls,0 as corti,
0  as dungtich ,0  as thuocdam, 0  as trongnuoc,0  as ngoainuoc
FROM(
SELECT case when loaithuoc.dmloaithuoc_maso = 1 then sum(tnt.thuocnoitru_thanhtien) else 0 end  as yhct ,
case when loaithuoc.dmloaithuoc_maso = 2 then sum(tnt.thuocnoitru_thanhtien) else 0 end  as cls 
