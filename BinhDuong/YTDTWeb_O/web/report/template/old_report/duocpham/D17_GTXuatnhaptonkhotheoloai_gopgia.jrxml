	<queryString><![CDATA[SELECT GOPGIA.DMTHUOC_MASO, GOPGIA.TEN_HANG, GOPGIA.DMDONVITINH_TEN, GOPGIA.TENPHANLOAI AS TENPHANLOAI, 0 AS DONGIA,
	SUM(GOPGIA.SL_TONDAU) AS SL_TONDAU, SUM(TT_TONDAU) AS TT_TONDAU,
	SUM(GOPGIA.SL_NHAP) AS SL_NHAP, SUM(GOPGIA.TT_NHAP) AS TT_NHAP,
	SUM(GOPGIA.SL_TRA) AS SL_TRA, SUM(GOPGIA.TT_TRA) AS TT_TRA,
	SUM(GOPGIA.SL_XUAT) AS SL_XUAT, SUM(GOPGIA.TT_XUAT) AS TT_XUAT,
	0 AS SL_THANHLY, 0 AS TT_THANHLY,
	SUM(GOPGIA.SL_TONCUOI) AS SL_TONCUOI, SUM(GOPGIA.TT_TONCUOI) AS TT_TONCUOI
FROM 
(
SELECT TK.DMTHUOC_MASO, CONCAT(TK.DMTHUOC_TEN , " " , TK.DMNCT_MA , " " , TK.DMQUOCGIA_MA) AS TEN_HANG,
      TK.DMPHANLOAITHUOC_TEN AS TENPHANLOAI,
      TK.DMDONVITINH_TEN, TK.DMPHANLOAITHUOC_MA AS DMPHANLOAITHUOC_MA, TK.TONKHO_DONGIA AS DONGIA,
      SUM(TK.TONDAU) AS SL_TONDAU, SUM(TK.TONDAU) * TK.TONKHO_DONGIA AS TT_TONDAU,
      SUM(TK.NHAP) AS SL_NHAP, SUM(TK.NHAP) * TK.TONKHO_DONGIA AS TT_NHAP,
      SUM(TK.TRA) AS SL_TRA, SUM(TK.TRA) * TK.TONKHO_DONGIA AS TT_TRA,
      SUM(TK.XUAT)  AS SL_XUAT, SUM(TK.XUAT) * TK.TONKHO_DONGIA AS TT_XUAT,
      0 AS SL_THANHLY, 0 AS TT_THANHLY ,
      (SUM(TK.TONDAU) + SUM(TK.NHAP) + SUM(TK.TRA) - SUM(TK.XUAT)) AS SL_TONCUOI,
      (SUM(TK.TONDAU) + SUM(TK.NHAP) + SUM(TK.TRA) - SUM(TK.XUAT)) * TK.TONKHO_DONGIA AS TT_TONCUOI
FROM
(
  SELECT TK.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, PLTHUOC.DMPHANLOAITHUOC_TEN,
       DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA,
       0 TONDAU, SUM(TK.TONKHO_NHAP) NHAP,
       SUM(TK.TONKHO_TRA) TRA, SUM(TK.TONKHO_XUAT) XUAT, TK.TONKHO_DONGIA,
       PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, TK.DMNCT_MASO, TK.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
	FROM TON_KHO TK INNER JOIN DM_THUOC THUOC ON TK.DMTHUOC_MASO = THUOC.DMTHUOC_MASO
                  INNER JOIN DM_DON_VI_TINH DVT ON THUOC.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
                  INNER JOIN DM_NGUON_KINH_PHI KP ON TK.DMNGUONKINHPHI_MASO = KP.DMNGUONKINHPHI_MASO
                  INNER JOIN DM_NGUON_CHUONG_TRINH NCT ON TK.DMNCT_MASO = NCT.DMNCT_MASO
                  INNER JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON THUOC.DMPHANLOAITHUOC_MASO = PLTHUOC.DMPHANLOAITHUOC_MASO
	                INNER JOIN DM_LOAI_THUOC LTHUOC ON PLTHUOC.DMLOAITHUOC_MASO = LTHUOC.DMLOAITHUOC_MASO
                  INNER JOIN DM_QUOC_GIA QG ON TK.DMQUOCGIA_MASO = QG.DMQUOCGIA_MASO
                  INNER JOIN DM_KHOA KHOA ON TK.DMKHOA_MASO = KHOA.DMKHOA_MASO
  WHERE (($P{DMKHOA_MASO} IS NOT NULL) OR KHOA.DMKHOA_MA like 'K%') AND (($P{DMKHOA_MASO} IS NULL) OR TK.DMKHOA_MASO = $P{DMKHOA_MASO})
  AND ($P{TUNGAY} IS NULL or DATEDIFF(DATE(TONKHO_NGAYGIOCN),DATE($P{TUNGAY})) >=0)
  AND ($P{DENNGAY} IS NULL or DATEDIFF(DATE(TONKHO_NGAYGIOCN),DATE($P{DENNGAY})) <=0)
  GROUP BY TK.DMTHUOC_MASO, TK.TONKHO_DONGIA, PLTHUOC.DMPHANLOAITHUOC_MA, TK.DMNCT_MASO

  union

  SELECT TKDAU.DMTHUOC_MASO, TKDAU.DMTHUOC_TEN, TKDAU.DMPHANLOAITHUOC_TEN,
      TKDAU.DMDONVITINH_TEN, TKDAU.DMPHANLOAITHUOC_MA, TKDAU.DMNCT_MA,
      TKDAU.TONDAU, TKDAU.NHAP, TKDAU.TRA, TKDAU.XUAT, TKDAU.TONKHO_DONGIA,
      TKDAU.DMLOAITHUOC_MASO, TKDAU.DMPHANLOAITHUOC_MASO, TKDAU.DMPHANNHOMTHUOC_MASO, TKDAU.DMNCT_MASO, TKDAU.DMNGUONKINHPHI_MASO, TKDAU.DMQUOCGIA_MA
  FROM
  (
	  SELECT TK.DMTHUOC_MASO, THUOC.DMTHUOC_TEN, PLTHUOC.DMPHANLOAITHUOC_TEN,
      		DVT.DMDONVITINH_TEN, PLTHUOC.DMPHANLOAITHUOC_MA, NCT.DMNCT_MA,
      		SUM(TK.TONKHO_TON) as TONDAU, 0 NHAP, 0 TRA, 0 XUAT, TK.TONKHO_DONGIA,
      		PLTHUOC.DMLOAITHUOC_MASO, PLTHUOC.DMPHANLOAITHUOC_MASO, THUOC.DMPHANNHOMTHUOC_MASO, TK.DMNCT_MASO, TK.DMNGUONKINHPHI_MASO, QG.DMQUOCGIA_MA
	  FROM TON_KHO TK INNER JOIN (
                                  SELECT MAX(TK.TONKHO_MA) AS TONKHO_MA FROM TON_KHO TK INNER JOIN DM_KHOA KHOA ON TK.DMKHOA_MASO = KHOA.DMKHOA_MASO
                              		WHERE (($P{DMKHOA_MASO} IS NOT NULL) OR KHOA.DMKHOA_MA like 'K%') AND (($P{DMKHOA_MASO} IS NULL) OR TK.DMKHOA_MASO = $P{DMKHOA_MASO})
                                  AND ($P{TUNGAY} IS NULL or DATEDIFF(DATE(TONKHO_NGAYGIOCN),DATE($P{TUNGAY})) <0)
                              		GROUP BY TK.TONKHO_MALIENKET, TK.DMKHOA_MASO
                              		ORDER BY TK.TONKHO_MA
                              ) AS T1 ON TK.TONKHO_MA = T1.TONKHO_MA
	  INNER JOIN DM_THUOC THUOC ON TK.DMTHUOC_MASO = THUOC.DMTHUOC_MASO
	  INNER JOIN DM_DON_VI_TINH DVT ON THUOC.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
	  INNER JOIN DM_NGUON_KINH_PHI KP ON TK.DMNGUONKINHPHI_MASO = KP.DMNGUONKINHPHI_MASO
	  INNER JOIN DM_KHOA KHOA ON TK.DMKHOA_MASO = KHOA.DMKHOA_MASO
	  INNER JOIN DM_NGUON_CHUONG_TRINH NCT ON TK.DMNCT_MASO = NCT.DMNCT_MASO
	  INNER JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON THUOC.DMPHANLOAITHUOC_MASO = PLTHUOC.DMPHANLOAITHUOC_MASO
	  INNER JOIN DM_LOAI_THUOC LTHUOC ON PLTHUOC.DMLOAITHUOC_MASO = LTHUOC.DMLOAITHUOC_MASO
    	  INNER JOIN DM_QUOC_GIA QG ON TK.DMQUOCGIA_MASO = QG.DMQUOCGIA_MASO
	  WHERE (($P{DMKHOA_MASO} IS NOT NULL) OR KHOA.DMKHOA_MA like 'K%') AND (($P{DMKHOA_MASO} IS NULL) OR TK.DMKHOA_MASO = $P{DMKHOA_MASO})
    	  AND (TK.TONKHO_TON > 0)
	  AND ($P{TUNGAY} IS NULL or DATEDIFF(DATE(TK.TONKHO_NGAYGIOCN),DATE($P{TUNGAY})) <0)
    	  GROUP BY TK.DMTHUOC_MASO, TK.TONKHO_DONGIA, PLTHUOC.DMPHANLOAITHUOC_MA, TK.DMNCT_MASO
	  ORDER BY TK.TONKHO_MA
  ) as TKDAU
) AS TK
WHERE ($P{DMLOAITHUOC_MASO} IS NULL OR TK.DMLOAITHUOC_MASO = $P{DMLOAITHUOC_MASO})
AND ($P{DMPHANLOAI_MA} IS NULL OR TK.DMPHANLOAITHUOC_MA IN ($P!{DMPHANLOAI_MA}))
AND ($P{NHOMTHUOCMASO} IS NULL or TK.DMPHANNHOMTHUOC_MASO = $P{NHOMTHUOCMASO})
AND ($P{DMNCT_MASO} IS NULL OR TK.DMNCT_MASO = $P{DMNCT_MASO})
AND ($P{DMNGUONKINHPHI_MASO} IS NULL OR TK.DMNGUONKINHPHI_MASO = $P{DMNGUONKINHPHI_MASO})
AND ($P{DMTHUOC_TEN} IS NULL OR UPPER(TK.DMTHUOC_TEN) LIKE CONCAT(CONCAT("%",$P{DMTHUOC_TEN}),"%"))
GROUP BY TK.DMTHUOC_MASO, TK.TONKHO_DONGIA, TK.DMPHANLOAITHUOC_MA, TK.DMNCT_MA
order by TK.DMPHANLOAITHUOC_TEN, TK.DMTHUOC_TEN
) AS GOPGIA
GROUP BY GOPGIA.DMTHUOC_MASO
ORDER BY GOPGIA.TENPHANLOAI, GOPGIA.TEN_HANG]]></queryString>

