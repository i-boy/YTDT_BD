	<parameter name="DIENTHOAI" isForPrompting="false" class="java.lang.String"/>
	<parameter name="TINH" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT  T.DMTHUOC_MASO, CONCAT(T.DMTHUOC_TEN , " " , NCT.DMNCT_MA , " " , QG.DMQUOCGIA_MA) AS TEN_HANG, 
PLT.DMPHANLOAITHUOC_MA AS DMPHANLOAITHUOC_MA ,
 DMDONVITINH_TEN , TONKHO_DONGIA , TK.TONDAU AS SL_TONDAU, SUM(TK.TONKHO_DONGIA * TK.TONDAU)AS TT_TONDAU ,'' AS DONGIA, PLT.DMPHANLOAITHUOC_TEN AS TENPHANLOAI,
 
 SUM(TK.NHAP) AS SL_NHAP,SUM(TK.NHAP * TK.TONKHO_DONGIA) AS TT_NHAP,
 SUM(TK.TRA) AS SL_TRA,SUM(TK.TRA * TK.TONKHO_DONGIA) AS TT_TRA,
 SUM(TK.XUAT) AS SL_XUAT, SUM(TK.XUAT * TK.TONKHO_DONGIA) AS TT_XUAT,
0 AS SL_THANHLY, 0 AS TT_THANHLY ,
 (TK.TONDAU + SUM(TK.NHAP) + SUM(TK.TRA) - SUM(TK.XUAT) ) AS SL_TONCUOI , 
 (SUM(TK.TONKHO_DONGIA * TK.TONDAU) + SUM(TK.NHAP * TK.TONKHO_DONGIA) + SUM(TK.TRA * TK.TONKHO_DONGIA) - SUM(TK.XUAT * TK.TONKHO_DONGIA)  ) AS TT_TONCUOI 
FROM
	(
	SELECT MA, T, NCT, SUM(TONDAU) TONDAU, 0 NHAP, 0 TRA, 0 XUAT, DMQUOCGIA_MASO, TONKHO_DONGIA FROM
	(
	SELECT (TK.TONKHO_MA) MA, TK.DMTHUOC_MASO T, TK.DMNCT_MASO NCT, (TK.TONKHO_TON) TONDAU, 0 NHAP, 0 TRA, 0 XUAT, T.DMTHUOC_TEN,T.DMTHUOC_HAMLUONG, T.DMDONVITINH_MASO, PLT.DMPHANLOAITHUOC_MA, TK.DMQUOCGIA_MASO, TK.TONKHO_DONGIA
	FROM TON_KHO TK INNER JOIN DM_THUOC T ON TK.DMTHUOC_MASO = T.DMTHUOC_MASO
		INNER JOIN DM_PHAN_LOAI_THUOC PLT ON T.DMPHANLOAITHUOC_MASO = PLT.DMPHANLOAITHUOC_MASO			
	WHERE ($P{DENNGAY} IS NULL OR DATEDIFF(DATE(TK.TONKHO_NGAYGIOCN),DATE($P{TUNGAY}))  < 0 )   
		AND ($P{DMKHOA_MASO} IS NULL OR TK.DMKHOA_MASO = $P{DMKHOA_MASO}) 			
		AND ($P{DMLOAITHUOC_MASO} IS NULL OR PLT.DMLOAITHUOC_MASO = $P{DMLOAITHUOC_MASO}) 		
		AND ($P{DMPHANLOAI_MA} IS NULL OR PLT.DMPHANLOAITHUOC_MA IN ($P!{DMPHANLOAI_MA}))  
		AND ($P{DMNCT_MASO} IS NULL OR TK.DMNCT_MASO = $P{DMNCT_MASO}) 
		AND ($P{DMNGUONKINHPHI_MASO} IS NULL OR TK.DMNGUONKINHPHI_MASO = $P{DMNGUONKINHPHI_MASO}) 	
	ORDER BY TK.TONKHO_MA DESC
	) TEMP
	GROUP BY DMTHUOC_TEN, DMTHUOC_HAMLUONG, DMDONVITINH_MASO, DMPHANLOAITHUOC_MA, NCT, TONKHO_DONGIA	
	UNION
	SELECT TK.TONKHO_MA MA, TK.DMTHUOC_MASO T, TK.DMNCT_MASO NCT, 0 TONDAU, SUM(TK.TONKHO_NHAP) NHAP, 
		SUM(TK.TONKHO_TRA) TRA, SUM(TK.TONKHO_XUAT) XUAT, TK.DMQUOCGIA_MASO, TK.TONKHO_DONGIA
	FROM TON_KHO TK INNER JOIN DM_THUOC T ON TK.DMTHUOC_MASO = T.DMTHUOC_MASO
		INNER JOIN DM_PHAN_LOAI_THUOC PLT ON T.DMPHANLOAITHUOC_MASO = PLT.DMPHANLOAITHUOC_MASO	
	WHERE ($P{TUNGAY} IS NULL OR DATEDIFF(DATE(TONKHO_NGAYGIOCN),DATE($P{TUNGAY})) >=0) 
		AND ($P{DENNGAY} IS NULL OR DATEDIFF(DATE(TONKHO_NGAYGIOCN),DATE($P{DENNGAY})) <=0)  
		AND ($P{DMKHOA_MASO} IS NULL OR TK.DMKHOA_MASO = $P{DMKHOA_MASO}) 			
		AND ($P{DMLOAITHUOC_MASO} IS NULL OR PLT.DMLOAITHUOC_MASO = $P{DMLOAITHUOC_MASO}) 
		AND ($P{DMPHANLOAI_MA} IS NULL OR PLT.DMPHANLOAITHUOC_MA IN ($P!{DMPHANLOAI_MA}))  
		AND ($P{DMNCT_MASO} IS NULL OR TK.DMNCT_MASO = $P{DMNCT_MASO}) 	
	GROUP BY T.DMTHUOC_TEN, T.DMTHUOC_HAMLUONG, T.DMDONVITINH_MASO, PLT.DMPHANLOAITHUOC_MA, TK.DMNCT_MASO, TONKHO_DONGIA
	ORDER BY MA	ASC) TK
	INNER JOIN DM_THUOC T ON TK.T = T.DMTHUOC_MASO
	INNER JOIN DM_PHAN_LOAI_THUOC PLT ON T.DMPHANLOAITHUOC_MASO = PLT.DMPHANLOAITHUOC_MASO	
	INNER JOIN DM_DON_VI_TINH DVT ON T.DMDONVITINH_MASO = DVT.DMDONVITINH_MASO
	INNER JOIN DM_NGUON_CHUONG_TRINH NCT ON TK.NCT = NCT.DMNCT_MASO 
	INNER JOIN DM_QUOC_GIA QG ON TK.DMQUOCGIA_MASO = QG.DMQUOCGIA_MASO 
GROUP BY TEN_HANG, DMDONVITINH_TEN, DONGIA]]></queryString>

