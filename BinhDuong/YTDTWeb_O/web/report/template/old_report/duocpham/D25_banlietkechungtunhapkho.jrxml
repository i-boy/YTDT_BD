	<queryString><![CDATA[SELECT pnk.PHIEUNHAPKHO_MA ,pnk.PHIEUNHAPKHO_NGAYGIOCN , ncc.DMNHACUNGCAP_TEN, pnk.PHIEUNHAPKHO_NGAYHOADON,pnk.PHIEUNHAPKHO_SOHOADON , pnk.PHIEUNHAPKHO_THANHTIEN , pnk.PHIEUNHAPKHO_MUCTHUE ,  (pnk.PHIEUNHAPKHO_THANHTIEN + ((pnk.PHIEUNHAPKHO_THANHTIEN * pnk.PHIEUNHAPKHO_MUCTHUE)/100)) as THANHTIEN_VAT ,nkp.DMNGUONKINHPHI_MA, pnk.PHIEUNHAPKHO_NGAYGIOCN as NGAYGHI 
FROM PHIEU_NHAP_KHO pnk 
left join DM_NGUON_KINH_PHI nkp on(pnk.DMNGUONKINHPHI_MASO = nkp.DMNGUONKINHPHI_MASO) 
left join DM_NHA_CUNG_CAP ncc on (pnk.DTDMNOIBAN_MA = ncc.DMNHACUNGCAP_MASO) 
where ($P{TUNGAY} is null or((DATE(pnk.PHIEUNHAPKHO_NGAYGIOCN) >= DATE($P{TUNGAY})) and 
		(DATE(pnk.PHIEUNHAPKHO_NGAYGIOCN) <= DATE($P{DENNGAY})) ) ) 
  and ($P{LOAITHUOCMASO} is null or pnk.DMLOAITHUOC_MASO = $P{LOAITHUOCMASO} )  
  and ($P{KHOMASO} is null or pnk.DTDMKHO_MASO = $P{KHOMASO}  )]]></queryString>

