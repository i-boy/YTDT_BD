	<parameter name="TuNgay" isForPrompting="false" class="java.util.Date"/>
	<parameter name="DenNgay" isForPrompting="false" class="java.util.Date"/>
	<parameter name="TenPLDoiTuong" isForPrompting="false" class="java.lang.String"/>
	<parameter name="MaPLDoiTuong" isForPrompting="false" class="java.lang.String"/>
	<parameter name="khoi" isForPrompting="false" class="java.lang.String"/>
	<parameter name="chon" isForPrompting="false" class="java.lang.String"/>
	<parameter name="MaKCBBHYT" isForPrompting="false" class="java.lang.String"/>
	<parameter name="TenKCBBHYT" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT BN.BENHNHAN_HOTEN HOTEN, BN.BENHNHAN_NGAYSINH NGAYSINH,GIOI.DMGT_TEN GIOITINH,
BN.BENHNHAN_TUOI TUOI,  KCBBHYT.DMBENHVIEN_TEN NOIKCBBDTEN,
					IF(BN.BENHNHAN_DONVITUOI=2,'T',IF(BN.BENHNHAN_DONVITUOI=3, 'N','')) DONVITUOI ,
	HSBABHYT.HSBABHYT_SOTHEBH SOTHEBHYT, KCBBHYT.DMBENHVIEN_MA NOIKCBBD,
if ( HSBA1.HSBA_SOVAOVIEN is null  ,''  , SUBSTRING(HSBA1.HSBA_SOVAOVIEN,5))
 SOVAOVIEN,

	HSBA1.HSBA_NGAYGIOVAOV NGAYGIOVAOV,HSBA1.HSBA_NGAYGIORAV NGAYGIORAV, BENH.DMBENHICD_MA BENH,
	
	(  IF ( HSBA1.HSBA_NGAYGIORAV IS NOT NULL AND HSBA_NGAYGIOVAOV IS NOT NULL, DATE(HSBA1.HSBA_NGAYGIORAV) - DATE(HSBA_NGAYGIOVAOV) + 1  , 0 )) SONGAYDIEUTRI,
	HS.HSTHTOAN_TONGCHI TONGCONG
	, HS.HSTHTOAN_BNTRA BNTRA, HS.HSTHTOAN_BHYT BHYTCHI
       	
,
  ( IF (HS.HSTHTOAN_PHONG IS NULL, 0, HS.HSTHTOAN_PHONG) +  IF (HS.HSTHTOAN_PHONGNDM IS NULL, 0, HS.HSTHTOAN_PHONGNDM) ) AS GIUONG,
  
   ( IF (HS.HSTHTOAN_THUOC IS NULL, 0, HS.HSTHTOAN_THUOC) +  IF (HS.HSTHTOAN_THUOCNDM IS NULL, 0, HS.HSTHTOAN_THUOCNDM) ) AS THUOC,
   HS.HSTHTOAN_MAU AS MAU,
HSTHTOAN_XNTDCN AS XETNGHIEM,
HSTHTOAN_CDHA AS CHANDOAN_HINH_ANH,
HSTHTOAN_DVKT_TT AS PTTT,
HSTHTOAN_DV_KTC AS KTC,

( IF (HS.HSTHTOAN_VTTH IS NULL, 0, HS.HSTHTOAN_VTTH) +  IF (HS.HSTHTOAN_VTTHNDM IS NULL, 0, HS.HSTHTOAN_VTTHNDM) ) AS  VTTH,
HSTHTOAN_CP_VC AS VANCHUYEN

FROM ((((((( HSBA_BHYT HSBABHYT JOIN HSBA HSBA1 ON HSBABHYT.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN)

		 JOIN DT_DM_KHOI_BHYT DTDMKHOIBHYT
          ON HSBABHYT.HSBABHYT_KHOIBH = DTDMKHOIBHYT.DTDMKHOIBHYT_MASO)

     LEFT JOIN DT_DM_PL_BHYT DTDMPLBHYT  ON DTDMKHOIBHYT.DTDMKHOIBHYT_PHANLOAI = DTDMPLBHYT.DTDMPHLOAIBHYT_MASO)
     LEFT JOIN BENH_NHAN BN ON BN.BENHNHAN_MA LIKE HSBA1.BENHNHAN_MA)
     LEFT JOIN DM_GIOI_TINH GIOI ON BN.DMGT_MASO = GIOI.DMGT_MASO)
     LEFT JOIN DM_BENH_VIEN KCBBHYT  ON KCBBHYT.DMBENHVIEN_MASO = HSBABHYT.HSBABHYT_MAKCB	)
     LEFT JOIN DM_BENH_ICD BENH ON BENH.DMBENHICD_MASO = HSBA1.HSBA_MACHDRAVIEN)
     LEFT JOIN HS_THTOAN HS ON HS.HSBA_SOVAOVIEN = HSBA1.HSBA_SOVAOVIEN

WHERE   ( $P{MaPLDoiTuong} IS NULL OR DTDMPLBHYT.DTDMPHLOAIBHYT_MA LIKE $P{MaPLDoiTuong} )
	

        AND ( $P{DenNgay} IS NULL OR DATEDIFF( DATE(HS.HSTHTOAN_NGAYTT) , DATE($P{DenNgay})) <= 0 )
        AND ($P{TuNgay} IS NULL OR DATEDIFF( DATE(HS.HSTHTOAN_NGAYTT) , DATE($P{TuNgay})) >= 0 )
and ($P{MaKCBBHYT} IS NULL OR KCBBHYT.DMBENHVIEN_MA LIKE $P{MaKCBBHYT})
order by KCBBHYT.DMBENHVIEN_TEN;]]></queryString>

