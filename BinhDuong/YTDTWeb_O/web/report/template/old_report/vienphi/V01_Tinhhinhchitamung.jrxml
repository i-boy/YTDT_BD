	<parameter name="DenNgay" isForPrompting="false" class="java.util.Date"/>
	<parameter name="TINH" isForPrompting="false" class="java.lang.String"/>
	<parameter name="nhanvienMa" isForPrompting="false" class="java.lang.String"/>
	<parameter name="CUM" isForPrompting="false" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT  BN.BENHNHAN_HOTEN HOTEN, BN.BENHNHAN_TUOI TUOI, IF(BN.BENHNHAN_DONVITUOI=2,'T'
,IF(BN.BENHNHAN_DONVITUOI=3, 'N','')) DONVITUOI, '' KHOA, TD1.KHOIBHYT_MA KHOIBHYT,
TUK.HOANUNGKHAM_SOTIEN AS SOTIEN,
TUK.HOANUNGKHAM_MA MAPHIEU, TUK.HOANUNGKHAM_NGAY NGAY,  NHANVIEN.DTDMNHANVIEN_MA THUNGAN
FROM (((TIEP_DON TD1 JOIN HOAN_UNG_KHAM TUK
          ON TD1.TIEPDON_MA LIKE TUK.TIEPDON_MA)
			JOIN DT_DM_NHAN_VIEN NHANVIEN ON NHANVIEN.DTDMNHANVIEN_MASO = TUK.HOANUNGKHAM_THUNGAN)
		  JOIN BENH_NHAN BN ON BN.BENHNHAN_MA LIKE TD1.BENHNHAN_MA)     
WHERE ($P{DenNgay} is null or DATE(TUK.HOANUNGKHAM_NGAY) <= DATE($P{DenNgay}))
        AND ($P{TuNgay} is null or DATE(TUK.HOANUNGKHAM_NGAY) >= DATE($P{TuNgay}) ) 
	and ($P{nhanvienMa} is null or NHANVIEN.DTDMNHANVIEN_MA like ($P{nhanvienMa}))
	and ($P{CUM} is null or TUK.HOANUNGKHAM_CUM = $P{CUM} ) 
		
UNION

SELECT  BN.BENHNHAN_HOTEN HOTEN, BN.BENHNHAN_TUOI TUOI, IF(BN.BENHNHAN_DONVITUOI=2,'T'
,IF(BN.BENHNHAN_DONVITUOI=3, 'N','')) DONVITUOI, DMKHOA.DMKHOA_MA KHOA, DTDMKHOIBHYT.DTDMKHOIBHYT_MA KHOIBHYT,
TU.HOANUNG_SOTIEN AS SOTIEN,
TU.HOANUNG_MA MAPHIEU, TU.HOANUNG_NGAYGIO NGAY,  NHANVIEN.DTDMNHANVIEN_MA THUNGAN
FROM ((((((HSBA HSBA1 JOIN HOAN_UNG TU ON HSBA1.HSBA_SOVAOVIEN LIKE TU.HSBA_SOVAOVIEN)
			JOIN DT_DM_NHAN_VIEN NHANVIEN ON NHANVIEN.DTDMNHANVIEN_MASO = TU.HOANUNG_THUNGAN)
            JOIN HSBA_KHOA HSBAKHOA ON HSBAKHOA.HSBA_SOVAOVIEN = HSBA1.HSBA_SOVAOVIEN)
			JOIN DM_KHOA DMKHOA ON DMKHOA.DMKHOA_MASO = HSBAKHOA.KHOA_MA)
			LEFT JOIN HSBA_BHYT HSBABHYT ON HSBABHYT.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN)
			LEFT JOIN DT_DM_KHOI_BHYT DTDMKHOIBHYT ON HSBABHYT.HSBABHYT_KHOIBH = DTDMKHOIBHYT.DTDMKHOIBHYT_MASO)
		  JOIN BENH_NHAN BN ON BN.BENHNHAN_MA LIKE HSBA1.BENHNHAN_MA  
WHERE   ($P{DenNgay} is null or DATE(TU.HOANUNG_NGAYGIO) <= DATE($P{DenNgay}))
        AND ($P{TuNgay} is null or DATE(TU.HOANUNG_NGAYGIO) >= DATE($P{TuNgay}) )
	and ($P{nhanvienMa} is null or NHANVIEN.DTDMNHANVIEN_MA like ($P{nhanvienMa}))
	and ($P{CUM} is null or TU.HOANUNG_CUM = $P{CUM} )]]></queryString>

