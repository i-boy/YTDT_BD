	<queryString><![CDATA[SELECT KHOAVAOVIEN.DMKHOA_MA KHOA, KHOARAVIEN.DMKHOA_MA MAKHOARAVIEN,
BN.BENHNHAN_TUOI TUOI, CONCAT(BN.BENHNHAN_DIACHI, ', ', X.DMXA_TEN, ', ', H.DMHUYEN_TEN) DIACHI,
IF(BN.BENHNHAN_DONVITUOI=2,'T'
,IF(BN.BENHNHAN_DONVITUOI=3, 'N','')) DONVITUOI , HSBABHYT.HSBABHYT_SOTHEBH,
 HSBA1.HSBA_SOVAOVIEN SOVAOVIEN , BN.BENHNHAN_HOTEN HOTEN,  HS.HSTHTOAN_TONGCHI TONGCHI, HS.HSTHTOAN_MIENDT MIENGIAM, 
(SELECT MIENGIAM_LYDO FROM  MIEN_GIAM WHERE MIENGIAM_MA = ( SELECT MAX(MG2.MIENGIAM_MA) FROM MIEN_GIAM MG2 WHERE MG2.HSBA_SOVAOVIEN 
  = HSBA1.HSBA_SOVAOVIEN)) LYDO,HS.HSTHTOAN_NGAYTT NGAYTT

FROM ((((((( (HSBA HSBA1 JOIN HS_THTOAN HS ON HS.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN)
			LEFT JOIN HSBA_BHYT HSBABHYT ON HSBABHYT.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN)
			LEFT JOIN DM_KHOA KHOAVAOVIEN ON HSBA1.HSBA_KHOAVAOV = KHOAVAOVIEN.DMKHOA_MASO )
			LEFT JOIN DM_KHOA KHOARAVIEN ON HSBA1.HSBA_KHOARAV = KHOARAVIEN.DMKHOA_MASO )
			LEFT JOIN BENH_NHAN BN ON BN.BENHNHAN_MA LIKE HSBA1.BENHNHAN_MA)
			LEFT JOIN DM_GIOI_TINH GIOI ON BN.DMGT_MASO = GIOI.DMGT_MASO)
			LEFT JOIN DM_HUYEN H ON BN.HUYEN_MA = H.DMHUYEN_MASO)
			LEFT JOIN DM_XA X ON BN.XA_MA = X.DMXA_MASO)
WHERE 
	( $P{TuNgay} IS NULL OR DATE(HS.HSTHTOAN_NGAYTT) >= DATE($P{TuNgay}))
 	and 
    ( $P{DenNgay} IS NULL OR DATE(HS.HSTHTOAN_NGAYTT) <= DATE($P{DenNgay}))
        and HS.HSTHTOAN_MIENDT > 0]]></queryString>

