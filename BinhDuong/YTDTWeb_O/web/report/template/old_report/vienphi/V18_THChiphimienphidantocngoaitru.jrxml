	HS.HSTHTOANK_TONGCHI TONGCHI,
	
	IF( HS.HSTHTOANK_MIENTHUOC IS NULL, 0, HS.HSTHTOANK_MIENTHUOC )	
	
	AS MIENPHI,
	
	CONCAT(IF(BN.BENHNHAN_DIACHI IS NULL
	,'',CONCAT(BN.BENHNHAN_DIACHI,' ')),IF(BN.XA_MA IS NULL,'',CONCAT(XA.DMXA_TEN,' ')),
	IF(BN.HUYEN_MA IS NULL,'',CONCAT(HUYEN.DMHUYEN_TEN,' ')),IF(BN.TINH_MA IS NULL,'',
	TINH.DMTINH_TEN)) DIACHI,
	TD1.TIEPDON_NGAYGIO NGAYGIO

FROM
  (((((((((((TIEP_DON TD1 JOIN BENH_NHAN BN  ON TD1.BENHNHAN_MA LIKE BN.BENHNHAN_MA)
              JOIN THAM_KHAM TK ON TK.TIEPDON_MA LIKE TD1.TIEPDON_MA)
			  JOIN DM_DOI_TUONG DT ON DT.DMDOITUONG_MASO = TD1.DOITUONG_MA)
			  JOIN CLS_KHAM CLSKHAM ON CLSKHAM.CLSKHAM_THAMKHAM = TK.THAMKHAM_MA )
              JOIN DT_DM_CLS_BANG_GIA BANGGIA ON BANGGIA.DTDMCLSBG_MASO = CLSKHAM_MAHANG)
              JOIN DT_DM_CLS CLS ON CLS.DTDMCLS_MASO = BANGGIA.DTDMCLSBG_MALOAI)   
			 LEFT JOIN HS_THTOANK HS ON HS.TIEPDON_MA = TD1.TIEPDON_MA	)
		     JOIN DM_GIOI_TINH GIOI ON BN.DMGT_MASO = GIOI.DMGT_MASO)
			 LEFT JOIN DM_TINH TINH ON BN.TINH_MA = TINH.DMTINH_MASO)
			LEFT JOIN DM_HUYEN HUYEN ON BN.HUYEN_MA = HUYEN.DMHUYEN_MASO) 
		LEFT JOIN DM_XA XA ON BN.XA_MA = XA.DMXA_MASO) 
		
WHERE  
 ( $P{MaSoDoiTuong} is null or TD1.DOITUONG_MA = $P{MaSoDoiTuong} )
AND ($P{denngay} is null or DATEDIFF(DATE($P{denngay}),DATE(TD1.TIEPDON_NGAYGIO)) >= 0 )
			AND ($P{tungay} is null or DATEDIFF(DATE(TD1.TIEPDON_NGAYGIO),DATE($P{tungay})) >= 0 )
AND
DTDMCLS_MA LIKE 'KH' 
AND
	(
		DT.DMDOITUONG_MA LIKE 'MP' 
		OR
		(
			CLSKHAM.CLSKHAM_MIEN = TRUE OR 
		   (
			HS.HSTHTOANK_MIENPHONG > 0 OR 
			HS.HSTHTOANK_MIENTHUOC > 0 OR
			HS.HSTHTOANK_KHONGTHU> 0 OR
			HS.HSTHTOANK_MIENTHUOCLAO> 0 OR
			HS.HSTHTOANK_MIENTE> 0 OR
			HS.HSTHTOANK_MIENDT> 0 OR
			HS.HSTHTOANK_MIENMAU> 0 OR
			HS.HSTHTOANK_XETGIAM> 0 
			)
		)	
	)]]></queryString>

