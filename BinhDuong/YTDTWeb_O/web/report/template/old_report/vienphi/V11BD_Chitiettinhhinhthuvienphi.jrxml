	<queryString><![CDATA[Select khoa.DMKHOA_TEN TENKHOA,
       tmp2.*
FROM
(Select tmp.MAKHOA MAKHOA,
       SUM(IFNULL(tmp.GIUONG,0)) AS GIUONG,
       SUM(IFNULL(tmp.THUOC,0)) AS THUOC,
       SUM(IFNULL(tmp.MAU,0)) AS MAU,
       SUM(IFNULL(tmp.XETNGHIEM,0)) AS XETNGHIEM,
       SUM(IFNULL(tmp.CHANDOAN_HINH_ANH,0)) AS CHANDOAN_HINH_ANH,
       SUM(IFNULL(tmp.PTTT,0)) AS PTTT,
       SUM(IFNULL(tmp.KTC,0)) AS KTC,
       SUM(IFNULL(tmp.THUTHUAT,0)) AS THUTHUAT,
       SUM(IFNULL(tmp.VANCHUYEN,0)) AS VANCHUYEN,
       SUM(IFNULL(tmp.CONGKHAM,0)) AS CONGKHAM,
       SUM(IFNULL(tmp.BNTRA,0)) AS BNTRA,
       SUM(IFNULL(tmp.TONGCONG,0)) AS TONGCONG
FROM
(SELECT hstt.HSTHTOAN_KHOA MAKHOA,
       SUM(( IFNULL(hstt.HSTHTOAN_PHONG,0) +  IFNULL(hstt.HSTHTOAN_PHONGNDM, 0))) AS GIUONG,
       SUM(( IFNULL(hstt.HSTHTOAN_THUOC,0) +  IFNULL(hstt.HSTHTOAN_THUOCNDM, 0))) AS THUOC,
       SUM(hstt.HSTHTOAN_MAU) AS MAU,
       SUM(hstt.HSTHTOAN_XNTDCN) AS XETNGHIEM,
       SUM(hstt.HSTHTOAN_CDHA) AS CHANDOAN_HINH_ANH,
       SUM(( IFNULL(hstt.HSTHTOAN_PHAUTHUAT,0) +  IFNULL(hstt.HSTHTOAN_PHAUTHUATNDM, 0))) AS PTTT,
       SUM(hstt.HSTHTOAN_DV_KTC) AS KTC,
       SUM(hstt.HSTHTOAN_DVKT_TT) AS THUTHUAT,
       SUM(hstt.HSTHTOAN_CP_VC) AS VANCHUYEN,
       SUM(hstt.HSTHTOAN_CONGKHAM) AS CONGKHAM,
       SUM(hstt.HSTHTOAN_BNTRA) AS BNTRA,
       SUM(IFNULL(hstt.HSTHTOAN_BNTRA,0) + IFNULL(hstt.HSTHTOAN_BHYT,0)) AS TONGCONG
FROM hs_thtoan hstt
WHERE hstt.HSTHTOAN_NGAYTT Is Not Null
      AND DATE(hstt.HSTHTOAN_NGAYTT) >= $P{TuNgay}
  AND DATE(hstt.HSTHTOAN_NGAYTT) <= $P{DenNgay}
  AND (hstt.HSTHTOAN_BOVIEN Is Null OR hstt.HSTHTOAN_BOVIEN = 0)
GROUP BY hstt.HSTHTOAN_KHOA

UniON
 SELECT bk.DMKHOA_MASO MAKHOA,
       0 AS GIUONG,
       SUM(( IFNULL(hsttk.HSTHTOANK_THUOC,0) +  IFNULL(hsttk.HSTHTOANK_THUOCNDM, 0))) AS THUOC,
       SUM(hsttk.HSTHTOANK_MAU) AS MAU,
       SUM(hsttk.HSTHTOANK_XNTDCN) AS XETNGHIEM,
       SUM(hsttk.HSTHTOANK_CDHA) AS CHANDOAN_HINH_ANH,
       SUM(( IFNULL(hsttk.HSTHTOANK_PHAUTHUAT,0) +  IFNULL(hsttk.HSTHTOANK_PHAUTHUATNDM, 0))) AS PTTT,
       SUM(hsttk.HSTHTOANK_DV_KTC) AS KTC,
       SUM(hsttk.HSTHTOANK_DVKT_TT) AS THUTHUAT,
       SUM(hsttk.HSTHTOANK_CP_VC) AS VANCHUYEN,
       SUM(hsttk.HSTHTOANK_CONGKHAM) AS CONGKHAM,
       SUM(hsttk.HSTHTOANK_BNTRA) AS BNTRA,
       SUM(IFNULL(hsttk.HSTHTOANK_BNTRA,0) + IFNULL(hsttk.HSTHTOANK_BHYT,0)) AS TONGCONG
FROM hs_thtoank hsttk
     LEFT JOIN tiep_don td ON hsttk.TIEPDON_MA = td.TIEPDON_MA
     LEFT JOIN dt_dm_ban_kham bk ON td.TIEPDON_BANKHAM = bk.DTDMBANKHAM_MASO
WHERE hsttk.HSTHTOANK_NGAYGIOTT Is Not Null
      AND hsttk.HSTHTOANK_NGAYGIOTT >= $P{TuNgay}
      AND hsttk.HSTHTOANK_NGAYGIOTT <= $P{DenNgay}
GROUP BY bk.DMKHOA_MASO) tmp
GROUP BY tmp.MAKHOA) tmp2
 LEFT JOIN dm_khoa khoa ON tmp2.MAKHOA = khoa.DMKHOA_MASO
ORDER BY khoa.DMKHOA_TEN ASC ;]]></queryString>

