	<queryString><![CDATA[SELECT BN.BENHNHAN_HOTEN HOTEN, BN.BENHNHAN_NGAYSINH NGAYSINH,GIOI.DMGT_TEN GIOITINH,
	HSBABHYT.HSBABHYT_SOTHEBH SOTHEBHYT, KCBBHYT.DTDMKCBBHYT_MA NOIKCBBD,HSBA1.HSBA_SOVAOVIEN SOVAOVIEN,
	HSBA1.HSBA_NGAYGIOVAOV NGAYGIOVAOV,HSBA1.HSBA_NGAYGIORAV NGAYGIORAV, BENH.DMBENHICD_MA BENH,
	
	(  IF ( HSBA1.HSBA_NGAYGIORAV IS NOT NULL AND HSBA_NGAYGIOVAOV IS NOT NULL, DATE(HSBA1.HSBA_NGAYGIORAV) - DATE(HSBA_NGAYGIOVAOV) + 1  , 0 )) SONGAYDIEUTRI,
	( IF ( HS.HSTHTOAN_BNTRA IS NULL, 0 , HS.HSTHTOAN_BNTRA) + IF (HS.HSTHTOAN_BHYT IS NULL, 0 , HS.HSTHTOAN_BHYT) ) TONGCONG
	, HS.HSTHTOAN_BNTRA BNTRA, HS.HSTHTOAN_BHYT BHYTCHI
       	
,
(  SELECT  SUM( CLSMO.CLSMO_DONGIA * (
                                  IF( CLSMO.CLSMO_LAN IS NULL, 0, CLSMO.CLSMO_LAN ) -
                                  IF( CLSMO.CLSMO_TRA IS NULL, 0, CLSMO.CLSMO_TRA )
                                )
             )
   FROM (((( HSBA HSBA2 JOIN HSBA_KHOA HSBAKHOA ON HSBAKHOA.HSBA_SOVAOVIEN LIKE HSBA2.HSBA_SOVAOVIEN)
             LEFT JOIN CLS_MO CLSMO ON CLSMO.HSBAKHOA_MASO = HSBAKHOA.HSBAKHOA_MASO )
             LEFT JOIN DT_DM_CLS_BANG_GIA BANGGIA ON BANGGIA.DTDMCLSBG_MASO = CLSMO.CLSMO_MAHANG)
             LEFT JOIN DT_DM_CLS CLS ON CLS.DTDMCLS_MASO = BANGGIA.DTDMCLSBG_MALOAI)
	     	

   WHERE HSBA2.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN AND DTDMCLS_MA LIKE 'GI'
) AS GIUONG,
(  SELECT  SUM(THUOCNT.THUOCNOITRU_DONGIA * 
                                  IF( THUOCNT.THUOCNOITRU_SOLUONG IS NULL, 0, THUOCNT.THUOCNOITRU_SOLUONG ) 
                                
             )
   FROM ((((( HSBA HSBA2 JOIN HSBA_KHOA HSBAKHOA ON HSBAKHOA.HSBA_SOVAOVIEN LIKE HSBA2.HSBA_SOVAOVIEN)
             
             LEFT JOIN THUOC_NOI_TRU THUOCNT ON THUOCNT.HSBA_KHOA = HSBAKHOA.HSBAKHOA_MASO )
             LEFT JOIN DM_THUOC DMTHUOC ON DMTHUOC.DMTHUOC_MASO = THUOCNT.THUOCNOITRU_MATHUOC)
             LEFT JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON PLTHUOC.DMPHANLOAITHUOC_MASO = DMTHUOC.DMPHANLOAITHUOC_MASO)
	      LEFT JOIN DM_LOAI_THUOC LOAITHUOC ON LOAITHUOC.DMLOAITHUOC_MASO = PLTHUOC.DMLOAITHUOC_MASO	)

   WHERE HSBA2.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN AND LOAITHUOC.DMLOAITHUOC_MA NOT LIKE 'YC'
) AS THUOC,
(  SELECT  SUM( CLSMO.CLSMO_DONGIA * (
                                  IF( CLSMO.CLSMO_LAN IS NULL, 0, CLSMO.CLSMO_LAN ) -
                                  IF( CLSMO.CLSMO_TRA IS NULL, 0, CLSMO.CLSMO_TRA )
                                )
             )
   FROM (((( HSBA HSBA2 JOIN HSBA_KHOA HSBAKHOA ON HSBAKHOA.HSBA_SOVAOVIEN LIKE HSBA2.HSBA_SOVAOVIEN)
             LEFT JOIN CLS_MO CLSMO ON CLSMO.HSBAKHOA_MASO = HSBAKHOA.HSBAKHOA_MASO )
             LEFT JOIN DT_DM_CLS_BANG_GIA BANGGIA ON BANGGIA.DTDMCLSBG_MASO = CLSMO.CLSMO_MAHANG)
             LEFT JOIN DT_DM_CLS CLS ON CLS.DTDMCLS_MASO = BANGGIA.DTDMCLSBG_MALOAI)
	     	

   WHERE HSBA2.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN AND DTDMCLS_MA LIKE 'MA'
) AS MAU,
(  SELECT  SUM( CLSMO.CLSMO_DONGIA * (
                                  IF( CLSMO.CLSMO_LAN IS NULL, 0, CLSMO.CLSMO_LAN ) -
                                  IF( CLSMO.CLSMO_TRA IS NULL, 0, CLSMO.CLSMO_TRA )
                                )
             )
   FROM (((( HSBA HSBA2 JOIN HSBA_KHOA HSBAKHOA ON HSBAKHOA.HSBA_SOVAOVIEN LIKE HSBA2.HSBA_SOVAOVIEN)
             LEFT JOIN CLS_MO CLSMO ON CLSMO.HSBAKHOA_MASO = HSBAKHOA.HSBAKHOA_MASO )
             LEFT JOIN DT_DM_CLS_BANG_GIA BANGGIA ON BANGGIA.DTDMCLSBG_MASO = CLSMO.CLSMO_MAHANG)
             LEFT JOIN DT_DM_CLS CLS ON CLS.DTDMCLS_MASO = BANGGIA.DTDMCLSBG_MALOAI)
	     	

   WHERE HSBA2.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN AND DTDMCLS_MA LIKE 'XN'
) AS XETNGHIEM,
(  SELECT  SUM( CLSMO.CLSMO_DONGIA * (
                                  IF( CLSMO.CLSMO_LAN IS NULL, 0, CLSMO.CLSMO_LAN ) -
                                  IF( CLSMO.CLSMO_TRA IS NULL, 0, CLSMO.CLSMO_TRA )
                                )
             )
   FROM (((( HSBA HSBA2 JOIN HSBA_KHOA HSBAKHOA ON HSBAKHOA.HSBA_SOVAOVIEN LIKE HSBA2.HSBA_SOVAOVIEN)
             LEFT JOIN CLS_MO CLSMO ON CLSMO.HSBAKHOA_MASO = HSBAKHOA.HSBAKHOA_MASO )
             LEFT JOIN DT_DM_CLS_BANG_GIA BANGGIA ON BANGGIA.DTDMCLSBG_MASO = CLSMO.CLSMO_MAHANG)
             LEFT JOIN DT_DM_CLS CLS ON CLS.DTDMCLS_MASO = BANGGIA.DTDMCLSBG_MALOAI)
	     	

   WHERE HSBA2.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN AND DTDMCLS_MA LIKE 'CD'
) AS CHANDOAN_HINH_ANH,
(  SELECT  SUM( CLSMO.CLSMO_DONGIA * (
                                  IF( CLSMO.CLSMO_LAN IS NULL, 0, CLSMO.CLSMO_LAN ) -
                                  IF( CLSMO.CLSMO_TRA IS NULL, 0, CLSMO.CLSMO_TRA )
                                )
             )
   FROM (((( HSBA HSBA2 JOIN HSBA_KHOA HSBAKHOA ON HSBAKHOA.HSBA_SOVAOVIEN LIKE HSBA2.HSBA_SOVAOVIEN)
             LEFT JOIN CLS_MO CLSMO ON CLSMO.HSBAKHOA_MASO = HSBAKHOA.HSBAKHOA_MASO )
             LEFT JOIN DT_DM_CLS_BANG_GIA BANGGIA ON BANGGIA.DTDMCLSBG_MASO = CLSMO.CLSMO_MAHANG)
             LEFT JOIN DT_DM_CLS CLS ON CLS.DTDMCLS_MASO = BANGGIA.DTDMCLSBG_MALOAI)
	     	

   WHERE HSBA2.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN AND ( DTDMCLS_MA LIKE 'PT' OR DTDMCLS_MA LIKE 'TT' )
) AS PTTT,
(  SELECT  SUM( CLSMO.CLSMO_DONGIA * (
                                  IF( CLSMO.CLSMO_LAN IS NULL, 0, CLSMO.CLSMO_LAN ) -
                                  IF( CLSMO.CLSMO_TRA IS NULL, 0, CLSMO.CLSMO_TRA )
                                )
             )
   FROM (((( HSBA HSBA2 JOIN HSBA_KHOA HSBAKHOA ON HSBAKHOA.HSBA_SOVAOVIEN LIKE HSBA2.HSBA_SOVAOVIEN)
             LEFT JOIN CLS_MO CLSMO ON CLSMO.HSBAKHOA_MASO = HSBAKHOA.HSBAKHOA_MASO )
             LEFT JOIN DT_DM_CLS_BANG_GIA BANGGIA ON BANGGIA.DTDMCLSBG_MASO = CLSMO.CLSMO_MAHANG)
             LEFT JOIN DT_DM_CLS CLS ON CLS.DTDMCLS_MASO = BANGGIA.DTDMCLSBG_MALOAI)
	     	

   WHERE HSBA2.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN AND DTDMCLS_MA LIKE 'KT' 
) AS KTC,

(  SELECT  SUM(THUOCNT.THUOCNOITRU_DONGIA * 
                                  IF( THUOCNT.THUOCNOITRU_SOLUONG IS NULL, 0, THUOCNT.THUOCNOITRU_SOLUONG ) 
                                
             )
   FROM ((((( HSBA HSBA2 JOIN HSBA_KHOA HSBAKHOA ON HSBAKHOA.HSBA_SOVAOVIEN LIKE HSBA2.HSBA_SOVAOVIEN)
             
             LEFT JOIN THUOC_NOI_TRU THUOCNT ON THUOCNT.HSBA_KHOA = HSBAKHOA.HSBAKHOA_MASO )
             LEFT JOIN DM_THUOC DMTHUOC ON DMTHUOC.DMTHUOC_MASO = THUOCNT.THUOCNOITRU_MATHUOC)
             LEFT JOIN DM_PHAN_LOAI_THUOC PLTHUOC ON PLTHUOC.DMPHANLOAITHUOC_MASO = DMTHUOC.DMPHANLOAITHUOC_MASO)
	      LEFT JOIN DM_LOAI_THUOC LOAITHUOC ON LOAITHUOC.DMLOAITHUOC_MASO = PLTHUOC.DMLOAITHUOC_MASO	)

   WHERE HSBA2.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN AND LOAITHUOC.DMLOAITHUOC_MA LIKE 'YC'
) AS VTTH,
(  SELECT  SUM( CLSMO.CLSMO_DONGIA * (
                                  IF( CLSMO.CLSMO_LAN IS NULL, 0, CLSMO.CLSMO_LAN ) -
                                  IF( CLSMO.CLSMO_TRA IS NULL, 0, CLSMO.CLSMO_TRA )
                                )
             )
   FROM (((( HSBA HSBA2 JOIN HSBA_KHOA HSBAKHOA ON HSBAKHOA.HSBA_SOVAOVIEN LIKE HSBA2.HSBA_SOVAOVIEN)
             LEFT JOIN CLS_MO CLSMO ON CLSMO.HSBAKHOA_MASO = HSBAKHOA.HSBAKHOA_MASO )
             LEFT JOIN DT_DM_CLS_BANG_GIA BANGGIA ON BANGGIA.DTDMCLSBG_MASO = CLSMO.CLSMO_MAHANG)
             LEFT JOIN DT_DM_CLS CLS ON CLS.DTDMCLS_MASO = BANGGIA.DTDMCLSBG_MALOAI)
	     	

   WHERE HSBA2.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN AND DTDMCLS_MA LIKE 'VC' 
) AS VANCHUYEN

FROM ((((((( HSBA_BHYT HSBABHYT JOIN HSBA HSBA1 ON HSBABHYT.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN)
			JOIN BENH_NHAN BN ON BN.BENHNHAN_MA LIKE HSBA1.BENHNHAN_MA)
		JOIN DM_GIOI_TINH GIOI ON BN.DMGT_MASO = GIOI.DMGT_MASO)
		LEFT JOIN DT_DM_KHOI_BHYT DTDMKHOIBHYT   ON HSBABHYT.HSBABHYT_KHOIBH = DTDMKHOIBHYT.DTDMKHOIBHYT_MASO)
		  
     LEFT JOIN DT_DM_PL_BHYT DTDMPLBHYT  ON DTDMKHOIBHYT.DTDMKHOIBHYT_PHANLOAI = DTDMPLBHYT.DTDMPHLOAIBHYT_MASO)
     
     LEFT JOIN DT_DM_KCB_BHYT KCBBHYT  ON KCBBHYT.DTDMKCBBHYT_MASO = HSBABHYT.HSBABHYT_MAKCB	)
     LEFT JOIN DM_BENH_ICD BENH ON BENH.DMBENHICD_MASO = HSBA1.HSBA_MACHDOANBD)	
     LEFT JOIN HS_THTOAN HS ON HS.HSBA_SOVAOVIEN = HSBA1.HSBA_SOVAOVIEN

WHERE ( $P{MaPLDoiTuong} IS NULL OR DTDMPLBHYT.DTDMPHLOAIBHYT_MA LIKE $P{MaPLDoiTuong} )
	AND
     (
	$P{chon} IS NULL OR
	( if ( $P{chon} = 'IN',
		 ($P{khoi} is null or DTDMKHOIBHYT.DTDMKHOIBHYT_MA IN ($P!{khoi})),
		 ($P{khoi} is null or DTDMKHOIBHYT.DTDMKHOIBHYT_MA NOT IN ($P!{khoi}))
              )
        ) 
      )
        AND 
   ( 
       $P{DenNgay} is null or  ( HS.HSTHTOAN_NGAYTT is not null and DATEDIFF(DATE(HS.HSTHTOAN_NGAYTT),DATE($P{DenNgay}))  <= 0 ) 
   )

     	AND
   (  
	$P{TuNgay} IS NULL OR ( HS.HSTHTOAN_NGAYTT is not null and DATEDIFF(DATE(HS.HSTHTOAN_NGAYTT),DATE($P{TuNgay}))   >= 0)
   )  
		;]]></queryString>

