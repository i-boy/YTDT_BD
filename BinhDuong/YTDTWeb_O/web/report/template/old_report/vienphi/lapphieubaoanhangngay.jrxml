	<queryString><![CDATA[SELECT pba.PHIEUBAOAN_MASO, pba.PHIEUBAOAN_MA, bn.BENHNHAN_HOTEN, DTDMMA_TEN, phong.DTDMP_TEN,
cda.DTDMCDA_TEN, la.DTDMLA_TEN, pba.PHIEUBAOAN_GIOAN
FROM PHIEU_BAO_AN pba
INNER JOIN HSBA ba ON ba.HSBA_SOVAOVIEN = pba.HSBA_SOVAOVIEN 
INNER JOIN BENH_NHAN bn ON bn.BENHNHAN_MA = ba.BENHNHAN_MA
INNER JOIN DT_DM_PHONG phong ON phong.DTDMP_MASO = pba.PHONG_MASO
INNER JOIN DT_DM_MUC_AN ma ON ma.DTDMMA_MASO = pba.MUCAN_MASO
INNER JOIN DT_DM_CHE_DO_AN cda on cda.DTDMCDA_MASO = pba.CHEDOAN_MASO
INNER JOIN DT_DM_LOAI_AN la ON la.DTDMLA_MASO = pba.LOAIAN_MASO
WHERE pba.PHIEUBAOAN_MA like $P{maphieu}]]></queryString>

