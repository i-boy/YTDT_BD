	<parameter name="soyte" isForPrompting="false" class="java.lang.String"/>
	<parameter name="benhvien" isForPrompting="false" class="java.lang.String"/>
	<parameter name="sodt" isForPrompting="false" class="java.lang.String"/>
	<parameter name="TINH" isForPrompting="false" class="java.lang.String"/>
	<parameter name="TuNgay" isForPrompting="false" class="java.util.Date"/>
	<parameter name="DenNgay" isForPrompting="false" class="java.util.Date"/>
	<parameter name="cum" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT  DMKHOA.DMKHOA_MA MAKHOA, DMKHOA.DMKHOA_TEN TENKHOA, BN.BENHNHAN_HOTEN HOTEN, BN.BENHNHAN_TUOI TUOI,
					IF(BN.BENHNHAN_DONVITUOI=2,'T',IF(BN.BENHNHAN_DONVITUOI=3, 'N','')) DONVITUOI , DTDMKHOIBHYT.DTDMKHOIBHYT_MA BHYT,
			TU.TAMUNG_SOTIEN THU, 0 CHI,
			TAMUNG_NGAYGIO NGAYTT, NHANVIEN.DTDMNHANVIEN_MA THUNGAN , 'TU' LOAI, 
			IF (DTDMKHOIBHYT.DTDMKHOIBHYT_MA IS  NULL OR DTDMKHOIBHYT.DTDMKHOIBHYT_MA LIKE '',TU.TAMUNG_SOTIEN ,0 ) THU_VIEN_PHI, 
			0 CHI_VIEN_PHI, 
			IF (DTDMKHOIBHYT.DTDMKHOIBHYT_MA IS NOT NULL AND DTDMKHOIBHYT.DTDMKHOIBHYT_MA <> '',TU.TAMUNG_SOTIEN ,0 ) THU_BHYT, 
			0 CHI_BHYT

	FROM

		(((((( HSBA HSBA2 JOIN TAM_UNG TU ON HSBA2.HSBA_SOVAOVIEN LIKE TU.HSBA_SOVAOVIEN )
			    LEFT JOIN HSBA_BHYT HSBABHYT ON HSBABHYT.HSBA_SOVAOVIEN LIKE HSBA2.HSBA_SOVAOVIEN)
				LEFT JOIN DM_KHOA DMKHOA ON  TU.TAMUNG_KHOA = DMKHOA.DMKHOA_MASO )
				LEFT JOIN DT_DM_NHAN_VIEN NHANVIEN ON NHANVIEN.DTDMNHANVIEN_MASO = TU.TAMUNG_THUNGAN)			
				LEFT JOIN DT_DM_KHOI_BHYT DTDMKHOIBHYT ON HSBABHYT.HSBABHYT_KHOIBH LIKE DTDMKHOIBHYT.DTDMKHOIBHYT_MASO)
				LEFT JOIN BENH_NHAN BN ON BN.BENHNHAN_MA LIKE HSBA2.BENHNHAN_MA)
				LEFT JOIN DT_DM_CUM CUM ON TU.TAMUNG_CUM = CUM.DTDMCUM_MASO
		WHERE 
		( $P{TuNgay} IS NULL OR DATE(TAMUNG_NGAYGIO) >= DATE($P{TuNgay}))
 	and 
    ( $P{DenNgay} IS NULL OR DATE(TAMUNG_NGAYGIO) <= DATE($P{DenNgay}))	
	and 
    ( $P{cum} IS NULL OR $P{cum} LIKE '' OR CUM.DTDMCUM_MA like $P{cum})	

UNION

	SELECT DMKHOA.DMKHOA_MA MAKHOA, DMKHOA.DMKHOA_TEN TENKHOA, BN.BENHNHAN_HOTEN HOTEN, BN.BENHNHAN_TUOI TUOI, 
					IF(BN.BENHNHAN_DONVITUOI=2,'T',IF(BN.BENHNHAN_DONVITUOI=3, 'N','')) DONVITUOI , DTDMKHOIBHYT.DTDMKHOIBHYT_MA BHYT,
			IF ( HS.HSTHTOAN_THTOAN > 0,  HS.HSTHTOAN_THTOAN, 0 ) THU, IF ( HS.HSTHTOAN_THTOAN < 0,  -1*HS.HSTHTOAN_THTOAN, 0 ) CHI,
			HS.HSTHTOAN_NGAYTT NGAYTT, NHANVIEN.DTDMNHANVIEN_MA THUNGAN, 'TT' LOAI , 
			
			IF (DTDMKHOIBHYT.DTDMKHOIBHYT_MA IS  NULL OR DTDMKHOIBHYT.DTDMKHOIBHYT_MA LIKE '',IF ( HS.HSTHTOAN_THTOAN > 0,  HS.HSTHTOAN_THTOAN, 0 ) ,0 ) THU_VIEN_PHI, 
			IF (DTDMKHOIBHYT.DTDMKHOIBHYT_MA IS  NULL OR DTDMKHOIBHYT.DTDMKHOIBHYT_MA LIKE '', IF ( HS.HSTHTOAN_THTOAN < 0,  -1*HS.HSTHTOAN_THTOAN, 0 ) ,0 )  CHI_VIEN_PHI, 
			IF (DTDMKHOIBHYT.DTDMKHOIBHYT_MA IS NOT NULL AND DTDMKHOIBHYT.DTDMKHOIBHYT_MA <> '',IF ( HS.HSTHTOAN_THTOAN > 0,  HS.HSTHTOAN_THTOAN, 0 ) ,0 ) THU_BHYT, 
			IF (DTDMKHOIBHYT.DTDMKHOIBHYT_MA IS NOT NULL AND DTDMKHOIBHYT.DTDMKHOIBHYT_MA <> '', IF ( HS.HSTHTOAN_THTOAN < 0,  -1*HS.HSTHTOAN_THTOAN, 0 ),0 ) CHI_BHYT
	FROM ((((( (HSBA HSBA1 JOIN HS_THTOAN HS ON HS.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN)
				LEFT JOIN HSBA_BHYT HSBABHYT ON HSBABHYT.HSBA_SOVAOVIEN LIKE HSBA1.HSBA_SOVAOVIEN)
				LEFT JOIN DM_KHOA DMKHOA ON  HS.HSTHTOAN_KHOA = DMKHOA.DMKHOA_MASO )
				LEFT JOIN DT_DM_NHAN_VIEN NHANVIEN ON NHANVIEN.DTDMNHANVIEN_MASO = HS.HSTHTOAN_THUNGAN)			
				LEFT JOIN DT_DM_KHOI_BHYT DTDMKHOIBHYT ON HSBABHYT.HSBABHYT_KHOIBH LIKE DTDMKHOIBHYT.DTDMKHOIBHYT_MASO)
				LEFT JOIN BENH_NHAN BN ON BN.BENHNHAN_MA LIKE HSBA1.BENHNHAN_MA)
				LEFT JOIN DT_DM_CUM CUM ON HS.HSTHTOAN_CUM = CUM.DTDMCUM_MASO
	WHERE 
		( $P{TuNgay} IS NULL OR DATE(HS.HSTHTOAN_NGAYTT) >= DATE($P{TuNgay}))
 	and 
    ( $P{DenNgay} IS NULL OR DATE(HS.HSTHTOAN_NGAYTT) <= DATE($P{DenNgay}))	
	and 
    ( $P{cum} IS NULL OR $P{cum} LIKE '' OR CUM.DTDMCUM_MA like $P{cum})]]></queryString>

