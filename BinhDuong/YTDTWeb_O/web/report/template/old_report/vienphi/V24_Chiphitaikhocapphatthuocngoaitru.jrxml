	<parameter name="chon2" isForPrompting="false" class="java.lang.String"/>
	<parameter name="khoi" isForPrompting="false" class="java.lang.String"/>
	<parameter name="noiKB" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT TD1.TIEPDON_NGAYGIO NGAYGIO, BN.BENHNHAN_HOTEN HOTEN, BN.BENHNHAN_NAMSINH NAMSINH,
	TD1.TIEPDON_SOTHEBH SOTHEBHYT, DTDMKHOIBHYT.DTDMKHOIBHYT_TYLEBHYT1 TYLEBHYT,
	CONCAT(IF(BN.BENHNHAN_DIACHI IS NULL,'',CONCAT(BN.BENHNHAN_DIACHI,' ')),IF(BN.XA_MA IS NULL,'',CONCAT(XA.DMXA_TEN,' ')),
		IF(BN.HUYEN_MA IS NULL,'',CONCAT(HUYEN.DMHUYEN_TEN,' ')),IF(BN.TINH_MA IS NULL,'',
			TINH.DMTINH_TEN)) DIACHI, PXBH.PHIEUXUATBH_THANHTIENB BNTRA, PXBH.PHIEUXUATBH_THANHTIEN  TONGCONG,  
				( IF ( PXBH.PHIEUXUATBH_THANHTIEN IS NULL, 0 , PXBH.PHIEUXUATBH_THANHTIEN ) -  
				  IF ( PXBH.PHIEUXUATBH_THANHTIENB IS NULL, 0 , PXBH.PHIEUXUATBH_THANHTIENB )
				 ) BHYTCHI


FROM ((((((TIEP_DON TD1 JOIN DT_DM_KHOI_BHYT DTDMKHOIBHYT
          ON TD1.KHOIBHYT_MA LIKE DTDMKHOIBHYT.DTDMKHOIBHYT_MA)
     JOIN PHIEU_XUAT_BH PXBH ON PXBH.TIEPDON_MA LIKE  TD1.TIEPDON_MA ) 		  
     JOIN BENH_NHAN BN ON BN.BENHNHAN_MA LIKE TD1.BENHNHAN_MA)     
	 LEFT JOIN DM_TINH TINH ON BN.TINH_MA = TINH.DMTINH_MASO ) 
	 LEFT JOIN DM_HUYEN HUYEN ON BN.HUYEN_MA = HUYEN.DMHUYEN_MASO)
	 LEFT JOIN DM_XA XA ON BN.XA_MA = XA.DMXA_MASO)	 

WHERE    ( if ( $P{chon1} = 'IN',
		 ($P{khoi} is null or DTDMKHOIBHYT.DTDMKHOIBHYT_MA IN ($P!{khoi})),
		 ($P{khoi} is null or DTDMKHOIBHYT.DTDMKHOIBHYT_MA NOT IN ($P!{khoi}))
              )
        ) AND 
	( if ( $P{chon2} = 'IN',
		 ($P{noiKB} is null or TD1.KCBBHYT_MA IN ($P!{noiKB})),
		 ($P{noiKB} is null or TD1.KCBBHYT_MA NOT IN ($P!{noiKB}))
              )
        ) 
	
	and( $P{DenNgay} IS NULL OR DATEDIFF(DATE(TD1.TIEPDON_NGAYGIO),DATE($P{DenNgay})) <= 0 )
        AND ( $P{TuNgay} IS NULL OR DATEDIFF(DATE(TD1.TIEPDON_NGAYGIO),DATE($P{TuNgay})) >= 0)]]></queryString>

