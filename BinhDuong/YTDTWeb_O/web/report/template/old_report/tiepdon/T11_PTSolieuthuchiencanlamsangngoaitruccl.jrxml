	<queryString><![CDATA[SELECT CBG.DTDMCLSBG_MA MASO, CBG.DTDMCLSBG_DIENGIAI NOIDUNG, DMC.DTDMCLS_MA PHANLOAI, COUNT(CLK.CLSKHAM_MA) SOLUONG
FROM CLS_KHAM CLK INNER JOIN THAM_KHAM TK ON CLK.CLSKHAM_THAMKHAM = TK.THAMKHAM_MA
	INNER JOIN TIEP_DON TD ON TK.TIEPDON_MA = TD.TIEPDON_MA	
	INNER JOIN DT_DM_CLS_BANG_GIA CBG ON CLK.CLSKHAM_MAHANG = CBG.DTDMCLSBG_MASO
	INNER JOIN DT_DM_CLS DMC ON CBG.DTDMCLSBG_MALOAI = DMC.DTDMCLS_MASO 
WHERE ($P{TUNGAY} IS NULL OR DATEDIFF(DATE(CLK.CLSKHAM_NGAYGIO), DATE($P{TUNGAY})) >= 0  ) 
	AND ($P{DENNGAY} IS NULL OR DATEDIFF( DATE(CLK.CLSKHAM_NGAYGIO) , DATE($P{DENNGAY})) <= 0  )		
	AND ($P{THUNGAN} IS NULL OR CLK.CLSKHAM_THUNGAN = $P{THUNGAN})	
	AND ($P{DOITUONG} IS NULL OR TD.DOITUONG_MA = $P{DOITUONG})
	AND ($P{LOAICLS} IS NULL OR CBG.DTDMCLSBG_MALOAI = $P{LOAICLS}) 
GROUP BY CBG.DTDMCLSBG_MA, CBG.DTDMCLSBG_DIENGIAI, DMC.DTDMCLS_MA]]></queryString>

